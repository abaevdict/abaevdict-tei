<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ævæzdæg" xml:lang="os">
    <form type="lemma">
        <orth>ævæzdæg</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>нежилой</q>
        </cit>
        <note type="comment" xml:lang="ru">, буквально „бездымный“</note>
        <note type="comment" xml:lang="en">, literally ‘smokeless’</note>
        <cit type="translation" xml:lang="en">
            <q>uninhabited</q>
        </cit>
        <note type="comment" xml:lang="ru">, буквально „бездымный“</note>
        <note type="comment" xml:lang="en">, literally ‘smokeless’</note>
    </sense>
    <etym xml:lang="ru">Из <ref type="xr" target="#entry_æ-"/> (негативная частица) и <ref type="xr"
            target="#entry_fæzdæg"/> ‘дым’.</etym>
    <etym xml:lang="en">From <ref type="xr" target="#entry_æ-"/> (a negative particle) and <ref
            type="xr" target="#entry_fæzdæg"/> ‘smoke’.</etym>
</entry>
