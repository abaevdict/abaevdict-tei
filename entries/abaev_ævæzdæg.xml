            <entry xml:id="entry_ævæzdæg" xml:lang="os">
                <form xml:id="form_d1217e66" type="lemma"><orth>ævæzdæg</orth></form>
                <sense xml:id="sense_d1217e69"><abv:tr xml:lang="ru">
                        <q>нежилой</q>
                    </abv:tr><note type="comment" xml:lang="ru">, буквально „бездымный“</note><note type="comment" xml:lang="en">, literally ‘smokeless’</note><abv:tr xml:lang="en">
                        <q>uninhabited</q>
                    </abv:tr><note type="comment" xml:lang="ru">, буквально „бездымный“</note><note type="comment" xml:lang="en">, literally ‘smokeless’</note></sense>
                <etym xml:lang="ru">Из <ref type="xr" target="#entry_æ-"/> (негативная частица) и
                        <ref type="xr" target="#entry_fæzdæg"/> ‘дым’.</etym>
                <etym xml:lang="en">From <ref type="xr" target="#entry_æ-"/> (a negative particle)
                    and <ref type="xr" target="#entry_fæzdæg"/> ‘smoke’.</etym>
            </entry>
