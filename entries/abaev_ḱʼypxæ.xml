    <text>
        <body>
            <entry xml:id="entry_ḱʼypxæ" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>ḱʼypxæ</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>kʼipxæ</orth></form>
                <sense xml:id="sense_d3314e72"><abv:tr xml:lang="ru">
                        <q>штамп (блинт) для тиснения на коже</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>stamp (blind) for skin stamping</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d3314e84" xml:lang="trk"><lang/>
                        <w>kepke</w>
                        <gloss><q>образец</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e92" xml:lang="trk"><w/>
                        <gloss><q>колодка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e98" xml:lang="trk"><w/>
                        <gloss><q>форма</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>II 1188</biblScope></bibl>), от
                        <mentioned xml:id="mentioned_d3314e108" xml:lang="trk"><lang/>
                        <w>kep</w>
                        <gloss><q>модель</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e117" xml:lang="trk"><w/>
                        <gloss><q>форма</q></gloss></mentioned> и пр., которое также налично в ос.
                        <ref type="xr" target="#entry_ǵīpp"/>.</etym>
                <etym xml:lang="en">From <mentioned xml:id="mentioned_d3314e128" xml:lang="trk"><lang/>
                        <w>kepke</w>
                        <gloss><q>sample</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e136" xml:lang="trk"><w/>
                        <gloss><q>boot-tree</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e142" xml:lang="trk"><w/>
                        <gloss><q>forma</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>II 1188</biblScope></bibl>), from
                        <mentioned xml:id="mentioned_d3314e152" xml:lang="trk"><lang/>
                        <w>kep</w>
                        <gloss><q>model</q></gloss></mentioned>, <mentioned xml:id="mentioned_d3314e161" xml:lang="trk"><w/>
                        <gloss><q>form</q></gloss></mentioned> etc., which is also present in
                    Ossetic <ref type="xr" target="#entry_ǵīpp"/>.</etym>
            </entry>
        </body>
