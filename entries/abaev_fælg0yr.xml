            <entry xml:id="entry_fælg0yr" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>fælg˳yr</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>fælgur</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>окружность</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>circumference</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>край</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>edge</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>карниз</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (у жернова, у
                            башни)</note><note type="comment" xml:lang="en"> (of a millstone, of a
                            tower)</note><abv:tr xml:lang="en">
                            <q>ledge</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (у жернова, у
                            башни)</note><note type="comment" xml:lang="en"> (of a millstone, of a
                            tower)</note></sense><sense><abv:tr xml:lang="ru">
                            <q>сусек</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>cornbin</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>adæm fædesi, kuj ba fælgurmæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>народ на тревогу, а собака — в сусек</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>the people go to the alarm, and the dog goes to hide in a cornbin</q>
                        </abv:tr>
                        <bibl><note type="comment" xml:lang="ru">поговорка; </note><note
                                type="comment" xml:lang="en">saying; </note><ref type="bibl"
                                target="#src_MD"/><biblScope>1936 I 71</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Может быть, из %ifæl-kur%i, к <mentioned xml:lang="sa"><lang/>
                        <w>kūla-</w>
                        <gloss><q>обрыв</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>берег</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">It may be from %ifæl-kur%i, to <mentioned xml:lang="sa"><lang/>
                        <w>kūla-</w>
                        <gloss><q>steep</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>bank</q></gloss></mentioned>.</etym>
