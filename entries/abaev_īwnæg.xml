    <text>
        <body>
            <entry xml:id="entry_īwnæg" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>īwnæg</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>ewnæg</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>один</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>one</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>единый</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>sole</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>единственный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>single</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>īwnæg læppu jyn wyd sawk˳yrm zærond fydæn</quote>
                        <abv:tr xml:lang="ru">
                            <q>единственный сын был у слепого старого отца</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>the old blind father had an only son</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Коста"/><biblScope>67</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>byn bawaj, īwnæg!</quote>
                        <abv:tr xml:lang="ru">
                            <q>пропади, одинокий!</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>disappear, a lonely person!</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Коста"/><biblScope>47</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>īwnægæj k˳yd cærys?</quote>
                        <abv:tr xml:lang="ru">
                            <q>как ты живешь один?</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>how do you live alone?</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Munk."/><biblScope>80</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>nur bajzadan ewnægæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>теперь мы остались одни</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>now we are alone</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>110</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>kæmi adtæ, mæ ewnæg ænsuvær?</quote>
                        <abv:tr xml:lang="ru">
                            <q>где ты был, мой единственный брат?</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>where have you been, my only brother?</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MSt."
                            /><biblScope>17%l2–3</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Восходит к иран. %i*aivanaka%i.</etym>
                <etym xml:lang="en">It traces back to Iranian %i*aivanaka%i.</etym>
            </entry>
