      <entry xml:id="entry_qybyllæ" xml:lang="os" abv:completeness="incomplete">
        <form type="lemma" xml:lang="os-x-iron"><orth>qybyllæ</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>qibillæ</orth></form>
        <sense n="1"><abv:tr xml:lang="ru">
