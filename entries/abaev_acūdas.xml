            <entry xml:id="entry_acūdas" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>acūdas</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>acodasæ</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>квасцы</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>alum</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из <mentioned xml:lang="trk"><lang/>
                        <w>aču-taš</w><note type="comment" xml:lang="ru">буквально</note><note
                            type="comment" xml:lang="en">literally</note>
                        <gloss><q>горький камень</q></gloss></mentioned>; ср. <mentioned
                        xml:lang="krc-x-balkar"><lang/>
                        <w>ačutaš</w>
                        <gloss><q>квасцы</q></gloss></mentioned>, <mentioned xml:lang="uz"><lang/>
                        <w>ačiqtaš</w></mentioned>, <mentioned xml:lang="uz"><w>ačiǧtaš</w>
                        <gloss><q>квасцы</q></gloss><note type="comment" xml:lang="ru"> (<bibl><ref
                                    type="bibl" target="#ref_Радлов"/><biblScope>I
                                510</biblScope></bibl>)</note><note type="comment" xml:lang="en">
                                    (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I
                                    510</biblScope></bibl>)</note></mentioned>, сюда же <mentioned
                        xml:lang="kbd"><lang/>
                        <w>axšādaš</w></mentioned>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="trk"><lang/>
                        <w>aču-taš</w><note type="comment" xml:lang="ru">буквально</note><note
                            type="comment" xml:lang="en">literally</note>
                        <gloss><q>bitter stone</q></gloss></mentioned>; cf. <mentioned
                        xml:lang="krc-x-balkar"><lang/>
                        <w>ačutaš</w>
                        <gloss><q>alum</q></gloss></mentioned>, <mentioned xml:lang="uz"><lang/>
                        <w>ačiqtaš</w></mentioned>, <mentioned xml:lang="uz"><w>ačiǧtaš</w>
                        <gloss><q>alum</q></gloss><note type="comment" xml:lang="ru"> (<bibl><ref
                                    type="bibl" target="#ref_Радлов"/><biblScope>I
                                510</biblScope></bibl>)</note><note type="comment" xml:lang="en">
                                    (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I
                                    510</biblScope></bibl>)</note></mentioned>, also here <mentioned
                        xml:lang="kbd"><lang/>
                        <w>axšādaš</w></mentioned>.</etym>
