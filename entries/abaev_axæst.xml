<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_axæst"
    xml:lang="os">
    <form type="lemma">
        <orth>axæst</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>узник</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>prisoner</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>арестант</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>arrestee</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>nʼaxæst ʒūry...</quote>
            <cit type="translation" xml:lang="ru">
                <q>наш заключенный говорит...</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>our prisoner says...</q>
            </cit>
            <bibl>
                <ref target="#src_ОЭп."/>
                <biblScope>25</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>axæstarǧ</quote>
            <cit type="translation" xml:lang="ru">
                <q>выкуп за заключенного</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>prisoner ransom</q>
            </cit>
            <bibl>
                <ref target="#src_ОЭп."/>
                <biblScope>25</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>xæjræǵy axæst fædæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>я попал в руки чорта</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I fell into the hands of the devil</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>xorz wazæǵy axæsty næ festæm</quote>
            <cit type="translation" xml:lang="ru">
                <q>не с добрым гостем мы связались</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>we contacted with an unkind guest</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>III 31</biblScope>
            </bibl>
        </cit>
    </cit>
