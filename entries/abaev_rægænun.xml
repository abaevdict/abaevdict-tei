      <entry xml:id="entry_rægænun" xml:lang="os-x-digor" abv:completeness="incomplete">
        <form type="lemma"><orth>rægænun</orth><form type="participle"
          ><orth>rægæd</orth></form></form>
        <usg><lang/></usg>
        <sense><sense><abv:tr xml:lang="ru">
              <q>сыпать</q>
            </abv:tr></sense><sense><abv:tr xml:lang="ru">
              <q>посыпать</q>
            </abv:tr><note type="comment"> (например, пищу солью, перцем)</note></sense></sense>
        <note type="comment">; <ref type="xr" target="#entry_zæryn" xml:lang="os-x-iron"><lang/>
            <w>zæryn</w></ref> id. (<bibl><ref type="bibl" target="#src_ОЯФ_I"/>
            <biblScope>461<hi rendition="#rend_subscript">689</hi></biblScope></bibl>)</note>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">cænxæ sæbæl <oRef>rægænæ</oRef> æma kærʒini
              xæccæ xwæræ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">посыпай их (помидоры) солью и ешь с хлебом</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_AK"/>
              <biblScope>III 18</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">xelagi særbæl cænxæ ku
                <oRef>nirrægænaj</oRef> æma е kud toxæn kæna, otæ Burxan ǧezæmaræ æma qurdoxæn
              kodta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">как мучается змея, если ей насыпать на голову
                соли, так страдал и мучился Бурхан</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MD"/>
              <biblScope>1940 III 54</biblScope></bibl>
          </example>
        </exampleGrp>
