    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>котел</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>cauldron</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="en">
                <q>kettle</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ag kæly jæ biltæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>(кипящий) котел льется через край</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(boiling) kettle overflows</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>45</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>i basi agæ æ galew kʼaxæj ku racæjkʼwærdta</quote>
            <cit type="translation" xml:lang="ru">
                <q>котел с супом он толкнул левой ногой</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he pushed the soup pot with his left foot</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>83</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>stur agæ ars qoppæǧtæj iradæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>большой котел кипел белыми пузырями</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a large kettle boiled with white bubbles</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>34[12]</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>agæ rajsta artæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>он снял котел с огня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he took the kettle off the fire</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>25[2]</biblScope>
            </bibl>
        </cit>
    </cit>
