<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dællagq0yr" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>dællagq˳yr</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>dælwaqur</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>передняя часть шеи ниже подбородка</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>a front part of the neck below the chin</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ūrsdællagq˳yr, sawærfyg</quote>
            <cit type="translation" xml:lang="ru">
                <q>с белой шеей, черными бровями</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>with a white neck, black eyebrows</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>dæ darǧ dællagq˳yr, dæ saw bazyrtæ!</quote>
            <cit type="translation" xml:lang="ru">
                <q>твоя длинная шея, твои черные крылья!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your long neck, your black wings!</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>83</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>æ dælwaqurtæj ba xortæ-mæjtæ xoduncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>из-под ее подбородка смеются солнца-месяцы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>sun-months are laughing from under her chin</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>II 24</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ūrsdællagq˳yr, sawærfyg</quote>
            <cit type="translation" xml:lang="ru">
                <q>с белой шеей, черными бровями</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>with a white neck, black eyebrows</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>dæ darǧ dællagq˳yr, dæ saw bazyrtæ!</quote>
            <cit type="translation" xml:lang="ru">
                <q>твоя длинная шея, твои черные крылья!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your long neck, your black wings!</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>83</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>æ dælwaqurtæj ba xortæ-mæjtæ xoduncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>из-под ее подбородка смеются солнца-месяцы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>sun-months are laughing from under her chin</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>II 24</biblScope>
            </bibl>
        </cit>
    </cit>
    <etym xml:lang="ru">Буквально „нижняя шея“; см. <ref type="xr" target="#entry_dællag"/> и <ref
            type="xr" target="#entry_q0yr"/>.</etym>
    <etym xml:lang="en">Literally “lower neck”; see <ref type="xr" target="#entry_dællag"/> and <ref
            type="xr" target="#entry_q0yr"/>.</etym>
</entry>
