            <entry xml:id="entry_gūcca" xml:lang="os">
                <form xml:id="form_d4270e66" xml:lang="os-x-iron" type="lemma"><orth>gūcca</orth></form>
                <form xml:id="form_d4270e69" xml:lang="os-x-digor" type="dialectal"><orth>gocca</orth></form>
                <sense xml:id="sense_d4270e72"><abv:tr xml:lang="ru">
                        <q>шапка</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>hat</q>
                    </abv:tr></sense>
                <abv:exampleGrp xml:id="exampleGrp_d4270e82">
                    <abv:example xml:id="example_d4270e84">
                        <quote>suǵzærijnæ dæ gocca, zældagæj ba dæ caca</quote>
                        <abv:tr xml:lang="ru">
                            <q>золотая твоя шапочка, из шелка твое платье</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>your hat is golden, your dress is made of silk</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                            157</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">„Инфантилизованное“ <ref type="xr" target="#entry_xūd"/> |
                    %ixod%i ‘шапка’.</etym>
                <etym xml:lang="en">“Infantilized” <ref type="xr" target="#entry_xūd"/> | %ixod%i
                    ‘hat’.</etym>
            </entry>
