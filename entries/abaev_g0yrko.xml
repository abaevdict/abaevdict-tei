    xml:id="entry_g0yrko" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>g˳yrko</orth><form type="variant"
                ><orth>gyrko</orth></form></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>gurko</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>жёлудь</q>
        </cit><cit type="translation" xml:lang="en">
            <q>acorn</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>...tūlʒ g˳yrkʼo radda, g˳yrk’o x˳y axæra</quote>
            <cit type="translation" xml:lang="ru">
                <q>...(чтобы) дуб дал жёлудь, жёлудь поела свинья</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>...(so that) an oak would give an acorn, a pig ate an acorn</q>
            </cit>
            <bibl><ref target="#src_ОТ"/><biblScope>61</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
