            <entry xml:id="entry_Idyl" xml:lang="os">
                <form xml:id="form_d964e66" type="lemma"><orth>Idyl</orth><form xml:id="form_d964e68" type="variant"><orth>Idil</orth></form><form xml:id="form_d964e70" type="variant"><orth>Edil</orth></form></form>
                <sense xml:id="sense_d964e73"><abv:tr xml:lang="ru">
                        <q>река Волга</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>Volga river</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из <mentioned corresp="#mentioned_d964e109" xml:id="mentioned_d964e85" xml:lang="trk"><lang/>
                        <w>Ätil</w></mentioned>, <mentioned corresp="#mentioned_d964e114" xml:id="mentioned_d964e90" xml:lang="trk"><w>Ädil</w></mentioned>,
                        <mentioned corresp="#mentioned_d964e117" xml:id="mentioned_d964e93" xml:lang="trk"><w>Edil</w></mentioned>, <mentioned corresp="#mentioned_d964e120" xml:id="mentioned_d964e96" xml:lang="trk"><w>Idəl</w>
                        <gloss><q>Волга</q></gloss></mentioned>, вообще ‘большая река’ (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I 842, 857,
                            1509</biblScope></bibl>).</etym>
                <etym xml:lang="en">From <mentioned corresp="#mentioned_d964e85" xml:id="mentioned_d964e109" xml:lang="trk"><lang/>
                        <w>Ätil</w></mentioned>, <mentioned corresp="#mentioned_d964e90" xml:id="mentioned_d964e114" xml:lang="trk"><w>Ädil</w></mentioned>,
                        <mentioned corresp="#mentioned_d964e93" xml:id="mentioned_d964e117" xml:lang="trk"><w>Edil</w></mentioned>, <mentioned corresp="#mentioned_d964e96" xml:id="mentioned_d964e120" xml:lang="trk"><w>Idəl</w>
                        <gloss><q>Volga</q></gloss></mentioned>, generally ‘big river’ (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I 842, 857,
                            1509</biblScope></bibl>).</etym>
            </entry>
