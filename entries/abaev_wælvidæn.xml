        <form xml:id="form_d903e66" type="lemma"><orth>wælvidæn</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d903e69">
          <abv:tr xml:lang="ru">
            <q>подкожный жир</q>
          </abv:tr>
        </sense>
        <abv:exampleGrp xml:id="exampleGrp_d903e75">
          <abv:example xml:id="example_d903e77">
            <quote>ors alasa wælvidæntæ ærwaǧta</quote>
            <abv:tr xml:lang="ru">
              <q>белый конь нагулял («отпустил») жиру</q>
            </abv:tr>
            <bibl><ref type="bibl" target="#ref_СОПам."/><biblScope>II 28, 187 42</biblScope></bibl>
          </abv:example>
        </abv:exampleGrp>
        <etym xml:lang="ru">Из <w>wæl-fid- </w>
          <gloss><q>(жир) поверх (<ref type="xr" target="#entry_wæl-">wæl</ref>) мяса
            (fid)</q></gloss>, <gloss><q>надмясный</q></gloss>, с формантом<m>-æn</m>.</etym>
