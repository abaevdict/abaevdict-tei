            <entry xml:id="entry_alacæq" xml:lang="os">
                <form type="lemma"><orth>alacæq</orth></form>
                <sense xml:id="sense_d1469e69"><abv:tr xml:lang="ru">
                        <q>шалаш</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>tent</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d1469e81" xml:lang="trk"><lang/>
                        <w>alačəq</w></mentioned>, <mentioned xml:id="mentioned_d1469e86" xml:lang="trk"><w>alačuq</w>
                        <gloss><q>шалаш</q></gloss><note type="comment" xml:lang="ru"> (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I
                                362</biblScope></bibl>)</note></mentioned>, откуда и <mentioned xml:id="mentioned_d1469e98" xml:lang="ru"><lang/>
                        <w>лачуга</w></mentioned>.</etym>
                <etym xml:lang="en">From <mentioned xml:id="mentioned_d1469e106" xml:lang="trk"><lang/>
                        <w>alačəq</w></mentioned>, <mentioned xml:id="mentioned_d1469e111" xml:lang="trk"><w>alačuq</w>
                        <gloss><q>tent</q></gloss><note type="comment" xml:lang="en">
                                    (<bibl><ref type="bibl" target="#ref_Радлов"/><biblScope>I
                                    362</biblScope></bibl>)</note></mentioned>, which <mentioned xml:id="mentioned_d1469e123" xml:lang="ru"><lang/>
                        <w>lačuga</w></mentioned> is also traced back to.</etym>
            </entry>
