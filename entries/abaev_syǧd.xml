      <entry xml:id="entry_syǧd" xml:lang="os" abv:completeness="russian-questions">
        <?oxy_comment_start author="oleg" timestamp="20230306T160219+0300" comment="Russian checked"?>
        <form xml:id="form_d354e66" type="lemma" xml:lang="os-x-iron"><orth>syǧd</orth></form><?oxy_comment_end?>
        <form xml:lang="os-x-digor" type="dialectal"><orth>suǧd</orth></form>
        <sense n="1">
          <sense xml:id="sense_d354e69">
            <abv:tr xml:lang="ru">
              <q>горелый</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>обгорелый</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>обожженный</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>сожженный</q>
            </abv:tr>
          </sense>
          <re>
            <form type="lemma"><orth>xuræj syǧd</orth><form type="variant"
                ><orth>xūrsyǧd</orth></form></form>
            <sense>
              <sense>
                <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                  <q xmlns="http://www.tei-c.org/ns/1.0">обожженный солнцем</q>
                </tr>
              </sense>
              <sense>
                <abv:tr xml:lang="ru">
                  <q>загорелый’</q>
                </abv:tr>
              </sense>
            </sense>
          </re>
          <?oxy_comment_start author="oleg" timestamp="20230306T160148+0300" comment="эти ре надо внутрь сенса"?>
          <re>
            <form type="lemma"><orth>syǧdæsmag</orth></form>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">запах горелого</q>
              </tr>
            </sense>
          </re><?oxy_comment_end?>
        </sense>
        <?oxy_comment_start author="Vados" timestamp="20221104T132136+0300" comment="Здесь я сделал так, что вторая группа смысла находится прямо после первой, а не после группы примеров. Можно так? По-другому не получается" id="i5f_t5k_swb" flag="done"?>
        <?oxy_comment_start author="oleg" timestamp="20230306T160103+0300" parentID="i5f_t5k_swb" comment="Да, нормально. Но там даже номер не нужен, потому что все один пример и он к первому значению (это и так по умолчанию так будет)" flag="done" mid="3"?>
        <sense n="2">
          <abv:tr xml:lang="ru">
            <q>пожар</q>
          </abv:tr>
        </sense><?oxy_comment_end?><?oxy_comment_end mid="3"?>
        <re>
          <form type="lemma"><orth>syǧdwat</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">пожарище</q>
            </tr>
          </sense>
        </re>
        <re>
          <note type="comment">ср. </note>
          <form type="lemma"><orth>Suǧdtæ</orth></form>
          <?oxy_comment_start author="oleg" timestamp="20230306T160212+0300" comment="вот это надо было значением с дефиницией, а не внутрь form, как было у тебя сделано"?>
          <sense>
            <def xml:lang="ru">название селения в Юго-Осетии</def>
          </sense><?oxy_comment_end?>
          <sense>
            <note type="comment"> = <mentioned xml:lang="ru"><lang/>
                <w>Горелово</w><note type="bibl">
                (<bibl><author>Некрасов</author></bibl>)</note></mentioned></note>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">næ fæste — cæfsgæ ǧædtæ sæ ænod
                <oRef>suǧdæsmagi</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">позади нас — горящие леса в душном запахе
                горелого</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Тайм._Мӕ_уарзт"/>
              <biblScope>68</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Прош. причастие от <ref type="xr" target="#entry_sūʒyn"><w
              xml:lang="os-x-iron">sūʒyn</w>
            <w xml:lang="os-x-digor">soʒun</w>
            <gloss><q>гореть</q></gloss>, <gloss><q>жечь</q></gloss></ref>, q. v.; ср. <mentioned
            xml:lang="fa"><lang/>
            <w>sūxt</w>, <w>sūxta</w></mentioned>, <mentioned xml:lang="ae"><lang/>
            <w>°suxta-</w>
            <gloss><q>сожженный</q></gloss></mentioned>.</etym>
