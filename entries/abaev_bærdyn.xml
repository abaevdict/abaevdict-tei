            <entry xml:id="entry_bærdyn" xml:lang="os">
                <form xml:id="form_d178e66" type="lemma"><orth>bærdyn</orth><form xml:id="form_d178e68" type="variant"><orth>bærdæn</orth></form></form>
                <sense xml:id="sense_d178e71"><abv:tr xml:lang="ru">
                        <q>поршень маслобойки</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>plunger of the butter churn</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d178e83" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>cilindri medægæ es „poršen“, ironaw — bærdæn</quote>
                        <abv:tr xml:lang="ru">
                            <q>в цилиндре есть поршень, по-осетински %sbæгdæn%s</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>there is a plunger in the churn, in Ossetic it is %sbærdæn%s</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_FS"/><biblScope>V 99</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Происхождение не ясно.</etym>
                <etym xml:lang="en">The etymology is not clear.</etym>
            </entry>
