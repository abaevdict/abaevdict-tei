            <entry xml:id="entry_kʼobor" xml:lang="os">
                <form type="lemma"><orth>kʼobor</orth></form>
                <sense xml:id="sense_d573e69"><sense xml:id="sense_d573e70"><abv:tr xml:lang="ru">
                            <q>крупный</q>
                        </abv:tr><abv:tr xml:lang="en">
