<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dæddyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>dæddyn</orth>
        <form type="inflected" subtype="ptcp">
            <orth>læværd</orth>
        </form>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>dæddun</orth>
        <form type="variant">
            <orth>tætun</orth>
        </form>
        <form type="inflected" subtype="ptcp">
            <orth>lævard</orth>
        </form>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>давать</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>give</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>na jæ dæddy mad</quote>
            <cit type="translation" xml:lang="ru">
                <q>мать не выдает (дочку)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>mother does not give (daughter)</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>62</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>cydæriddær dæ qæwy, uj dyn dæddæm</quote>
            <cit type="translation" xml:lang="ru">
                <q>все, что тебе нужно, мы тебе даем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>everything you need we give you</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>54</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>dæ odi bæsti ma od dæddun</quote>
            <cit type="translation" xml:lang="ru">
                <q>вместо твоей души отдаю свою душу</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I give my soul instead of your soul</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>3</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>kures tætuj æ fæzzæg</quote>
            <cit type="translation" xml:lang="ru">
                <q>осень приносит сноп (хлеба)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>autumn brings a sheaf (of grain)</q>
            </cit>
            <bibl>
                <ref target="#src_Gurdž._Æduli"/>
                <biblScope>70</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
