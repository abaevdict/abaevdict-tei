    <body>
      <entry xml:id="entry_sæddyn" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d1824e66" type="lemma" xml:lang="os-x-iron"><orth>sæddyn</orth><form
            type="participle"><orth>sast</orth></form></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>sæddun</orth><form type="participle"
              ><orth>sast</orth></form></form>
        <sense xml:id="sense_d1824e69" n="1">
          <sense>
            <abv:tr xml:lang="ru">
              <q>ломать</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>ломаться</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>разбивать</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>разбиваться</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>рубить (дрова)</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>нарушать</q>
            </abv:tr>
          </sense>
        </sense>
        <sense n="2">
          <sense>
            <abv:tr xml:lang="ru">
              <q>покорять</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>усмирять</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>покоряться</q>
            </abv:tr>
          </sense>
          <sense/>
        </sense>
        <sense n="3">
          <sense>
            <abv:tr xml:lang="ru">
              <q>признавать(ся)</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>сознаваться</q>
            </abv:tr>
          </sense>
        </sense>
        <re>
          <form type="lemma"><orth>ænæsæddon</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">непослушный</q>
            </tr>
          </sense>
        </re>
        <re>
          <form type="lemma"><orth>ænæsast</orth></form>
          <sense>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">непокоренный</q>
              </tr>
            </sense>
            <sense>
              <abv:tr xml:lang="ru">
                <q>девственный, не паханный, целинный</q>
              </abv:tr>
              <note type="comment"> (о земле)</note>
            </sense>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict" n="1">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">næ bīræǧæn asast jæ dændag</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">у нашего волка обломался зуб</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>135</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dūryn asasta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">он разбил кувшин</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <biblScope>1957 III 59</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">... æmæ asasta kærʒyntæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">... и преломил хлебы</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Марк"/>
              <biblScope>6 41</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ūs sūgtæ sasta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">женщина рубила дрова</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ИАА"/>
              <biblScope>III </biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">jæ tyx basædda!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">да сломится его сила!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>18</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">bak˳ymdta īх sæddyn</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">лед не поддался (<gloss><q>не дал себя
                    сломать</q></gloss>)</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ОЭ"/>
              <biblScope>I 32</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">sæ ard basastoj, fæsajdtoj næ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">они нарушили свою клятву, обманули нас</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <biblScope>1957 III 11</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">та basædd dæ ard</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">не нарушай свою клятву</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>40</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ew (cirtʒævæn) basastæj, īnnæ bа та
              ægas</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">один (надгробный камень) сломался, другой еще
                цел</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ОЭ"/>
              <biblScope>I 102</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">Xadizæt otæ ræsuǧd æma otæ ræwæg kafta æma,
              ænǧældæn, æ kʼaxi buni karki ajkæ dær næ basastajdæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">Хадизат танцевала так изящно и так легко, что,
                кажется, под ее ногой не разбилось бы и куриное яйцо</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MD"/>
              <biblScope>34₅</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dawwæ osemæn sæ taxæ basastæj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">«у двух женщин сломилась сила</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MSt."/>
              <biblScope>34₅</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">...mæ zærond færskitæ min та bssædda</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">...чтобы он не поломал мои старые ребра</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MSt."/>
              <biblScope>24₈</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dæ nifs та rasæddæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">пусть не сломится твой дух</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_DZ"/>
              <biblScope>40</biblScope></bibl>
          </example>
        </exampleGrp>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict" n="2">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">Xīwy коту adæm basastysty, mæsg˳yty dægæltæ
              ældary k’ūху raddoj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">жители Хевского ущелья покорились, вручили
                князю ключи от (своих) боевых башен</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>10</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">Tajmaraz fonʒyssæg læǵy raz dær... næ
              basastaid</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">Таймураз не покорился бы и перёд сотней
                мужчин</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>46</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ʒævgar fæqazydysty, fælæ ʒy nīḱī basast
              nomʒyd barǵytæj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">порядком поджигитовали, но никто из
                прославленных наездников не уступил</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Čerm."/>
              <biblScope>104</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ʒyrdæj dæ nīḱī basædʒæn</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">словами тебя никто не смирит (<gloss><q>не
                    осилит</q></gloss>)</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Брит."/>
              <biblScope>16</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">basæddyny qomys уп næj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">ничем его не проймешь</q>
            </tr>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">... dæ suǧzærīnæ ræsuǧd xætæl, qal kizgi
              zærdæ kæmæj basastaj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">...твоя золотая прекрасная свирель, которой ты
                покорил сердце гордой девушки</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ОЭп."/>
              <biblScope>42₃₂₄₋₃₂₅</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">cæj, mæ xor, cæj basæddæ, та wo min
              xevast</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">ну же, мое солнышко, покорись, не будь
                своевольной</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Gurdž._Æduli"/>
              <biblScope>101</biblScope></bibl>
          </example>
        </exampleGrp>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict" n="3">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">fætyx ta myl dæ, sæddyn yl mæxædæg</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">«ты опять меня осилил, я сам это признаю</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>228</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dūryn asasta æmæ jyl пае sæddy</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">он разбил кувшин и не признается</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <biblScope>1957 III 59</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">jæ axxosyl basastī</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">он признал свою вину</q>
            </tr>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">basastæj læg æ fudbæl</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">муж признал свою вину</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_AK"/>
              <biblScope>II 41</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Основа <mentioned xml:lang="os"><m>sædd-</m></mentioned> — из <mentioned
            xml:lang="oos"><m>sænd-</m></mentioned> с ассимиляцией <m>nd</m> → <m>dd</m>, как <ref
            type="xr" target="#entry_bæddyn"><m>bædd-</m> (<w>bæddyn</w>)</ref>
          <gloss><q>вязать</q></gloss> из <mentioned xml:lang="oos"><w>band-</w></mentioned>.
          Восходит к иран. <mentioned xml:lang="ira"><m type="rec">sčand-</m> (<m type="rec"
              >skand-</m>)</mentioned>, и.е. <mentioned xml:lang="ine"><m>ske(n)d-</m></mentioned>
              (<bibl><ref type="bibl" target="#ref_Pokorny"/>
            <biblScope>918 — 919</biblScope></bibl>). Ср. <mentioned xml:lang="fa"><lang/>
            <w>šᶤkustan</w>:<w>šᶤkan-</w>
            <gloss><q>ломать(ся)</q></gloss>, <gloss><q>разбивать(ся)</q></gloss>,
                <gloss><q>сгибать</q></gloss>, <gloss><q>обуздать</q></gloss>,
                <gloss><q>укрощать</q></gloss>, <gloss><q>нанести поражение</q></gloss></mentioned>,
            <mentioned xml:lang="pal-x-pazend"><lang/>
            <w>škandan</w>
          </mentioned>, <mentioned xml:lang="pal"><lang/>
            <w>škastan</w></mentioned>, <mentioned xml:lang="x-balochi"><lang/>
            <w>sind</w>: <w>sist</w>
            <gloss><q>ломать</q></gloss></mentioned> (<bibl><ref type="bibl"
              target="#ref_Зарубин._Бел."/>
            <biblScope>I 9, 21, 92, 181</biblScope></bibl> и др.) <mentioned xml:lang="sgh"><lang/>
            <w>ṣ̌ᶤčand-</w>
            <gloss><q>резать</q></gloss>, <gloss><q>рубить</q></gloss></mentioned> (<bibl><ref
              type="bibl" target="#ref_Tomaschek"/>
            <biblScope>890</biblScope></bibl>; <bibl><ref type="bibl" target="#ref_Зарубин._Шугн."/>
            <biblScope>273</biblScope></bibl>), <mentioned xml:lang="wbl"><lang/>
            <w>škend-</w>, <w>škεd-</w>
            <gloss><q>ломать</q></gloss>
            <note type="bibl"> (<bibl><ref type="bibl" target="#ref_Lorimer"/>
                <biblScope>II 229</biblScope></bibl>)</note>, <w>šand-</w>
            <gloss><q>to compound</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Shaw"/>
            <biblScope>204</biblScope></bibl>), <mentioned xml:lang="isk"><lang/>
            <w>fərsənd</w> (<w type="rec">fra-sčand-</w>) <gloss><q>разрываться</q></gloss>,
                <gloss><q>растерзывать</q></gloss>, <gloss><q>драть</q></gloss></mentioned>
              (<bibl><ref type="bibl" target="#ref_Пахалина"/>
            <biblScope>200</biblScope></bibl>), <mentioned xml:lang="sog"><lang/>
            <w type="rec">skand-</w> в <w>ktskntʼk</w>
            <gloss><q>разрушенный</q></gloss>, <w>qṭskndmʼhkyʼh</w>
            <gloss><q>разрушительное намерение</q></gloss></mentioned> ()</etym>
      </entry>
    </body>
  </text>
