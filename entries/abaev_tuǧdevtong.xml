      <entry xml:id="entry_tuǧdevtong" xml:lang="os-x-digor" abv:completeness="russian-questions">
        <?oxy_comment_start author="oleg" timestamp="20230302T211054+0300" comment="Russian checked"?>
        <form xml:id="form_d4870e66" type="lemma"
            ><orth><?oxy_comment_start author="irina" timestamp="20230127T125643+0300" comment="nb идет не по порядку"?>tuǧdevtong<?oxy_comment_end?></orth></form><?oxy_comment_end?>
        <usg><lang/></usg>
        <sense xml:id="sense_d4870e69">
          <abv:tr xml:lang="ru">
            <q>готовый, снаряженный к бою</q>
          </abv:tr>
        </sense>
        <note type="comment">;
            <?oxy_comment_start author="oleg" timestamp="20230302T211047+0300" comment="это есть как re к слову xæst"?><ref
            type="xr" target="#entry_xæst" xml:lang="os-x-iron"><lang/>
            <w>xæstīvtong</w>
            <gloss>id</gloss></ref><?oxy_comment_end?></note>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">læqwæn æ fidi gærztæ isbasta, ʽsbadtæj æ
              bæxbæl æma qæbær <oRef>tuǧdevtongæj</oRef> Donifarsmæ bacudæj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">юноша привязал оружие своего отца, сел на коня
                и, отлично снаряженный к бою, отправился в (аул) Донифарс</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_DZ"/>
              <biblScope>27</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_tuǧd"/> и <ref type="xr"
            target="#entry_īvtong"/>.</etym>
