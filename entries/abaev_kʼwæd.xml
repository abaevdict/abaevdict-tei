            <entry xml:id="entry_kʼwæd" xml:lang="os-x-digor">
                <form type="lemma"><orth>kʼwæd</orth><form type="variant"
                    ><orth>kwæt</orth></form></form>
                <usg><lang/></usg>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>квок</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>chuck</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>квохтанье курицы</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>hen's chuching</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>kark bilæbæl kʼwæt-kʼwæt kænuj</quote>
                        <abv:tr xml:lang="ru">
                            <q>курица квохчет на берегу</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a chicken is chucking on the shore</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_AK"/><biblScope>II 44</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="lez"><lang/>
                        <w>qwed</w>
                        <gloss><q>дикая курочка</q></gloss></mentioned>, <mentioned xml:lang="lez"><w/>
                        <gloss><q>куропатка</q></gloss></mentioned>
                            (<bibl><author>Гаджиев</author><biblScope xml:lang="ru">Русско-лезг.
                            сл., стр. 325</biblScope><biblScope xml:lang="en">Russian-Lezgian
                            dictionary, p. 325</biblScope></bibl>).</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="lez"><lang/>
                        <w>qwed</w>
                        <gloss><q>wild hen</q></gloss></mentioned>, <mentioned xml:lang="lez"><w/>
                        <gloss><q>partridge</q></gloss></mentioned>
                            (<bibl><author>Gadzhiev.</author><biblScope xml:lang="ru">Русско-лезг.
                            сл., стр. 325</biblScope><biblScope xml:lang="en">Russian-Lezgian
                            dictionary, p. 325</biblScope></bibl>).</etym>
