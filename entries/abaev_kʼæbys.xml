    <sense>
        <cit type="translation" xml:lang="ru">
            <q>щенок</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>puppy</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>æxsīnæn ærbaxastoj nog zad dywwæ kʼæbysy</quote>
            <cit type="translation" xml:lang="ru">
                <q>княгине принесли двух новорожденных щенят</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>two newborn puppies were brought to the princess</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>IV 47</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>Badelatæn kʼæbucijæn xattæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>для Баделят (феодалов) он служил щенком</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>for badelyats (feudal lords) he served as a puppy</q>
            </cit>
            <bibl>
                <ref target="#src_Qazb."/>
                <biblScope>37</biblScope>
            </bibl>
        </cit>
    </cit>
