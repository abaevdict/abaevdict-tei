      <entry xml:id="entry_tertt" xml:lang="os-x-iron" abv:completeness="complete">
        <form xml:id="form_d1872e66" type="lemma"><orth>tertt</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d1872e69">
          <abv:tr xml:lang="ru">
            <q>каприз</q>
          </abv:tr>
          <abv:tr xml:lang="en">
            <q>whim</q>
          </abv:tr>
        </sense>
        <re>
          <form type="lemma"><orth>tertt adæjmag</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">капризный человек</q>
            </tr>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">capricious person</q>
            </tr>
          </sense>
        </re>
        <re>
          <form type="lemma"><orth>fætertt wyn</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">закапризничать</q>
            </tr>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">get capricious</q>
            </tr>
          </sense>
          <note type="bibl"> (<bibl><ref type="bibl" target="#src_Сл."/></bibl>)</note>
        </re>
        <etym xml:lang="ru">Ср. <mentioned xml:lang="ru-x-dial"><lang/>
            <w>тырта</w>, <w>тыртыга</w>, <w>тартыга</w>
            <gloss><q>сварливый</q></gloss>, <gloss><q>спорщик</q></gloss>,
              <gloss><q>буян</q></gloss></mentioned>.</etym>
        <etym xml:lang="en">Cf. <mentioned xml:lang="ru-x-dial"><lang/>
            <w>tyrta</w>, <w>tyrtyga</w>, <w>tartyga</w>
            <gloss><q>quarrelsome</q></gloss>, <gloss><q>arguer</q></gloss>,
              <gloss><q>brawler</q></gloss></mentioned>.</etym>
      </entry>
