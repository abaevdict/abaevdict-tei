            <entry xml:id="entry_dekʼanoz" xml:lang="os">
                <form type="lemma"><orth>dekʼanoz</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>служитель культа, состоящий при „дзуаре“ (святилище)</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>religious worker of the “dzwar” (sanctuary)</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>жрец</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>priest</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Из <mentioned xml:lang="ka"><lang/>
                        <w>dekʼanozi</w><note type="comment" xml:lang="ru">в литературном
                            языке</note><note type="comment" xml:lang="en">in formal language</note>
                        <gloss><q>главный священнослужитель</q></gloss></mentioned>, <mentioned
                        xml:lang="ka"><w/><note type="comment" xml:lang="ru">у горцев (мохевцев,
                            пшавов, хевсур)</note><note type="comment" xml:lang="en">in mountain
                            dialects (Mokhevian, Pshavian, Khevsurian)</note>
                        <gloss><q>человек, состоящий при святилище</q></gloss></mentioned>. К
                        <mentioned xml:lang="la"><lang/>
                        <w>decanus</w></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._Gr."/><biblScope>10</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ka"><lang/>
                        <w>dekʼanozi</w><note type="comment" xml:lang="ru">в литературном
                            языке</note><note type="comment" xml:lang="en">in formal language</note>
                        <gloss><q>principal priest</q></gloss></mentioned>, <mentioned xml:lang="ka"
                            ><w/><note type="comment" xml:lang="ru">у горцев (мохевцев, пшавов,
                            хевсур)</note><note type="comment" xml:lang="en">in mountain dialects
                            (Mokhevian, Pshavian, Khevsurian)</note>
                        <gloss><q>sanctuary man</q></gloss></mentioned>. To <mentioned xml:lang="la"><lang/>
                        <w>decanus</w></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._Gr."/><biblScope>10</biblScope></bibl>.</etym>
