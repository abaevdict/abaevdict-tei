      <entry xml:id="entry_xætyn_2" n="2" xml:lang="os-x-iron" abv:completeness="english-questions">
        <form xml:id="form_d407e66" type="lemma"><orth>xætyn</orth><form type="participle"
              ><orth>xatt</orth></form></form>
        <usg><lang/></usg>
        <sense>
          <sense xml:id="sense_d407e69">
            <abv:tr xml:lang="ru">
              <q>ворочаться</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>toss and turn</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>поворачиваться</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>turn</q>
            </abv:tr>
          </sense>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">je ʽrzag jæ komy nal
              <oRef>xattī</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">язык больше не ворочался у нее во рту</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">her tongue was no longer moving in her
                mouth</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>56</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Medium к <ref type="xr" target="#entry_xætyn_1"><hi
              rendition="#rend_superscript">1</hi><w>xatun</w>
            <gloss><q>поворачивать</q></gloss></ref>. См. еще <ref type="xr"
            target="#entry_xūrxætæn"/>.</etym>
        <etym xml:lang="en">Medium to  <ref type="xr" target="#entry_xætyn_1"><hi
              rendition="#rend_superscript">1</hi><w>xatun</w>
            <gloss><q>turn (sth.)</q></gloss></ref>. See also <ref type="xr"
            target="#entry_xūrxætæn"/>.</etym>
      </entry>
