      <entry xml:id="entry_qædærmæg" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d665e66" type="lemma" xml:lang="os-x-iron"><orth>qædærmæg</orth></form>
        <form xml:id="form_d665e69" xml:lang="os-x-digor" type="dialectal"><orth>ǧædærmæg</orth></form>
        <sense xml:id="sense_d665e72"><sense xml:id="sense_d665e73"><abv:tr xml:lang="ru">
              <q>лесоматериал</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>timber</q>
            </abv:tr></sense><sense xml:id="sense_d665e82"><abv:tr xml:lang="ru">
              <q>древесина</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>wood</q>
            </abv:tr></sense></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict" xml:id="exampleGrp_d665e92">
          <example xml:id="example_d665e94">
            <quote xmlns="http://www.tei-c.org/ns/1.0">ǧædærmæg æmbujun bajdædta, kʼozotæ
              isævzurstæj æ xurfi</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">древесина стала гнить, в ней завелись
                грибки</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">the wood began to rot, fungi got into it</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_SD"/>
              <biblScope>191<hi rendition="#rend_subscript">4</hi></biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_qæd_1"/> и <ref type="xr" target="#entry_ærmæg"/>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_qæd_1"/> and <ref type="xr" target="#entry_ærmæg"/>.</etym>
