<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ævvaxs" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>ævvaxs</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>ænvaxs</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>близкий</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>close</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>близ</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>near smth.</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>недалеко</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>nearby</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>dard lymænæj ævvaxs æznag x˳yzdær u</quote>
            <cit type="translation" xml:lang="ru">
                <q>лучше враг вблизи, чем друг вдали</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Better an enemy close than a friend away</q>
            </cit>
            <bibl>
                <ref target="#src_ОТ"/>
                <biblScope>10</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>xīdmæ. ævvaxs æj banygædtoj</quote>
            <cit type="translation" xml:lang="ru">
                <q>его похоронили ;близ моста</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he was burried close to the bridge</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>38</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ʒwary kūvændonme sabyr bacydysty æmæ ævvaxs næ wændync</quote>
            <cit type="translation" xml:lang="ru">
                <q>тихо подошли к святилищу дзуара (божества), но близко не смеют</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[they] quietly approached the sanctuary of the dzwar (deity), but did not dare to
                    get close</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
                <biblScope>178</biblScope>
            </bibl>
        </cit>
    </cit>
