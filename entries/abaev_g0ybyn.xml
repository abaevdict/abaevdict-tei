<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_g0ybyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>g˳ybyn</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>gubun</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>живот</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stomach</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>брюхо</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>belly</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>g˳ybynnīz</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>болезнь живота</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stomachache</q>
            </cit>
            <note type="comment" xml:lang="ru">(д. %iqæstinez%i) </note>
            <note type="comment" xml:lang="en">(Digor %iqæstinez%i) </note>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>(k˳yʒ) awaǧta ... būrfyx kærʒyn jæ g˳ybynmæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>(пес) отправил в свой живот спеченный с желтой (корочкой) хлеб</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the dog sent baked bread with the yellow crust to his stomach</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>240</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ærdæbon īw ʒynga anyqq˳yrdton, æmæ wædæj nyrmæ mæ g˳ybyn rīssy</quote>
            <cit type="translation" xml:lang="ru">
                <q>недавно я проглотил одного комара, и с тех пор у меня болит живот</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I swallowed one mosquito recently, and since then I have a stomachache</q>
            </cit>
            <bibl>
                <ref target="#src_ОЭ"/>
                <biblScope>I 62</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>ustur gubun, kʼælos siwtæ, fædæggin ǧog ...</quote>
            <cit type="translation" xml:lang="ru">
                <q>корова с большим животом, изогнутыми рогами, вымистая...</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a cow with a big belly, with curved horns, puffy ...</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>23</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>raǧæn ræwæg gubunæn ʒæbæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>легкая для спины, приятная для живота (дорожная провизия)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>light for the back, pleasant for the abdomen (travel provisions)</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>92</biblScope>
            </bibl>
        </cit>
    </cit>
