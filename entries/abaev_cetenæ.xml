    xml:id="entry_cetenæ" xml:lang="os-x-digor">
    <form type="lemma"><orth>cetenæ</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>корзина (для рукоделия и пр.)</q>
        </cit><cit type="translation" xml:lang="en">
            <q>basket (for handicraft etc.)</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>mæ bolat kærdæn ceteni</quote>
            <cit type="translation" xml:lang="ru">
                <q>мои булатные ножницы в корзиночке</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my damask steel scissors are in a little basket</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>22</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>osæ ba... æ suǧzærīnæ ʒikkotæ suǧzærīnæ cetenæmæ rawaʒidæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>женщина опускала свои золотые кудри в золотую корзину</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the woman put down her golden curls into a golden basket</q>
            </cit>
            <bibl><ref target="#src_Диг._сказ."/><biblScope>17</biblScope></bibl>
        </cit>
    </cit>
