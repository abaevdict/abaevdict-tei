      <entry xml:id="entry_xædbyl-" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d3327e66" type="lemma" xml:lang="os-x-iron"><orth>xædbyl-</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>xwædbilæ</orth></form>
        <re>
          <form type="lemma"><orth>xædbylyl</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">у самого берега, обрыва</q>
            </tr>
          </sense>
          <re>
            <form type="lemma"><orth>xædbylmæ</orth></form>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">к самому берегу, обрыву</q>
              </tr>
            </sense>
          </re>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">Aragwyjy dony <oRef>xædbylyl</oRef>, xoxy
              qæbysym Ganis, zæǧgæ, īw ḱysyl qæwy cardysty ænæzænæg læg æmæ ūs</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">у самого берега реки Арагвы, на лоне горы, в
                маленьком ауле Ганис жили бездетные муж и жена</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>11</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_xæd-"/> и <ref type="xr"
            target="#entry_byl"/>.</etym>
      </entry>
