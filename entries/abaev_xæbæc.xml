      <entry xml:id="entry_xæbæc" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d828e66" type="lemma" xml:lang="os-x-iron"><orth>xæbæc</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>xæbæccæ</orth></form>
        <sense>
          <sense xml:id="sense_d828e69">
            <abv:tr xml:lang="ru">
              <q>раскоряка</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>с расставленными врозь ногами</q>
            </abv:tr>
          </sense>
        </sense>
        <re>
          <form type="lemma"><orth><oRef>xæbæcæj</oRef></orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">раскорякой</q>
            </tr>
          </sense>
        </re>
        <re>
          <form type="lemma"><orth><oRef>xæbæc(cæj)</oRef> læwwyn, cæwyn, badyn</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0" rendition="#rend_doublequotes">стоять, идти,
                сидеть раскорякой</q>
            </tr>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">wæjug dwarbæl <oRef>xæbæccæ</oRef>
              ærlæwdtæj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">ваюг (циклоп) стал раскорякой у двери</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_СОПам."/>
              <biblScope>II 15</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">rawaǧta gæbæt, cæwun rajdædta
                <oRef>xæbæccæ</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">он отпустил брюхо, ходить стал раскорякой</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Тайм._Мӕ_уарзт"/>
              <biblScope>56</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Звукоизобразительное слово; ср. <mentioned xml:lang="os"
              ><phr>gæbæc-gæbæc kænyn</phr>
            <gloss><q>ковылять</q></gloss></mentioned>.</etym>
      </entry>
