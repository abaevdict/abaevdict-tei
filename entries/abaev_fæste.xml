<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_fæste"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>fæste</orth>
        <form type="variant">
            <orth>fæstijæ</orth>
        </form>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>fæste</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>сзади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>behind</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>назади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>from behind</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>позади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>from the rear</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>razæj cydysty bæxǵyntæ, fæste — fistæǵytæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>впереди ехали конные, позади шли пешие</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>horsemen rode ahead, pedestrians walked behind</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>dūǧy Wyryzmæǵy Ærfæn fæste bazzadi</quote>
            <cit type="translation" xml:lang="ru">
                <q>на скачках (конь) Урузмага Арфан остался позади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Uruzmag's horse Arfan was left behind at the races</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>mæ furttæj fæstegæj cæf nekæd neke adtæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>из моих сыновей никогда никто не получал рану сзади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>none of my sons ever got a back wound</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>II 18</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>finʒi ku cæwaj, fæstæmæ næ baǧæwaj kænæ, fæstekkæj ku waj, dæ bazuri buni næ
                bakænæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>если ты будешь идти впереди, оберегай нас назад, если ты будешь сзади, накрой нас
                    своим крылом!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>if you go ahead, protect us from the back, if you are behind, cover us with your
                    wing!</q>
            </cit>
            <bibl>
                <note type="comment" xml:lang="ru">из гимна Уасгерги; </note>
                <note type="comment" xml:lang="en">from the Wasgergi anthem; </note>
                <ref target="#src_СОПам."/>
                <biblScope>II 139</biblScope>
            </bibl>
        </cit>
    </cit>
