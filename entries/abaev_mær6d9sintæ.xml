      <entry xml:id="entry_mær6d9sintæ" xml:lang="os-x-digor" abv:completeness="complete">
        <form type="lemma"><orth>mær(d)sintæ</orth></form>
        <usg><lang/></usg>
        <sense><abv:tr xml:lang="ru">
            <q>носилки для покойника</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>stretcher for a dead man</q>
          </abv:tr></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">baǧudæj sæ æveppajdi mard xæssunmæ nur
                <oRef>mærsintæ</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">неожиданно понадобились им носилки, чтобы нести
                покойницу</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">they suddently needed a stretcher for carrying
                the dead woman</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Iræf"/>
              <biblScope>42</biblScope></bibl>
          </example>
        </exampleGrp>
