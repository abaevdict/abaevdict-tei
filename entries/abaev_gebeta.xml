            <entry xml:id="entry_gebeta" xml:lang="os-x-digor">
                <form xml:id="form_d4394e66" type="lemma"><orth>gebeta</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d4394e72"><abv:tr xml:lang="ru">
                        <q>пузатый</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>big-bellied</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Ср. <ref type="xr" target="#entry_gæbæt"/> ‘бурдюк’; ср. также
                        <mentioned xml:id="mentioned_d4394e86" xml:lang="xmf"><lang/>
                        <w>k’abet’i</w>
                        <gloss><q>большой</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4394e94" xml:lang="xmf"><w/>
                        <gloss><q>утучненный</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Кипшидзе"/><biblScope>249</biblScope></bibl>).</etym>
                <etym xml:lang="en">Compare <ref type="xr" target="#entry_gæbæt"/> ‘wineskin’;
                    compare also <mentioned xml:id="mentioned_d4394e109" xml:lang="xmf"><lang/>
                        <w>k’abet’i</w>
                        <gloss><q>big</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4394e117" xml:lang="xmf"><w/>
                        <gloss><q>obese</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Кипшидзе"/><biblScope>249</biblScope></bibl>).</etym>
            </entry>
