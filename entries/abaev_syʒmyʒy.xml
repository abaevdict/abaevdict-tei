    <body>
      <entry xml:id="entry_syʒmyʒy" xml:lang="os" abv:completeness="english-questions">
        <form xml:id="form_d4295e66" type="lemma" xml:lang="os-x-iron"><orth>syʒmyʒy</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>siʒmuʒi</orth></form>
        <sense xml:id="sense_d4295e69">
          <abv:tr xml:lang="ru">
            <q>задом наперед</q>
          </abv:tr>
          <abv:tr xml:lang="en">
            <q>backwards</q>
          </abv:tr>
        </sense>
        <re>
          <form type="lemma"><orth>syʒmyʒy cæwyn</orth></form>
          <sense>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">идти, двигаться задом наперед</q>
              </tr>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="en">
                <q xmlns="http://www.tei-c.org/ns/1.0">go, move backwards</q>
              </tr>
            </sense>
            <sense>
              <abv:tr xml:lang="ru">
                <q>пятиться</q>
              </abv:tr>
              <abv:tr xml:lang="en">
                <q>move back</q>
              </abv:tr>
            </sense>
            <sense>
              <abv:tr xml:lang="ru">
                <q>отступать</q>
              </abv:tr>
              <abv:tr xml:lang="en">
                <q>retreat</q>
              </abv:tr>
            </sense>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0"><oRef>fæssiʒmuʒʼ</oRef> æncæ bæxgintæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">всадники отступили</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">the riders retreated</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Iræf"/>
              <biblScope>59</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">К <ref type="xr" target="#entry_syʒ"><w xml:lang="os-x-iron">syʒ</w>
            <w xml:lang="os-x-digor">siʒæ</w></ref> с ассонирующей второй частью (<bibl><ref
              type="bibl" target="#ref_ГО"/>
            <biblScope>§ 202<hi rendition="#rend_subscript">2</hi></biblScope></bibl>).</etym>
        <etym xml:lang="en">To <ref type="xr" target="#entry_syʒ"><w xml:lang="os-x-iron">syʒ</w>
            <w xml:lang="os-x-digor">siʒæ</w></ref> with the assonating second part (<bibl><ref
              type="bibl" target="#ref_ГО"/>
            <biblScope>§ 202<hi rendition="#rend_subscript">2</hi></biblScope></bibl>).</etym>
      </entry>
    </body>
