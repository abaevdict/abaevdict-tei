        <body>
            <entry xml:id="entry_æmtʼery" xml:lang="os-x-iron">
                <form xml:id="form_d980e66" type="lemma"><orth>æmtʼery</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d980e72"><sense xml:id="sense_d980e73"><abv:tr xml:lang="ru">
                            <q>смешение</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>mixing</q>
                        </abv:tr></sense><sense xml:id="sense_d980e82"><abv:tr xml:lang="ru">
                            <q>хаос</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>chaos</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d980e94">
                        <quote>adæm sæmtʼery sty</quote>
                        <abv:tr xml:lang="ru">
                            <q>народ смешался, обратился в беспорядочную толпу</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>people mixed up, turned into a messy crowd</q>
                        </abv:tr>
                    </abv:example>
                    <abv:example xml:id="example_d980e109">
                        <quote>mīǧtæ fæstæjæ æmtʼeryjæ wajync</quote>
                        <abv:tr xml:lang="ru">
                            <q>тучи бегут сзади хаотической массой</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>chaotic mass of clouds is running behind</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Niger"/><biblScope>134</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned corresp="#mentioned_d980e163" xml:id="mentioned_d980e131" xml:lang="ka"><lang/>
                        <w>mt’veri</w>
                        <gloss><q>пыль</q></gloss></mentioned>, <mentioned corresp="#mentioned_d980e179" xml:id="mentioned_d980e139" xml:lang="ka"><w/>
                        <gloss><q>прах</q></gloss></mentioned>, <mentioned corresp="#mentioned_d980e171" xml:id="mentioned_d980e145" xml:lang="ka"><lang/>
                        <w>mt’vreva</w>
                        <gloss><q>ломать</q></gloss></mentioned>, <mentioned corresp="#" xml:id="mentioned_d980e153" xml:lang="ka"><w/>
                        <gloss><q>крушить</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned corresp="#mentioned_d980e131" xml:id="mentioned_d980e163" xml:lang="ka"><lang/>
                        <w>mt’veri</w>
                        <gloss><q>dust</q></gloss></mentioned>, <mentioned corresp="#mentioned_d980e145" xml:id="mentioned_d980e171" xml:lang="ka"><lang/>
                        <w>mt’vreva</w>
                        <gloss><q>break</q></gloss></mentioned>, <mentioned corresp="#mentioned_d980e139" xml:id="mentioned_d980e179" xml:lang="ka"><w/>
                        <gloss><q>destroy</q></gloss></mentioned>.</etym>
