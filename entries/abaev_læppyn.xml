    <body>
      <entry xml:id="entry_læppyn" xml:lang="os-x-iron" abv:completeness="incomplete">
        <form type="lemma"><orth>læppyn</orth></form>
        <usg><lang/></usg>
        <sense><sense><abv:tr xml:lang="ru">
              <q>детеныш</q>
            </abv:tr></sense><sense><abv:tr xml:lang="ru">
              <q>птенец</q>
            </abv:tr></sense></sense>
        <re>
          <form type="lemma"><orth>læppyn-cærgæs</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">орленок</q>
            </tr></sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ysnyvond kænyn… dywwæ æxsīnæǵy
              læppyny</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">принести в жертву… двух птенцов голубиных</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Лука"/>
              <biblScope><hi rendition="#rend_italic">2</hi> 24</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">zærvatyḱḱy læppyntæ kʼanawy
              nyxxawdtoj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">птенцы ласточки упали в канаву</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>108</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">tærqus-læppynyl aftæ ma fæxūd</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">не смейся так над зайчонком</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>107</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">kævdæsy gædy jæ læppyntæj qazyd</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">в яслях кошка играла со своими котятами</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Niger"/>
              <biblScope>142</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">kæcæjdær fæzyndī xæjræǵy læppyn, æmæ nyn næ
              amond askʼæfyny fænd yskodta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">откуда-то явился детеныш дьявола и замыслил
                похитить наше счастье</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Arsen"/>
              <biblScope>79</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_læqwæn"/>.</etym>
      </entry>
