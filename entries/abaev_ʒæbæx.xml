    <sense><sense><cit type="translation" xml:lang="ru">
                <q>хороший</q>
            </cit><cit type="translation" xml:lang="en">
                <q>good</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>здоровый</q>
            </cit><cit type="translation" xml:lang="en">
                <q>healthy</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>хорошо</q>
            </cit><cit type="translation" xml:lang="en">
                <q>well</q>
            </cit></sense></sense>
    <re><form type="lemma"><orth>ʒæbæx kænyn</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>лечить</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>cure</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>кастрировать</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>castrate</q>
                </cit></sense></sense></re>
    <re><form type="lemma"><orth>ysʒæbæx wyn</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>поправиться</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>recover</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>вылечиться</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>get oneself well</q>
                </cit></sense></sense></re>
    <re><form type="lemma"><orth>ʒæbæxag</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>добро</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>good</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>что-либо вкусное съестное</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>something edible that is delicious</q>
                </cit></sense></sense></re>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>wæ, ʒæbæx læqwæn!</quote>
            <cit type="translation" xml:lang="ru">
                <q>о, славный юноша!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>oh, glorious young man!</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>kizgaj ræsuǧd æma ʒæbæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>девушка красивая и хорошая</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>beautiful and good girl</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>65</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>bajqūs ʒæbæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>послушай хорошенько</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>listen well</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>52</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су k˳ystæn bæzzydtæn ʒæbæxaær myzdyl?</quote>
            <cit type="translation" xml:lang="ru">
                <q>на какую работу я годился за лучшую плату?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what job was I good for to be paid better?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>55</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ci ʒæbæx ǧazis</quote>
            <cit type="translation" xml:lang="ru">
                <q>как ты хорошо играешь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>how well you play</q>
            </cit>
            <bibl><ref target="#src_MSt."/><biblScope>8%l12%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>zærdi ʒæbæxæn mæsugi særæj fækkæsioncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>для услады сердца они смотрят с вершины башни</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>they are looking from the top of the tower for the delight of the heart</q>
            </cit>
            <bibl><ref target="#src_MSt."/><biblScope>26%l9—10%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>Xetægæn æ xizin alli ʒæbæxagæj iʒag fæwwuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>у Хетага сума бывает полна всякого добра</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Khetag's sum is full of all kinds of goods</q>
            </cit>
            <bibl><ref target="#src_Bes._Ka_ke"/><biblScope>10</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>xostæ myn kodta, æmæ sʒæbæx dæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>он делал мне лекарства, и я выздоровел</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he made medicine for me and I recovered</q>
            </cit>
            <bibl><ref target="#src_Munk."/><biblScope>140</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>jæ cæftæj næma sʒæbæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>он еще не оправился от ран</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he has not recovered from his wounds yet</q>
            </cit>
            <bibl><ref target="#src_Сека"/><biblScope>51</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>læǧuz boǧati ʒæbæx kænun ǧæwuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>плохих быков надо кастрировать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>bad bulls need to be castrated</q>
            </cit>
            <bibl><ref target="#src_FS"/><biblScope>V 62</biblScope></bibl>
        </cit>
    </cit>
