                        </abv:tr></sense><sense xml:id="sense_d5090e79"><abv:tr xml:lang="ru">
                            <q>толщина</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>thickness</q>
                        </abv:tr></sense><sense xml:id="sense_d5090e88"><abv:tr xml:lang="ru">
                            <q>тучный</q>
                        </abv:tr><abv:tr xml:lang="en">
