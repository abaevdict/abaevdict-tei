                        <gloss><q>abundant</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e329" xml:lang="sa"><w>bahula-</w>
                        <gloss><q>thick</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e335" xml:lang="sa"><w/>
                        <gloss><q>мощный</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e342" xml:lang="xcl"><lang/>
                        <w>bazum</w>
                        <gloss><q>much</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e350" xml:lang="grc"><lang/>
                        <w>παχύς</w>
