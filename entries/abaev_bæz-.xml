                        <gloss><q>толстый</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e131" xml:lang="x-balochi"><w/>
                        <gloss><q>густой</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e137" xml:lang="x-balochi"><w>bāz</w>
                        <gloss><q>много</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e143" xml:lang="wbl"><lang/>
                        <w>bāj</w><note type="comment" xml:lang="ru"> (?)</note>
                        <gloss><q>толстый</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e154" xml:lang="kho"><lang/>
                        <w>baysga-(*bazga-)</w>
                        <gloss><q>глубокий</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e162" xml:lang="ae"><lang/>
                        <w>bāzah-</w>
                        <gloss><q>величина</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e170" xml:lang="ae"><w/></mentioned>, <mentioned xml:id="mentioned_d5090e173" xml:lang="sa"><lang/>
                        <w>bahu-</w>
                        <gloss><q>густой</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e181" xml:lang="sa"><w/>
                        <gloss><q>обильный</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e187" xml:lang="sa"><w>bahula-</w>
                        <gloss><q>толстый</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e194" xml:lang="sa"><w/>
                        <gloss><q>мощный</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e200" xml:lang="xcl"><lang/>
                        <w>bazum</w>
                        <gloss><q>много</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e208" xml:lang="grc"><lang/>
                        <w>παχύς</w>
                        <gloss><q>толстый</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e216" xml:lang="grc"><w/>
                        <gloss><q>густой</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5090e222" xml:lang="grc"><w/>
