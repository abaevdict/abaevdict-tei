            <entry xml:id="entry_bæz-" xml:lang="os">
                <form type="lemma"><orth>bæz-</orth></form>
                <sense xml:id="sense_d5090e69"><sense xml:id="sense_d5090e70"><abv:tr xml:lang="ru">
