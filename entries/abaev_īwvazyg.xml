            <entry xml:id="entry_īwvazyg" xml:lang="os">
                <form xml:id="form_d203e66" xml:lang="os-x-iron" type="lemma"><orth>īwvazyg</orth></form>
                <form xml:id="form_d203e69" xml:lang="os-x-digor" type="dialectal"><orth>ewvazug</orth></form>
                <sense xml:id="sense_d203e72"><sense xml:id="sense_d203e73"><abv:tr xml:lang="ru">
                            <q>одинарный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>single</q>
                        </abv:tr></sense><sense xml:id="sense_d203e82"><abv:tr xml:lang="ru">
                            <q>не сдвоенный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>not double</q>
                        </abv:tr></sense></sense>
                <sense xml:id="sense_d203e92"><abv:tr xml:lang="ru">
                        <q>несложный</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>simple</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из %iīw-faz-yg%i. См. <ref type="xr" target="#entry_faz"/>. Ср.
                        <ref type="xr" target="#entry_dyvazyg"/> ‘двойной’.</etym>
                <etym xml:lang="en">It is from %iīw-faz-yg%i. See <ref type="xr" target="#entry_faz"/>. Cf. <ref type="xr" target="#entry_dyvazyg"/> ‘double’.</etym>
            </entry>
