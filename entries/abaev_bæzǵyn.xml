<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_bæzǵyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>bæzǵyn</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>bæzgin</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>толстый</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>thick</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>густой</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>dense</q>
            </cit>
        </sense>
    </sense>
    <re>
        <re>
            <form type="lemma">
                <orth>bæzǵyn kʼ˳ymæl</orth>
            </form>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>густая брага</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>thick wash</q>
                </cit>
            </sense>
        </re>
        <re>
            <form type="lemma">
                <orth>bæzǵyn kærdæg</orth>
            </form>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>густая трава</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>thick grass</q>
                </cit>
            </sense>
            <note type="comment" xml:lang="ru">; ант. — <ref type="xr" target="#entry_tænæg"/>
                ‘тонкий’, ‘жидкий'</note>
            <note type="comment" xml:lang="en">; antonym — <ref type="xr" target="#entry_tænæg"/>
                ‘thin’, ‘liquid'</note>
        </re>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>bæzǵyn carm</quote>
            <cit type="translation" xml:lang="ru">
                <q>толстая кожа</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>thick skin</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>(læppū) donæn jæ bæzǵyny ærbas-kʼærdta</quote>
            <cit type="translation" xml:lang="ru">
                <q>(юноша) погнал (лошадь) по глубокому (месту) реки</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(a young man) drove (a horse) along the deep (place) of the river</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>20</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ʒæbæxdær baqyrnyt wæ bæzǵyn qælæstæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>получше подпевайте вашими густыми голосами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>join better with your deep voices</q>
            </cit>
            <bibl>
                <ref target="#src_Æfx._Xæs."/>
                <biblScope>99</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>bæzgin quræj niccæjxoduj (Bæbæt)</quote>
            <cit type="translation" xml:lang="ru">
                <q>(Бабат) рассмеялся густым голосом</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(Babat) laughed in a deep voice</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>52</biblScope>
            </bibl>
        </cit>
    </cit>
