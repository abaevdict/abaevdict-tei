      <entry xml:id="entry_ūdysmag" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d4938e66" type="lemma"><orth>ūdysmag</orth></form>
        <sense xml:id="sense_d4938e69" xml:lang="os-x-iron">
          <usg><lang/></usg>
          <abv:tr xml:lang="ru">
            <q><note xml:lang="ru" type="comment">(слабые) </note><note xml:lang="en" type="comment"
                >(слабые) </note>признаки жизни</q>
          </abv:tr>
          <abv:tr xml:lang="en">
            <q><note xml:lang="ru" type="comment">(слабые) </note><note xml:lang="en" type="comment"
                >(weak) </note>life signs</q>
          </abv:tr>
        </sense>
        <abv:exampleGrp xml:id="exampleGrp_d4938e75">
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">k˳yd tæntʼīxæg ū, ūdysmag ʒy k˳y næ
              īs</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">до чего он слаб, он еле живой</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">how weak he is, he is barely alive</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Нафи"/>
              <biblScope>72</biblScope></bibl>
          </example>
        </abv:exampleGrp>
        <etym xml:lang="ru">Сложение из <ref type="xr" target="#entry_ūd"
                ><w>ūd</w><gloss><q>душа</q></gloss>, <gloss><q>жизнь</q></gloss></ref> и <ref
            type="xr" target="#entry_smag"><w>smag</w><gloss><q>запах</q></gloss></ref>.</etym>
        <etym xml:lang="en">Compound from <ref type="xr" target="#entry_ūd"
                ><w>ūd</w><gloss><q>soul</q></gloss>, <gloss><q>life</q></gloss></ref> and <ref
            type="xr" target="#entry_smag"><w>smag</w><gloss><q>smell</q></gloss></ref>.</etym>
      </entry>
