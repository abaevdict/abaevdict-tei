    <text>
        <body>
            <entry xml:id="entry_bæxsnyg" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>bæxsnyg</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>bæxsnug</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>суровая нитка из конопли</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>coarse thread made of hemp</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>xufijnæj don xaston, æsmesæj bæxsnug bijun</quote>
                        <abv:tr xml:lang="ru">
                            <q>в решете носила воду, из песка вью бечевку</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>I carried water in a sieve, I twisted twine from sand</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                            173</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>Zulemæt ærista særibæddænæj ærcij æd bæxsnug</quote>
                        <abv:tr xml:lang="ru">
                            <q>Зулемат сняла с косынки трехгранную иглу с ниткой</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Zulemat removed a triangular needle with thread from a scarf</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>123</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>sapoznikki bæxsnugutæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>сапожные нитки</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>boot needles</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_SD"
                            /><biblScope>245%l3%l</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>bændæn æma bæxsnug bijæn fabriktæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>фабрики для витья веревок и ниток</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>factories for twisting of ropes and threads</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_FS"/><biblScope>V 122</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Может быть, из *%imæxsnyg%i, к <mentioned xml:lang="fa"><lang/>
                        <w>mašnak</w><note type="comment" xml:lang="ru"> (<bibl><ref type="bibl"
                                    target="#ref_Ягелло"/></bibl>)</note>
                        <gloss><q>сорт веревки</q></gloss></mentioned>, <mentioned xml:lang="fa"
                            ><w/><note type="comment" xml:lang="ru"
                                    ><bibl><biblScope>Riсhаrdsоn</biblScope></bibl></note>
                        <gloss><q>rope</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Maybe from *%imæxsnyg%i, to <mentioned xml:lang="fa"><lang/>
                        <w>mašnak</w><note type="comment" xml:lang="ru"> (<bibl><ref type="bibl"
                                    target="#ref_Ягелло"/></bibl>)</note><note type="comment"
                            xml:lang="en"> (<bibl><ref type="bibl" target="#ref_Ягелло"
                            /></bibl>)</note>
                        <gloss><q>a kind of rope</q></gloss></mentioned>, <mentioned xml:lang="fa"
                            ><w/><note type="comment" xml:lang="ru"
                                    ><bibl><biblScope>Riсhаrdsоn</biblScope></bibl></note><note
                            type="comment" xml:lang="en"
                                ><bibl><biblScope>Riсhаrdsоn</biblScope></bibl></note>
                        <gloss><q>rope</q></gloss></mentioned>.</etym>
            </entry>
