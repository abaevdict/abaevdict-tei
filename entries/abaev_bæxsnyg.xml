<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_bæxsnyg" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>bæxsnyg</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>bæxsnug</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>суровая нитка из конопли</q>
        </cit><cit type="translation" xml:lang="en">
            <q>coarse thread made of hemp</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>xufijnæj don xaston, æsmesæj bæxsnug bijun</quote>
            <cit type="translation" xml:lang="ru">
                <q>в решете носила воду, из песка вью бечевку</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I carried water in a sieve, I twisted twine from sand</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>II 173</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>Zulemæt ærista særibæddænæj ærcij æd bæxsnug</quote>
            <cit type="translation" xml:lang="ru">
                <q>Зулемат сняла с косынки трехгранную иглу с ниткой</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Zulemat removed a triangular needle with thread from a scarf</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>123</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>sapoznikki bæxsnugutæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>сапожные нитки</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>boot needles</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>245%l3%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>bændæn æma bæxsnug bijæn fabriktæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>фабрики для витья веревок и ниток</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>factories for twisting of ropes and threads</q>
            </cit>
            <bibl><ref target="#src_FS"/><biblScope>V 122</biblScope></bibl>
        </cit>
    </cit>
