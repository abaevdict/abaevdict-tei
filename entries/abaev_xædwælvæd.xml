      <entry xml:id="entry_xædwælvæd" xml:lang="os-x-iron" abv:completeness="incomplete">
        <form xml:id="form_d1553e66" type="lemma"><orth>xædwælvæd</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d1553e69">
          <abv:tr xml:lang="ru">
            <q>тотчас после, вслед</q>
          </abv:tr>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dūǧættæj īw næ ræzty rawadī, jæ xædwælvæd
              īnnæbaræg dær razyndī</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">один из участников скачки пронесся мимо нас,
                тотчас вслед за ним показался и другой всадник</q>
            </tr>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_xæd-"/> и <ref type="xr"
            target="#entry_wælvæd"/>.</etym>
