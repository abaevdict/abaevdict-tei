    <etym xml:lang="ru">Слово имеет соответствия в кавказских и угро-финских языках: <mentioned
            xml:lang="x-vaynakh"
            ><lang/><w>čʼug</w><gloss><q>кольцо</q></gloss></mentioned><mentioned
            xml:lang="x-vaynakh"><w/><gloss><q>серьга</q></gloss></mentioned>, <mentioned
            xml:lang="x-vaynakh"><w/><gloss><q>крючок</q></gloss></mentioned>, , <mentioned
            xml:lang="x-crc"><lang/><w>čʼukʼ</w><gloss><q>крючок</q></gloss></mentioned> (<bibl><ref
                target="#ref_СМК"/><biblScope>XXV 93</biblScope></bibl>), <mentioned
            xml:lang="krc-x-balkar"
            ><lang/><w>čək</w><gloss><q>узел</q></gloss></mentioned><mentioned
            xml:lang="krc-x-balkar"><w/><gloss><q>петля</q></gloss></mentioned>, <mentioned
            xml:lang="krc-x-balkar"><w/><gloss><q>звено</q></gloss></mentioned>, , <mentioned
            xml:lang="sva"><lang/><w>cəg</w><gloss><q>подпятник мельницы</q></gloss></mentioned>,
            <mentioned xml:lang="kv"
            ><lang/><w>dzug</w><gloss><q>крюк</q></gloss></mentioned><mentioned xml:lang="kv"
                ><w>džag</w><gloss><q>петля</q></gloss></mentioned>, <mentioned xml:lang="kv"
                    ><w/><gloss><q>узел</q></gloss></mentioned>, , <mentioned xml:lang="hu"
                ><lang/><w>czege</w><gloss><q>clausura</q></gloss></mentioned>. — <mentioned
            xml:lang="ae"><lang/><w>čaku-</w><gloss><q>метательное
            оружие</q></gloss></mentioned><mentioned xml:lang="ae"
            ><w/><gloss><q>молот</q></gloss></mentioned>, , <mentioned xml:lang="fa"
                ><lang/><w>čak</w><gloss><q>вид деревянного молотка</q></gloss></mentioned> вряд ли
        имеют сюда отношение.%n<bibl><ref target="#ref_ОЯФ_I"
        /><biblScope>298</biblScope></bibl>.</etym>
    <etym xml:lang="en">The word has correspondences in Caucasian and Finno-Ugric languages:
            <mentioned xml:lang="x-vaynakh"
            ><lang/><w>čʼug</w><gloss><q>ring</q></gloss></mentioned><mentioned xml:lang="x-vaynakh"
                    ><w/><gloss><q>earring</q></gloss></mentioned>, <mentioned xml:lang="x-vaynakh"
                    ><w/><gloss><q>hook</q></gloss></mentioned>, , <mentioned xml:lang="x-crc"
                ><lang/><w>čʼukʼ</w><gloss><q>hook</q></gloss></mentioned> (<bibl><ref
                target="#ref_СМК"/><biblScope>XXV 93</biblScope></bibl>), <mentioned
            xml:lang="krc-x-balkar"
            ><lang/><w>čək</w><gloss><q>knot</q></gloss></mentioned><mentioned
            xml:lang="krc-x-balkar"><w/><gloss><q>loop</q></gloss></mentioned>, <mentioned
            xml:lang="krc-x-balkar"><w/><gloss><q>link</q></gloss></mentioned>, , <mentioned
            xml:lang="sva"><lang/><w>cəg</w><gloss><q>mill bearing</q></gloss></mentioned>,
            <mentioned xml:lang="kv"
            ><lang/><w>dzug</w><gloss><q>hook</q></gloss></mentioned><mentioned xml:lang="kv"
                ><w>džag</w><gloss><q>loop</q></gloss></mentioned>, <mentioned xml:lang="kv"
                    ><w/><gloss><q>knot</q></gloss></mentioned>, , <mentioned xml:lang="hu"
                ><lang/><w>czege</w><gloss><q>clausura</q></gloss></mentioned>. — <mentioned
            xml:lang="ae"><lang/><w>čaku-</w><gloss><q>throwing
            weapon</q></gloss></mentioned><mentioned xml:lang="ae"
            ><w/><gloss><q>hammer</q></gloss></mentioned>, , <mentioned xml:lang="fa"
                ><lang/><w>čak</w><gloss><q>a kind of wooden hammer</q></gloss></mentioned> unlikely
        refer here.%n<bibl><ref target="#ref_ОЯФ_I"/><biblScope>298</biblScope></bibl>.</etym>
