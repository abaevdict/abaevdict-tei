    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>oj færstæ æcqærcægæj bid iskind æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>его (дома) стены оплетены кольчужными кольцами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>its (of the house) walls are braided with chain armour chains</q>
            </cit>
            <bibl><ref target="#src_Диг._сказ."/><biblScope>49</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>cæggag</quote>
            <cit type="translation" xml:lang="ru">
                <q rendition="singlequotes">прут, из которого делалось деревянное кольцо</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q rendition="singlequotes">the rod used for making wooden rings</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>bærzæ cæggag</quote>
            <cit type="translation" xml:lang="ru">
