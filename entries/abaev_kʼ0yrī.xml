<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_kʼ0yrī" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>kʼ˳yrī</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>kʼwære</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>неделя</q>
        </cit><cit type="translation" xml:lang="en">
            <q>week</q>
        </cit></sense>
    <re><form type="lemma"><orth>kʼ˳yrīsær</orth></form><sense><cit type="translation" xml:lang="ru">
                <q>понедельник</q>
            </cit><cit type="translation" xml:lang="en">
                <q>Monday</q>
            </cit></sense></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>mæ fyd īw kʼ˳yrīgaj ʒæbīdyrʒwan cyd</quote>
            <cit type="translation" xml:lang="ru">
                <q>мой отец по неделям охотился за турами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my father hunted tours for weeks</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>54</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>avd kʼwærej cæwuj, oj razæj æz fesafton Ʒanʒiraqi</quote>
            <cit type="translation" xml:lang="ru">
                <q>семь недель прошло, как я погубил Дзандзирак</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>seven weeks have passed since I destroyed Zanzirak</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>43</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>Zærdæværæny razmæ kʼ˳yrīsær bony ūcy qæwæn sæ ʒwary bon væjjy</quote>
            <cit type="translation" xml:lang="ru">
                <q>перед Троицей в понедельник бывает праздник святого-покровителя этого аула</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>on Monday before the Trinity there is sometimes a feast of the saint patron of
                    this village</q>
            </cit>
            <bibl><ref target="#src_Сека"/><biblScope>128</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>Tūtyræn jæ kʼ˳yrīsær, dyccæg æmæ rtyccæg dæ q˳yddag kæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>понедельник, вторник и среду (недели божества) Тутыр делай свои дела</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>do your things on Monday, Tuesday and Wednesday of (the week of deity) Tutyr</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>73</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>kʼwære bælasæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>недельное дерево</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>week tree</q>
            </cit>
            <note type="comment" xml:lang="ru"> (название обряда в честь новорожденного)</note>
            <note type="comment" xml:lang="en"> (name of the ceremony in the honour of a
                newborn)</note>
            <bibl><ref target="#src_СОПам."/><biblScope>II 122</biblScope></bibl>
            <bibl><ref target="#src_ОЭп."/><biblScope>63</biblScope></bibl>
        </cit>
    </cit>
