<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_goc"
    xml:lang="os-x-digor">
    <form type="lemma"><orth>goc</orth></form>
    <re><form type="lemma"><orth>goc kænun</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>переходить</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>cross</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>переселяться</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>move</q>
                </cit></sense></sense></re>
    <re><form type="lemma"><orth>gocgænagæ (marǧ)</orth></form><sense><cit type="translation"
                xml:lang="ru">
                <q>перелетная (птица)</q>
            </cit><cit type="translation" xml:lang="en">
                <q>migratory (bird)</q>
            </cit></sense></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>gocgænagæ mærǧtæ næmæ æzdæxun bajdædtoncæ bælttæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>перелетные птицы стали возвращаться к нам стаями</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>migratory birds began to return to us in packs</q>
            </cit>
            <bibl><ref target="#src_AK"/><biblScope>I 55</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>gocgænagæ mærǧtæ fættaxtæncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>перелетные птицы улетели</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>migratory birds flew away</q>
            </cit>
            <bibl><ref target="#src_AK"/><biblScope>I 70</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>gocgænagæ mærǧtæ næmæ æzdæxun bajdædtoncæ bælttæj</quote>
