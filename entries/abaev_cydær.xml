<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_cydær"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>cydær</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>cidær</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>что-то</q>
        </cit>
    </sense>
    <re>
        <form xml:lang="os-x-iron" type="lemma">
            <orth>cydæriddær</orth>
        </form>
        <form xml:lang="os-x-digor" type="dialectal">
            <orth>cidæriddær</orth>
        </form>
        <sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>что бы ни</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>whatever</q>
                </cit>
            </sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>все</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>ever</q>
                </cit>
            </sense>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>æz x˳ycæwæj ændær cydær kūryn</quote>
            <cit type="translation" xml:lang="ru">
                <q>я прошу у бога чего-то другого</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>i ask god for something else</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>35</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>īwyl, cydæriddær lasta jæ ḱyræty, ... maxmæ ærxawdtoj</quote>
            <cit type="translation" xml:lang="ru">
                <q>все, что он вез в своем обозе, досталось нам</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>everything that he carried in his wagon train went to us</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>155</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>cidær adtæj, æ fusun Oræzmægmæ ærkodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>как бы то ни было, он остановился у Оразмага</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>anyways, he stopped at Orazmag's</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>II 58</biblScope>
            </bibl>
        </cit>
    </cit>
