    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>оцепеневший</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>petrified</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>остолбеневший</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stuporous</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>с остановившимся взглядом</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>with a fixed gaze</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>fæǵīx wyn</orth>
        </form>
        <sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>остолбенеть</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>be dumbfounded</q>
                </cit>
            </sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>уставиться взглядом</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>stare</q>
                </cit>
            </sense>
        </sense>
    </re>
    <re xml:lang="os-x-digor">
        <lang/>
        <form type="lemma">
            <orth>kʼex-kast</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>остановившийся, пристальный взгляд</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>fixed gaze</q>
            </cit>
        </sense>
    </re>
    <re xml:lang="os-x-digor">
