            <entry xml:id="entry_izærdalyng6tæ9" xml:lang="os">
                <form xml:id="form_d4111e66" xml:lang="os-x-iron" type="lemma"><orth>izærdalyng(tæ)</orth></form>
                <form xml:id="form_d4111e69" xml:lang="os-x-digor" type="dialectal"><orth>izærdaling(itæ)</orth></form>
                <sense xml:id="sense_d4111e72"><abv:tr xml:lang="ru">
                        <q>сумерки</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>twilight</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d4111e84" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>izærdalingitæ kænun bajdædta</quote>
                        <abv:tr xml:lang="ru">
                            <q>начало смеркаться</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>it started to grow dark</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Bes._Ka_ke"/><biblScope>60</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">См. <ref type="xr" target="#entry_izær"/> ‘вечер’ и <ref type="xr" target="#entry_talyng"/> ‘темный’, ‘темнота’.</etym>
                <etym xml:lang="en">See <ref type="xr" target="#entry_izær"/> ‘evening’ and <ref type="xr" target="#entry_talyng"/> ‘dark’, ‘darkness’.</etym>
