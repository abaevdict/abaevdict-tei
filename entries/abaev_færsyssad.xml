            <entry xml:id="entry_færsyssad" xml:lang="os">
                <form xml:id="form_d3446e66" xml:lang="os-x-iron" type="lemma"><orth>færsyssad</orth></form>
                <form xml:id="form_d3446e69" xml:lang="os-x-digor" type="dialectal"><orth>færsinsad</orth></form>
                <sense xml:id="sense_d3446e72"><sense xml:id="sense_d3446e73"><abv:tr xml:lang="ru">
                            <q>отточенный с боков</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>sharpened at the sides</q>
                        </abv:tr></sense><sense xml:id="sense_d3446e82"><abv:tr xml:lang="ru">
