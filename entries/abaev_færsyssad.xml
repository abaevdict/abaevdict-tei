            <entry xml:id="entry_færsyssad" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>færsyssad</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>færsinsad</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>отточенный с боков</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>sharpened at the sides</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>хорошо отточенный</q>
                        </abv:tr><abv:tr xml:lang="en">
