            <entry xml:id="entry_axsæv" xml:lang="os" abv:completeness="complete">
                <?oxy_comment_start author="oleg" timestamp="20240124T093831+0300" comment="checked"?>
                <form xml:id="form_d2858e66" xml:lang="os-x-iron" type="lemma"
                    ><orth>axsæv</orth></form><?oxy_comment_end?>
                <form xml:id="form_d2858e69" xml:lang="os-x-digor" type="dialectal"
                        ><orth>axsævi</orth></form>
                <sense xml:id="sense_d2858e72">
                    <abv:tr xml:lang="ru">
                        <q>сегодня ночью</q>
                    </abv:tr>
                    <abv:tr xml:lang="en">
                        <q>tonight</q>
                    </abv:tr>
                </sense>
                <etym xml:lang="ru">Из <ref type="xr" target="#entry_a"><w>a</w>
                        <gloss><q>этот</q></gloss></ref> и <ref type="xr" target="#entry_xsæv"
                            ><w>xsæv</w>
                        <gloss><q>ночь</q></gloss></ref>. <mentioned xml:lang="os"
                            ><gloss>Adj.</gloss>
                        <w xml:lang="os-x-iron">axsævykkon</w><w xml:lang="os-x-digor"
                            >axsævikkon</w></mentioned>. См. <ref type="xr" target="#entry_a"/> и
                        <ref type="xr" target="#entry_xsæv"/>; ср. <ref type="xr"
                        target="#entry_abon"/>, <ref type="xr" target="#entry_ībon"/>.<lb/><bibl>Вс.
                            <author>Миллер</author>. <ref type="bibl" target="#ref_Miller._Gr."
                            >Gr.</ref>
                        <biblScope>85</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <ref type="xr" target="#entry_a"><w>a</w>
                        <gloss><q>this</q></gloss></ref> and <ref type="xr" target="#entry_xsæv"
                            ><w>xsæv</w>
                        <gloss><q>night</q></gloss></ref>. <mentioned xml:lang="os"
                            ><gloss>Adj.</gloss>
                        <w xml:lang="os-x-iron">axsævykkon</w><w xml:lang="os-x-digor"
                            >axsævikkon</w></mentioned>. See <ref type="xr" target="#entry_a"/> and
                        <ref type="xr" target="#entry_xsæv"/>; cf. <ref type="xr"
                        target="#entry_abon"/>, <ref type="xr" target="#entry_ībon"/>.<lb/><bibl>Ws.
                            <author>Miller</author>. <ref type="bibl" target="#ref_Miller._Gr."
                            >Gr.</ref>
                        <biblScope>85</biblScope></bibl></etym>
            </entry>
