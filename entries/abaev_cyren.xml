<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_cyren"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>cyren</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>ciren</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>яркое пламя</q>
        </cit><cit type="translation" xml:lang="en">
            <q>clear flame</q>
        </cit></sense>
    <re><form type="lemma"><orth>cyren kænyn</orth><form type="variant">(<orth>cyryn
                kænyn</orth>)</form></form><sense><sense><cit type="translation" xml:lang="ru">
                    <q>возбуждать</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>excite</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>разжигать</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>rouse</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>стимулировать</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>stimulate</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>подстрекать</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>provoke</q>
                </cit></sense></sense></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>cyraǧy cyren fæjlawæntæ kodta alyrdæm</quote>
            <cit type="translation" xml:lang="ru">
                <q>пламя свечи металось в разные стороны</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the flame of the candle was rushing in different directions</q>
            </cit>
            <bibl><ref target="#src_Qazaq"/><biblScope>79</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>(cigonæn) æ buni sagi siwæj ciren art æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>под ним (чугунным котлом) — яркий огонь из оленьих рогов</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>under it (cast-iron kettle) there is a clear fire made of antlers</q>
            </cit>
            <bibl><ref target="#src_Диг._сказ."/><biblScope>49%l16%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>dæn nurtækkæ æz ævzalu, fal festʒænæn ciren ārt</quote>
            <cit type="translation" xml:lang="ru">
                <q>я сейчас (холодный) уголь, но стану бурным пламенем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I am now (cold) coal, but I will become a violent flame</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>111</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>cʼina ...æ astæwæj isciren kænidæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>стог загорался ярким пламенем из середины</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the haystack lit up from the middle with a bright flame</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>II 116</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>næ bæstæ — qwæcæ, ciren</quote>
            <cit type="translation" xml:lang="ru">
                <q>наш край (объят) дымом, пламенем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>our land is (enveloped) in smoke, flame</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>292%l3%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>vagoni farsbæl cireni xuzæn pærpær kodta syrx turusa</quote>
            <cit type="translation" xml:lang="ru">
                <q>на стене вагона подобно пламени волновалось красное знамя</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a red banner fluttered like a flame on the wall of the car</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>265%l2%l</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>cæfsuj i ǧæw ...ciren pellontæ æskʼæruj</quote>
            <cit type="translation" xml:lang="ru">
                <q>горит аул, полышет бурное пламя</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>aul is burning, a stormy flame is flaring</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>74</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>...cʼæx ciren waǧta</quote>
            <cit type="translation" xml:lang="ru">
                <q>...испускал синее пламя</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>... emitted a blue flame</q>
            </cit>
            <bibl><ref target="#src_Bes."/><biblScope>109</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>šyryn syǧd kodtoj</quote>
            <cit type="translation" xml:lang="ru">
                <q>горели ярким пламенем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[they] were burning with a clear flame</q>
            </cit>
            <bibl><ref target="#src_ЮОПам."/><biblScope>II 121</biblScope></bibl>
        </cit>
    </cit>
