<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dæxædæg" xml:lang="os">
    <form type="lemma">
        <orth>dæxædæg</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>ты сам</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>thy thyself</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>dæxædæg myn bæzz</quote>
            <cit type="translation" xml:lang="ru">
                <q>ты сам будь годен</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>may thy be good thyself</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>88</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>kæd dæxwædæg næ xwæcis, wæddær min eske bamonæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>если ты сам не борешься, то укажи мне кого-нибудь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>if thy do not fight thyself, then point me someone</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>24%l6%l</biblScope>
            </bibl>
        </cit>
    </cit>
