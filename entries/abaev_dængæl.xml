<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dængæl" xml:lang="os">
    <form type="lemma">
        <orth>dængæl</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>раздувшийся</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>bloated</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>распухший</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>swollen</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>nyddængæl wyn</orth>
        </form>
        <sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>раздуться</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>swell up</q>
                </cit>
            </sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>распухнуть</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>bloat</q>
                </cit>
            </sense>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ūdon æxsæn Dadaj dær saw-dængælæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>среди них (был) и Дадай, мрачный, надутый</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>among them there was also Dadai, gloomy, bloated</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
                <biblScope>116</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>typpyr æmæ dængælæj amælaj!</quote>
            <cit type="translation" xml:lang="ru">
                <q>чтоб тебе околеть раздувшимся и распухшим</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>may you die swollen and bloated!</q>
            </cit>
            <bibl>
                <biblScope xml:lang="ru">бранное выражение</biblScope>
                <biblScope xml:lang="en">pejorative expression</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>nittuppur, niddængæl æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>он раздулся, распух</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he became swollen, bloated</q>
            </cit>
        </cit>
    </cit>
