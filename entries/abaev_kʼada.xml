            <cit type="translation" xml:lang="ru">
                <q>как разлившаяся река</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>like a flooded river</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>118</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
