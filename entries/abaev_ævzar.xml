<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ævzar"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>ævzar</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>ævzaræ</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>всход (о растениях)</q>
            </cit><cit type="translation" xml:lang="en">
                <q>sprout</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>зародыш</q>
            </cit><cit type="translation" xml:lang="en">
                <q>embryo</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>næ x˳ymtæ dīssaǵy xorz ævzar yswaǧtoj</quote>
            <cit type="translation" xml:lang="ru">
                <q>наши нивы пустили удивительно хорошие всходы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>our cornfields gave surprisingly good sprouts</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>Kjabæxan ævzarti æxsænti cæwuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>Кябахан идет между всходами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Kyabakhan goes between the sprouts</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>200</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>ūs ūdgojmaǵy ævzaræj ærwæzzaw</quote>
            <cit type="translation" xml:lang="ru">
                <q>женщина отяжелела (забеременела) от человеческого зародыша</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the woman became pregnant from a human embryo (?)</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>IV 47</biblScope></bibl>
        </cit>
    </cit>
