            <entry xml:id="entry_cʼass" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>cʼass</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>cʼassæ</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>щель</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>chink</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>скважина</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>well</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>ущелье</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>gorge</q>
                        </abv:tr></sense><sense><note type="comment" xml:lang="ru">в дигорском
                            вообще</note><note type="comment" xml:lang="en">in Digor in
                            general</note><abv:tr xml:lang="ru">
                            <q>дыра</q>
                        </abv:tr><note type="comment" xml:lang="ru">ср. и. <ref type="xr"
                                target="#entry_x0ynkʼ"/></note><note type="comment" xml:lang="en"
                            >cf. Iron <ref type="xr" target="#entry_x0ynkʼ"/></note><note
                            type="comment" xml:lang="ru">в дигорском вообще</note><note
                            type="comment" xml:lang="en">in Digor in general</note><abv:tr
                            xml:lang="en">
                            <q>hole</q>
                        </abv:tr><note type="comment" xml:lang="ru">ср. и. <ref type="xr"
                                target="#entry_x0ynkʼ"/></note><note type="comment" xml:lang="en"
                            >cf. Iron <ref type="xr" target="#entry_x0ynkʼ"/></note></sense></sense>
                <re>
                    <form type="lemma"><orth>хоху cʼass</orth></form>
                    <sense><sense><abv:tr xml:lang="ru">
                                <q>горная глушь</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>mountain thicket</q>
                            </abv:tr></sense><sense><abv:tr xml:lang="ru">
                                <q>горная трущоба</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>mountain hovel</q>
                            </abv:tr></sense></sense>
                </re>
                <re xml:lang="os-x-digor">
                    <lang/>
                    <form type="lemma"><orth>finʒi cʼasæ</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>ноздря</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>nostril</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>aly sætælæg dær jæxi сʼassy læzæry</quote>
                        <abv:tr xml:lang="ru">
                            <q>всякий слизняк околевает в своей ракушке (дыре)</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>every slug dies in its shell (hole)</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_ЮОПам."/><biblScope>III
                            191</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>neftgun zænxæ nicʼcʼasæ kænuncæ bunmæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>в нефтеносной земле делают скважину вниз</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a well is made in the oil containing soil</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_FS"/><biblScope>V 81</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="x-vaynakh"><lang/>
                        <w>čʼaz</w></mentioned>, <mentioned xml:lang="x-vaynakh"><w>čʼož</w>
                        <gloss><q>ущелье</q></gloss></mentioned>, а также <mentioned xml:lang="trk"
                        dialect="ug"><lang/>
                        <w>čat</w>
                        <gloss><q>щель</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug"><w/>
                        <gloss><q>ущелье</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="x-vaynakh"><lang/>
                        <w>čʼaz</w></mentioned>, <mentioned xml:lang="x-vaynakh"><w>čʼož</w>
                        <gloss><q>gorge</q></gloss></mentioned>, and also <mentioned xml:lang="trk"
                        dialect="ug"><lang/>
                        <w>čat</w>
                        <gloss><q>chink</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug"><w/>
                        <gloss><q>gorge</q></gloss></mentioned>.</etym>
            </entry>
