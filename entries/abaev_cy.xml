    <form xml:lang="os-x-iron" type="lemma"><orth>cy</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>ci</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>что</q>
        </cit><note type="comment" xml:lang="ru"> (вопросительное и относительное
            местоимение)</note><note type="comment" xml:lang="en"> (interrogative and relative
            pronoun)</note><cit type="translation" xml:lang="en">
            <q>what</q>
        </cit><note type="comment" xml:lang="ru"> (вопросительное и относительное
            местоимение)</note><note type="comment" xml:lang="en"> (interrogative and relative
            pronoun)</note></sense>
    <re><re><form type="lemma"><orth>nicy</orth></form></re><re><form type="lemma"
                ><orth>amcy</orth></form><sense><sense><cit type="translation" xml:lang="ru">
                        <q>ничто</q>
                    </cit><cit type="translation" xml:lang="en">
                        <q>nothing</q>
                    </cit></sense><sense><cit type="translation" xml:lang="ru">
                        <q>ничего</q>
                    </cit></sense></sense></re><re><form type="lemma"
                    ><orth>alcy</orth></form><sense><cit type="translation" xml:lang="ru">
                    <q>все</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>everything</q>
                </cit></sense></re><re><form type="lemma"><orth>isty</orth></form><sense><cit
                    type="translation" xml:lang="ru">
                    <q>что-либо</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>something</q>
                </cit><note type="comment" xml:lang="ru">(из %iiscy%i) </note><note type="comment"
                    xml:lang="en">(from %iiscy%i) </note></sense></re><re><form type="lemma"
                    ><orth>cydær</orth></form><sense><cit type="translation" xml:lang="ru">
                    <q>что-то</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>something</q>
                </cit></sense></re></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ūm ta cy mī kænūt?</quote>
            <cit type="translation" xml:lang="ru">
                <q>что вы там делаете?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what are you doing there?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>101</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су kodtaj, су?</quote>
            <cit type="translation" xml:lang="ru">
                <q>что с тобой случилось?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what happened to you?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>100</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ci kænuj cuma me ʽlxujnæ?</quote>
            <cit type="translation" xml:lang="ru">
                <q>что с моим веретеном?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what happened to my spindle?</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>21</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ci ærcudæj?</quote>
            <cit type="translation" xml:lang="ru">
                <q>что случилось?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what happened?</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>21</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су k˳ystæn bæzzydtæn?</quote>
            <cit type="translation" xml:lang="ru">
                <q>для какой работы я был пригоден?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what job was I qualified for?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>55</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су xosʒaw wydtæn!</quote>
            <cit type="translation" xml:lang="ru">
                <q>каким я был косарем!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what a chopper I was!</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>57</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ci xabar zæǧaj?</quote>
            <cit type="translation" xml:lang="ru">
                <q>какую весть скажешь?“</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>what message will you say?</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>34</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су fæci mæ zæxx?</quote>
            <cit type="translation" xml:lang="ru">
                <q>куда девалась моя земля?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>where has my land disappeared?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>57</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су fædæ, kæm dæ?</quote>
            <cit type="translation" xml:lang="ru">
                <q>куда ты девался, где ты?</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>where have you gone, where are you?</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>62</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>су nymæt cæǧdaj, — q˳ynǵyndær</quote>
            <cit type="translation" xml:lang="ru">
                <q>бурка, которую трясешь, (становится) ворсистей</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the felt cloak you shake (becomes) hairy</q>
            </cit>
            <bibl><biblScope xml:lang="ru">поговорка</biblScope><biblScope xml:lang="en"
                    >saying</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>ḱī су agura, ūj yssary</quote>
            <cit type="translation" xml:lang="ru">
                <q>кто что ищет, то и находит</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>all good things come to he who waits</q>
            </cit>
            <bibl><biblScope xml:lang="ru">поговорка</biblScope><biblScope xml:lang="en"
                    >saying</biblScope></bibl>
        </cit>
    </cit>
