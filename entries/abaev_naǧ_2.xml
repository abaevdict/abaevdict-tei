        <usg><lang/></usg>
        <sense><abv:tr xml:lang="ru">
            <q>мякоть</q>
          </abv:tr><note type="comment"> (?)</note></sense>
        <re>
          <form type="lemma"><orth>saw naǧ</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">филейная часть</q>
