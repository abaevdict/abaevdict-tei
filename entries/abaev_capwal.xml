            <entry xml:id="entry_capwal" xml:lang="os">
                <form type="lemma"><orth>capwal</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>бег лошади</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>horse race</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>dæ bæxæn jæ capwal dissag ū</quote>
                        <abv:tr xml:lang="ru">
                            <q>у твоей лошади удивительный бег</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>your horse has a wonderful race</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="trk"><lang/>
                        <w>čapa</w>
                        <gloss><q>рысь</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>галоп</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>бег</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>езда</q></gloss></mentioned>, <mentioned xml:lang="trk"
                            ><w>čapal</w>
                        <gloss><q>быть пущену вскачь</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Радлов"/><biblScope>III 1920</biblScope></bibl>).</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="trk"><lang/>
                        <w>čapa</w>
                        <gloss><q>trot</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>gallop</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>run</q></gloss></mentioned>, <mentioned xml:lang="trk"><w/>
                        <gloss><q>ride</q></gloss></mentioned>, <mentioned xml:lang="trk"
                            ><w>čapal</w>
                        <gloss><q>be broken into a gallop</q></gloss></mentioned> (<bibl><ref
                            type="bibl" target="#ref_Радлов"/><biblScope>III
                    1920</biblScope></bibl>).</etym>
            </entry>
