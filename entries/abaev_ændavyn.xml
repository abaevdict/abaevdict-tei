<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ændavyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>ændavyn</orth>
        <form type="inflected" subtype="ptcp">
            <orth>ændævd</orth>
        </form>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>ændavun</orth>
        <form type="inflected" subtype="ptcp">
            <orth>ændavd</orth>
        </form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>греть</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to warm</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>давать себя чувствовать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to affect smb.</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>трогать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to touch</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>xūr dardmæ ændavy</quote>
            <cit type="translation" xml:lang="ru">
                <q>солнце греет на далекое расстояние</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the sun heats for a great distance</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>xūr tyngdær ærændævta, mīt hæjttæj tadī</quote>
            <cit type="translation" xml:lang="ru">
                <q>солнце сильнее пригрело, снег бурно таял</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the sun warmed up more, the snow was rapidly melting</q>
            </cit>
            <bibl>
                <ref target="#src_F."/>
                <biblScope>1957 III 30</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>wazal ændagddær ændavuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>холод сильнее дает себя чувствовать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>it s getting colder here</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>78</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>dæ nyxæstæ mæ ne ʽndavync</quote>
            <cit type="translation" xml:lang="ru">
                <q>твои слова меня не трогают</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your words don’t touch me</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>fælladæj sæ wajtaǧd tarf fynæj ærændævta</quote>
            <cit type="translation" xml:lang="ru">
                <q>усталых, их сразу охватил глубокий сон</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>as they were tired, they immediately fell asleep</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>II 144</biblScope>
            </bibl>
        </cit>
    </cit>
