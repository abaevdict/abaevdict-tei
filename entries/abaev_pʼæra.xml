        <usg><lang/></usg>
        <sense xml:id="sense_d5295e74"><abv:tr xml:lang="ru">
            <q>гнилое дерево</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>rotten wood</q>
          </abv:tr></sense>
        <abv:exampleGrp>
