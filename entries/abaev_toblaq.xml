            <def xml:lang="ru">растение <q>девясил</q></def>
            <def xml:lang="en">the plant <q>elecampane</q></def>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>Inula helenium</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>Inula helenium</q>
            </abv:tr>
