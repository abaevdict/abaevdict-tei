            <abv:tr xml:lang="ru">
              <q>развалины</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>руины</q>
            </abv:tr>
          </sense>
        </sense>
        <re>
          <form type="lemma"><orth>k˳yrojy xælccag</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
