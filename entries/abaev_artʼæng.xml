                <form xml:id="form_d1899e66" type="lemma"><orth>artʼæng</orth><form
                        xml:id="form_d1899e68" type="variant"><orth>art’ang</orth></form></form><?oxy_comment_end?>
                <sense xml:id="sense_d1899e71">
                    <abv:tr xml:lang="ru">
                        <q>ременная веревка у саней</q>
                    </abv:tr>
                    <abv:tr xml:lang="en">
                        <q>belt rope of a sled</q>
                    </abv:tr>
                </sense>
                <etym xml:lang="ru">Из <ref type="xr" target="#entry_ar"><w>ar</w>
                        <gloss><q>оглобля</q></gloss></ref> и <mentioned xml:lang="os"><w type="rec"
                            >tang</w>
                        <gloss><q>ремень</q></gloss></mentioned>, см. <ref type="xr"
                        target="#entry_æxtong"/>.</etym>
                <etym xml:lang="en">From <ref type="xr" target="#entry_ar"><w>ar</w>
                        <gloss><q>thill, shaft</q></gloss></ref> and <mentioned xml:lang="os"><w
                            type="rec">tang</w>
                        <gloss><q>belt</q></gloss></mentioned>, see <ref type="xr"
                        target="#entry_æxtong"/>.</etym>
