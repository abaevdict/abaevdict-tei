    <form xml:lang="os-x-iron" type="lemma"><orth>īskæd</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>eskæd</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>когда-нибудь</q>
        </cit><cit type="translation" xml:lang="en">
            <q>some time</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>mæ ʒurdtæn kæd arǧ iskæna eskæd</quote>
            <cit type="translation" xml:lang="ru">
                <q>авось она оценит когда-нибудь мои слова</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>maybe some time she will appreciate my words</q>
            </cit>
            <bibl><ref target="#src_Gurdž._Æduli"/><biblScope>99</biblScope></bibl>
        </cit>
    </cit>
