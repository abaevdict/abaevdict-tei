            <entry xml:id="entry_īskæd" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>īskæd</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>eskæd</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>когда-нибудь</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>some time</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>mæ ʒurdtæn kæd arǧ iskæna eskæd</quote>
                        <abv:tr xml:lang="ru">
                            <q>авось она оценит когда-нибудь мои слова</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>maybe some time she will appreciate my words</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Gurdž._Æduli"
