      <entry xml:id="entry_qærmædon" xml:lang="os-x-iron" abv:completeness="incomplete">
        <form type="lemma"><orth>qærmædon</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d2401e72"><abv:tr xml:lang="ru">
            <q>теплый минеральный источник</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>warm mineral spring</q>
          </abv:tr><note xml:lang="ru" type="comment">, в частности кавказские Минеральные
            воды</note><note xml:lang="en" type="comment">, in particular, Caucasian Mineral
            waters</note></sense>
        <etym xml:lang="ru">Из <ref type="xr" target="#entry_qarm"><w>qarm</w>
            <q>теплый</q></ref> и <ref type="xr" target="#entry_don_1"><w>don</w>
            <q>вода</q></ref>.</etym>
        <etym xml:lang="en">From <ref type="xr" target="#entry_qarm"><w>qarm</w>
            <q>warm</q></ref> and <ref type="xr" target="#entry_don_1"><w>don</w>
            <q>water</q></ref>.</etym>
