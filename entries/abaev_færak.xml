        <body>
            <entry xml:id="entry_færak" xml:lang="os">
                <form xml:id="form_d710e66" xml:lang="os-x-iron" type="lemma"><orth>færak</orth></form>
                <form xml:id="form_d710e69" xml:lang="os-x-digor" type="dialectal"><orth>færwakæ</orth></form>
                <sense xml:id="sense_d710e72"><abv:tr xml:lang="ru">
                        <q>отруби</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>bran</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Восходит к <mentioned corresp="#mentioned_d710e144" xml:id="mentioned_d710e84" xml:lang="ira"><lang/>
                        <w>*parvā-ka</w></mentioned>, где <hi rendition="#rend_italic">*parvā-</hi> отвечает <mentioned corresp="#mentioned_d710e149" xml:id="mentioned_d710e89" xml:lang="ine"><lang/>
                        <w>*pelvā-</w></mentioned>; ср. <mentioned corresp="#mentioned_d710e154" xml:id="mentioned_d710e94" xml:lang="inc-x-old"><lang/>
                        <w>palāva-</w>
                        <gloss><q>мякина</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e162" xml:id="mentioned_d710e102" xml:lang="inc-x-old"><w/>
                        <gloss><q>шелуха</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e168" xml:id="mentioned_d710e108" xml:lang="cu"><lang/>
                        <w n="0">плѣва</w>
                        <gloss><q>мякина</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e177" xml:id="mentioned_d710e118" xml:lang="ru"><lang/>
                        <w>полова, пелёва</w></mentioned>, <mentioned corresp="#mentioned_d710e182" xml:id="mentioned_d710e123" xml:lang="prg"><lang/>
                        <w>pelwo</w></mentioned>, <mentioned corresp="#mentioned_d710e187" xml:id="mentioned_d710e128" xml:lang="lv"><lang/>
                        <w>pelawas</w></mentioned>, <mentioned corresp="#mentioned_d710e192" xml:id="mentioned_d710e133" xml:lang="lt"><lang/>
                        <w>pelvas</w>
                        <gloss><q>мякина</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Traces back to <mentioned corresp="#mentioned_d710e84" xml:id="mentioned_d710e144" xml:lang="ira"><lang/>
                        <w>*parvā-ka</w></mentioned>, where <hi rendition="#rend_italic">*parvā-</hi> corresponds to <mentioned corresp="#mentioned_d710e89" xml:id="mentioned_d710e149" xml:lang="ine"><lang/>
                        <w>*pelvā-</w></mentioned>; cf. <mentioned corresp="#mentioned_d710e94" xml:id="mentioned_d710e154" xml:lang="inc-x-old"><lang/>
                        <w>palāva-</w>
                        <gloss><q>chaff</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e102" xml:id="mentioned_d710e162" xml:lang="inc-x-old"><w/>
                        <gloss><q>husk</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e108" xml:id="mentioned_d710e168" xml:lang="cu"><lang/>
                        <w>plěva</w>
                        <gloss><q>chaff</q></gloss></mentioned>, <mentioned corresp="#mentioned_d710e118" xml:id="mentioned_d710e177" xml:lang="ru"><lang/>
                        <w>polova, pelëva</w></mentioned>, <mentioned corresp="#mentioned_d710e123" xml:id="mentioned_d710e182" xml:lang="prg"><lang/>
                        <w>pelwo</w></mentioned>, <mentioned corresp="#mentioned_d710e128" xml:id="mentioned_d710e187" xml:lang="lv"><lang/>
                        <w>pelawas</w></mentioned>, <mentioned corresp="#mentioned_d710e133" xml:id="mentioned_d710e192" xml:lang="lt"><lang/>
                        <w>pelvas</w>
                        <gloss><q>chaff</q></gloss></mentioned>.</etym>
            </entry>
