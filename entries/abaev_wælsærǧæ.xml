      <entry xml:id="entry_wælsærǧæ" xml:lang="os-x-digor" abv:completeness="incomplete">
        <form xml:id="form_d5709e66" type="lemma"><orth>wælsærǧæ</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d5709e69">
          <abv:tr xml:lang="ru">
            <q>над</q>
          </abv:tr>
          <abv:tr xml:lang="ru">
            <q>возле</q>
          </abv:tr>
        </sense>
        <re>
          <form type="lemma"><orth>wælsærǧæmæ</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">к</q>
            </tr>
            <abv:tr xml:lang="ru">
              <q>вплотную</q>
            </abv:tr>
          </sense>
        </re>
        <abv:exampleGrp xml:id="exampleGrp_d5709e75">
          <abv:example xml:id="example_d5709e77">
            <quote>fædesæn sæ razæj racudæj eci saw Selæn ma sæ mardi <oRef>wælsærǧæmæ</oRef>
              baxudta </quote>
            <abv:tr xml:lang="ru">
              <q>впереди погони пошел тот черный <note type="comment">(пес)</note> Селан и привел их
                к мертвецу</q>
            </abv:tr>
            <bibl><ref type="bibl" target="#ref_СОПам."/><biblScope>II 131,
                185<hi>143</hi></biblScope></bibl>
          </abv:example>
        </abv:exampleGrp>
