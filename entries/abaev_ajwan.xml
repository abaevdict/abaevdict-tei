    <text>
        <body>
            <entry xml:id="entry_ajwan" xml:lang="os-x-digor">
                <form type="lemma"><orth>ajwan</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d447e72"><sense xml:id="sense_d447e73"><abv:tr xml:lang="ru">
                            <q>насмешка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>mockery</q>
                        </abv:tr></sense><sense xml:id="sense_d447e82"><abv:tr xml:lang="ru">
                            <q>издевка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>sneer</q>
                        </abv:tr></sense><sense xml:id="sense_d447e91"><abv:tr xml:lang="ru">
                            <q>глумление</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>mocking</q>
                        </abv:tr></sense></sense>
                <re xml:id="re_d447e101">
                    <form type="lemma"><orth>ajwan kænun</orth></form>
                    <sense xml:id="sense_d447e106"><abv:tr xml:lang="ru">
                            <q>издеваться</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>bully (v.)</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d447e119">
                        <quote>æ ʒurdtæ mæmæ ajwani xuzæn fækkastæncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>его слова показались мне издевкой</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>his words seemed to me as a mockery</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MD"/><biblScope>1956 VII
                            49</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d447e138">
                        <quote>Guʒunabæl ajwangængæ nixxoduncæ kʼæl-kʼælæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>насмехаясь над Гудзуна, они хохочут</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>they laugh while mocking at Gudzun</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>47</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:id="mentioned_d447e161" xml:lang="kbd"><lang/>
                        <w>awan</w>
                        <gloss><q>издевка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d447e169" xml:lang="kbd"><w/>
                        <gloss><q>глумление</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned xml:id="mentioned_d447e178" xml:lang="kbd"><lang/>
                        <w>awan</w>
                        <gloss><q>mockery</q></gloss></mentioned>, <mentioned xml:id="mentioned_d447e186" xml:lang="kbd"><w/>
                        <gloss><q>mocking</q></gloss></mentioned>.</etym>
            </entry>
