                <sense xml:id="sense_d5022e72"><sense xml:id="sense_d5022e73"><abv:tr xml:lang="ru">
                            <q>род</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>clan</q>
                        </abv:tr></sense><sense xml:id="sense_d5022e82"><abv:tr xml:lang="ru">
                            <q>поколение</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>generation</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d5022e94">
                        <quote>bajǧosetæ mæ kadængæmæ, næ ræsuǧd axil</quote>
                        <abv:tr xml:lang="ru">
                            <q>послушайте мое сказание, наше славное поколение</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>listen to my legend, our glorious generation</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>33</biblScope></bibl>
                    </abv:example>
