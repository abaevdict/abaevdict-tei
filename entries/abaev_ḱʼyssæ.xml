<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ḱʼyssæ" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>ḱʼyssæ</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>kʼinsæ</orth></form>
    <sense n="1" xml:id="sense_ḱʼyssæ.1" xml:lang="os-x-iron"><usg><lang/></usg><cit
            type="translation" xml:lang="ru">
            <q>детские изготовления из теста</q>
        </cit><cit type="translation" xml:lang="en">
            <q>things that children make from dough</q>
        </cit></sense>
    <sense n="2" xml:id="sense_ḱʼyssæ.2" xml:lang="os-x-digor"><usg><lang/></usg><cit
            type="translation" xml:lang="ru">
            <q>тесто</q>
        </cit><cit type="translation" xml:lang="en">
            <q>dough</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>syvællon ḱʼyssætæ kæny</quote>
            <cit type="translation" xml:lang="ru">
                <q>ребенок делает что-либо из теста (лепешки, пирожки)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the child makes something from the dough (flatbread, pies)</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>kæmi wotæ kʼinsæj fælmændær, kæmi mæsugaw ænæsæddgæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>то вы мягче теста, то тверды, как башня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one time you are softer than dough, another time you are as solid as a tower</q>
            </cit>
            <bibl><ref target="#src_Gurdž._Æduli"/><biblScope>78</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>syvællon ḱʼyssætæ kæny</quote>
            <cit type="translation" xml:lang="ru">
                <q>ребенок делает что-либо из теста (лепешки, пирожки)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the child makes something from the dough (flatbread, pies)</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>kæmi wotæ kʼinsæj fælmændær, kæmi mæsugaw ænæsæddgæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>то вы мягче теста, то тверды, как башня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one time you are softer than dough, another time you are as solid as a tower</q>
            </cit>
            <bibl><ref target="#src_Gurdž._Æduli"/><biblScope>78</biblScope></bibl>
        </cit>
    </cit>
