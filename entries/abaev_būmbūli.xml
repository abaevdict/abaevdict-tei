<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_būmbūli" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>būmbūli</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>bomboli</orth><form type="variant"
                ><orth>bunbuli</orth></form></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>пух</q>
        </cit><cit type="translation" xml:lang="en">
            <q>down feather</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>būmbūli baz</quote>
            <cit type="translation" xml:lang="ru">
                <q rendition="singlequotes">пуховая подушка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q rendition="singlequotes">down-pillow</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>būmbūlijy wæz dær ʒy næj</quote>
            <cit type="translation" xml:lang="ru">
                <q>в нем нет и веса пуха</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he does not even weighs as much as a down feather</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>wati medæg sire kʼela, oj wæle ba gorcʼe æma bunbuli baz</quote>
            <cit type="translation" xml:lang="ru">
                <q>в комнате — диван, на нем ковер и пуховая подушка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>there is a sofa in the room, a carpet and dow-pillow are on it</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>II 24</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>bonbuli xussænbæl... æxe ærgælʒidæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>он заваливался на пуховую постель</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he fell on the down feather bed</q>
            </cit>
            <bibl><ref target="#src_Bes._Ka_ke"/><biblScope>4</biblScope></bibl>
        </cit>
    </cit>
