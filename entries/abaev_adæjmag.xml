<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_adæjmag" xml:lang="os">
    <form type="lemma"><orth>adæjmag</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>человек</q>
            </cit><cit type="translation" xml:lang="en">
                <q>person</q>
            </cit></sense><sense><cit type="translation" xml:lang="en">
                <q>human being</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>nædær x˳ycaw dæn, nædær zæd, dæn zæxxon adæjmag</quote>
            <cit type="translation" xml:lang="ru">
                <q>я не бог и не ангел, я земной человек</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I am neither a god nor an angel, I am a human being</q>
            </cit>
            <bibl><ref target="#src_Munk."/></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>kæmi dær æj eci adæjmag, e min jeretæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>где бы ни был этот человек, найдите мне его</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>wherever this man is, find him for me</q>
            </cit>
            <bibl><ref target="#src_MSt."/><biblScope>109</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
