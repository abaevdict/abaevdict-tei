<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_cʼykk"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>cʼykk</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>cʼink</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>удар</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>knock</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>звук удара</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>knocking sound</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>nyrma nīk˳y jæxsy сʼykk, nīk˳y bæxy x˳yrrytt</quote>
            <cit type="translation" xml:lang="ru">
                <q>пока не слышно ни удара плетки, ни ржания лошади</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one can not still hear neither a lash nor horse neighing</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>cʼyḱḱytæ rajdydtoj qama ʽmæ xsargardæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>стали наносить удары кинжалом и шашкой</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[they] began to strike with a dagger and saber</q>
            </cit>
            <bibl>
                <ref target="#src_Æfx._Xæs."/>
                <biblScope>108</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>qwælti ǧazun me ʽfsæjnagæj, cʼīnk kænun</quote>
            <cit type="translation" xml:lang="ru">
                <q>шутя, играю железом, ударяю</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I am playing with iron, beat while joking</q>
            </cit>
            <bibl>
                <ref target="#src_AK"/>
                <biblScope>III 58</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>ǧædi særæj festægæj ærbacʼink kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>из надлесья он отмахал пешком</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he went by feet from the upper forest</q>
            </cit>
            <bibl>
                <ref target="#src_SD"/>
                <biblScope>263%l2%2</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>ew sæǧæ ma bajzadæj buduri, æma ʼj beræǧ racʼink lasta</quote>
            <cit type="translation" xml:lang="ru">
                <q>одна коза осталась в поле, и волк ее тяпнул</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one goat remained in the field, and the wolf bit it</q>
            </cit>
            <bibl>
                <ref target="#src_SD"/>
                <biblScope>267%l2%l</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>bacudæncæ Buculamæ æma omi araq cʼink kodtoncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>пошли к Буцула и там хлестали араку</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>went to Bucula and there gulped araq down</q>
            </cit>
            <bibl>
                <ref target="#src_SD"/>
                <biblScope>280%l2%l</biblScope>
            </bibl>
        </cit>
    </cit>
    <re>
        <form type="lemma">
            <orth>ærсʼykk æxson</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>кремень для высекания огня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>flint for making fire</q>
            </cit>
        </sense>
        <note>(<bibl><ref target="#src_ЮОПам."/><biblScope>I 173</biblScope></bibl>)</note>
    </re>
