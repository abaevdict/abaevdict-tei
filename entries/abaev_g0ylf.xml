<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_g0ylf"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>g˳ylf</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>gulf</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>сильное течение</q>
            </cit><cit type="translation" xml:lang="en">
                <q>heavy current</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>сильная волна</q>
            </cit><cit type="translation" xml:lang="en">
                <q>intense wave</q>
            </cit></sense></sense>
    <re><form type="lemma"><orth>g˳ylf kænyn</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>набегать волной</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>come (about oncoming wave)</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>толпиться</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>crowd</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>тесниться</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>be herded</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>хлынуть</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>gush out</q>
                </cit></sense></sense></re>
    <re><note type="comment" xml:lang="ru">в д. варианты</note><note type="comment" xml:lang="en">in
            Digor there are lemma variants</note><form type="lemma"><orth>gurf,
        kurf</orth></form></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>adæm ag˳ylf kodtoj dony bylmæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>народ хлынул на берег</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>people rushed ashore</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>don tyngær raivyld, g˳ylf-g˳ylf kæny</quote>
            <cit type="translation" xml:lang="ru">
                <q>река сильней разлилась, бурлит волнами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the river overflowed, its waves are seething</q>
            </cit>
            <bibl><ref target="#src_F."/><biblScope>1957 III 30</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>komi xurfi cʼæx cæqaltæ arvīstoncæ sæ ew gulf</quote>
            <cit type="translation" xml:lang="ru">
                <q>в ущелье синие волны хлынули разом</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>blue waves rushed into the gorge at once</q>
            </cit>
            <bibl><ref target="#src_Qazb."/><biblScope>54</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>æ fur qurmæj fækkurf kodta i dældær</quote>
            <cit type="translation" xml:lang="ru">
                <q>в смятении (народ) хлынул вниз</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(people) gushed down in dismay</q>
            </cit>
            <bibl><ref target="#src_Sam."/><biblScope>58</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>rafædes æncæ Digoræ dalæ kommæ ængurfæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>всей массой хлынули на тревогу вниз в ущелье дигорцы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the Digor people rushed down into the gorge at the alarm</q>
            </cit>
            <bibl><ref target="#src_Iræf"/><biblScope>55</biblScope></bibl>
        </cit>
    </cit>
