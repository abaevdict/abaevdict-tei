      <entry xml:id="entry_xærzæxsæv" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d3477e66" type="lemma" xml:lang="os-x-iron"><orth>xærzæxsæv</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>xwærzæxsæv</orth></form>
        <sense xml:id="sense_d3477e69">
          <abv:tr xml:lang="ru">
            <q>доброй ночи</q>
          </abv:tr>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">com wal xæʒarmæ, kæd næm īsty īs… — næ, næ,
              nīcy qæwy, <oRef/>!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">пойдем пока в дом, быть может, у нас что-нибудь
                найдется (чем угостить)... — нет, нет, ничего не надо, доброй ночи!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Брит."/>
              <biblScope>14</biblScope></bibl>
          </example>
        </exampleGrp>
