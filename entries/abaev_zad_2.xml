      <entry xml:id="entry_zad_2" n="2" xml:lang="os" abv:completeness="russian-questions">
        <?oxy_comment_start author="oleg" timestamp="20230706T164604+0300" comment="Russian checked"?>
        <form xml:id="form_d2191e66" type="lemma"><orth>zad</orth></form><?oxy_comment_end?>
        <sense xml:id="sense_d2191e69">
          <abv:tr xml:lang="ru">
            <q>рожденный</q>
          </abv:tr>
        </sense>
        <re>
          <note type="comment"
            ><?oxy_comment_start author="irina" timestamp="20230605T155656+0300" comment="у Абаева без точки с зпт" id="al1_y24_1yb"?><?oxy_comment_start author="oleg" timestamp="20230706T164458+0300" parentID="al1_y24_1yb" comment="с этим ничего не поделать" mid="2"?>в<?oxy_comment_end mid="2"?><?oxy_comment_end?>
            сложениях</note>
          <re>
            <form type="lemma"><orth>ʒæǧælzad</orth></form>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">незаконнорожденный</q>
              </tr>
            </sense>
            <note type="comment"> (см. <ref type="xr" target="#entry_ʒæǧæl"/>)</note>
          </re>
          <re>
            <form type="lemma"
                ><orth><?oxy_comment_start author="irina" timestamp="20230605T155719+0300" comment="у абаева через зпт ре сделаны...." flag="done"?>æ<?oxy_comment_end?>vzongzad
                (qūg)</orth></form>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">несвоевременно, запоздало отелившаяся
                  (корова)</q>
              </tr>
            </sense>
            <note type="comment"> (см. <ref type="xr" target="#entry_ævzong"/>)</note>
          </re>
        </re>
        <etym xml:lang="ru">Прош. причастие от <ref type="xr" target="#entry_zajyn_1"><w>zajyn</w>
            <gloss><q>рождать</q></gloss></ref>. Ср. <mentioned xml:lang="fa"><lang/>
            <w>zād</w>, <w>zāda</w>
            <gloss><q>рожденный</q></gloss>, <gloss><q>сын</q></gloss>,
              <gloss><q>детеныш</q></gloss></mentioned>, <mentioned xml:lang="fa"><w>ḥaramzāda</w>
            <gloss><q>незаконнорожденный</q></gloss></mentioned>, <mentioned xml:lang="sog"><lang/>
            <w type="rec">zāt</w>, <w type="rec">zātak</w> (<w>zʼʼt</w>, <w>zʼʼtk</w>)
                <gloss><q>детеныш</q></gloss></mentioned>, <mentioned><mentioned xml:lang="kho"><lang/>
              <w>ysāta</w>
              <note type="comment">(<w>-zātæ</w>)</note></mentioned>, <mentioned xml:lang="ae"><lang/>
              <w>zāta-</w></mentioned>, <mentioned xml:lang="inc-x-old"><lang/>
              <w>ǰāta-</w></mentioned>
            <gloss><q>рожденный</q></gloss></mentioned>. См. <ref type="xr" target="#entry_zajyn_1"
          />.</etym>
      </entry>
