      <entry xml:id="entry_qulqasxan" xml:lang="os" abv:completeness="incomplete">
        <form type="lemma"><orth>qulqasxan</orth></form>
        <sense><abv:tr xml:lang="ru">
            <q>беглый раб</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>runaway slave</q>
          </abv:tr></sense>
        <note type="bibl"> (<bibl><ref type="bibl" target="#src_Диг._сказ."/>
            <biblScope>53<hi rendition="#rend_subscript">14</hi></biblScope></bibl>)</note>
        <etym xml:lang="ru">Из <mentioned xml:lang="trk"><lang/>
            <w>qul-qač-ɣan-</w></mentioned>, ср. <mentioned xml:lang="trk"><lang/>
            <w>qul</w>
            <gloss><q>раб</q></gloss> и <w>qač</w>
            <gloss><q>убегать</q></gloss> (<m>-ɣan</m>
            <gloss>причастный суффикс</gloss>)</mentioned>.</etym>
        <etym xml:lang="en">From <mentioned xml:lang="trk"><lang/>
            <w>qul-qač-ɣan-</w></mentioned>, cf. <mentioned xml:lang="trk"><lang/>
            <w>qul</w>
            <gloss><q>slave</q></gloss> and <w>qač</w>
            <gloss><q>run away</q></gloss> (<m>-ɣan</m>
            <gloss>participle suffix</gloss>)</mentioned>.</etym>
      </entry>
