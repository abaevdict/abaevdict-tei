    <body>
      <entry xml:id="entry_nad_1" xml:lang="os" abv:completeness="russian-questions" n="1">
        <form type="lemma"><orth>nad</orth></form>
        <sense><sense><abv:tr xml:lang="ru">
              <q>побитый</q>
            </abv:tr></sense><sense><abv:tr xml:lang="ru">
              <q>побои</q>
            </abv:tr></sense></sense>
        <?oxy_comment_start author="Vados" timestamp="20220517T205742+0300" comment="У Абаева здесь запятая, оставить так?"?>
        <re><?oxy_comment_end?>
          <form type="lemma"><orth>fydnad</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">жестокие побои</q>
            </tr></sense>
        </re>
        <re>
          <form type="lemma"><orth>sawnad</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">черные побои</q>
            </tr></sense>
          <note type="comment"> (до синяков, но без крови)</note>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ælǧystæj wæd <oRef>nadæj</oRef> су пæ
              bavzærston!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">какой только я не перенес брани и побоев!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>56</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ralæwwydysty fæjjawyl, <oRef>fydnad</oRef> æj
              fækodtoj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">стали колотить пастуха, жестоко его избили</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ИАА"/>
              <biblScope>II 301</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Прош. причастие от <ref type="xr" target="#entry_næmyn"><w>næmyn</w>
            <gloss><q>бить</q></gloss></ref>.</etym>
      </entry>
