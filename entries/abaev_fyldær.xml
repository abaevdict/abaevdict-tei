<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_fyldær" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>fyldær</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>fuldær</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>больше (по количеству)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>more (in quantity)</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>большая часть</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the greater part</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>большей частью</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>mostly</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>xæxbæsty mīt fyldær wary</quote>
            <cit type="translation" xml:lang="ru">
                <q>в горах больше выпадает снега</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>more snow falls in the mountains</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>kūsǵytæ dær fæfyldær kodtoj bæxtæn sæ xærd</quote>
            <cit type="translation" xml:lang="ru">
                <q>работники увеличили корм лошадям</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>workers increased feed for horses</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>74</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>fuldær ke warzton...</quote>
            <cit type="translation" xml:lang="ru">
                <q>кого я больше любил</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>whom I loved more</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>26%l12%l</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>fyldær rynḱyn væjjyn</quote>
            <cit type="translation" xml:lang="ru">
                <q>большей частью я хвораю</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>mostly I am sick</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>fæsīvædæn sæ fyldær ax˳yrmæ acydysty</quote>
            <cit type="translation" xml:lang="ru">
                <q>большая часть молодежи поехала на учебу</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>most of the youth went to study</q>
            </cit>
        </cit>
    </cit>
