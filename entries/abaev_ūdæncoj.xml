      <entry xml:id="entry_ūdæncoj" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d5979e66" type="lemma" xml:lang="os-x-iron"><orth>ūdæncoj</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>odæncojnæ</orth></form>
        <sense xml:id="sense_d5979e69">
          <abv:tr xml:lang="ru">
            <q>(душевный) покой</q>
          </abv:tr>
          <abv:tr xml:lang="en">
            <q>peace (of mind)</q>
          </abv:tr>
        </sense>
        <abv:exampleGrp xml:id="exampleGrp_d5979e75">
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0"><oRef>ūdæncoj</oRef> ma fena maxæn næ cardy
              fænd ḱī xaly
              <?oxy_comment_start author="elohim" timestamp="20230403T131600+0300" comment="?"?>baræm''ændæj<?oxy_comment_end?></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">пусть не увидит покоя, кто разрушает умышленно
                замыслы нашей жизни</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">let him not see peace, who deliberately
                destroys the plans of our life</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>37</biblScope></bibl>
          </example>
        </abv:exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_ūd"><w>ūd</w></ref> и <ref type="xr"
            target="#entry_æncoj"><w>æncoj</w></ref>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_ūd"><w>ūd</w></ref> and <ref type="xr"
            target="#entry_æncoj"><w>æncoj</w></ref>.</etym>
      </entry>
