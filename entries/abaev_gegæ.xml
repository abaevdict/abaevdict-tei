            <entry xml:id="entry_gegæ" xml:lang="os-x-digor">
                <form type="lemma"><orth>gegæ</orth></form>
                <usg><lang/></usg>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>неприязнь</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>disfavour</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>вражда</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>enmity</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>Boriatæ ma Æxsærtæggatæ rajdajænæj dær sæ kæræʒemæ ba gegæ gængæ
                            cudæncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>Бораевы и Ахсартаговы с самого начала были между собой во вражде</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>The Boraevs and the Akhsartagovs were at enmity from the very
                                beginning</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                            42</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из <mentioned xml:lang="trk" dialect="ug qwm"><lang/>
                        <w>käk</w></mentioned>, <mentioned xml:lang="trk" dialect="ug qwm"
                            ><w>kek</w>
                        <gloss><q>вражда</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>ненависть</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>злоба</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>недоброжелательство</q></gloss></mentioned> (<bibl><ref
                            type="bibl" target="#ref_Радлов"/><biblScope>II
                    1058</biblScope></bibl>).</etym>
                <etym xml:lang="en">From <mentioned xml:lang="trk" dialect="ug qwm"><lang/>
                        <w>käk</w></mentioned>, <mentioned xml:lang="trk" dialect="ug qwm"
                            ><w>kek</w>
                        <gloss><q>enmity</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>hatred</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>spite</q></gloss></mentioned>, <mentioned xml:lang="trk"
                        dialect="ug qwm"><w/>
                        <gloss><q>ill will</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Радлов"/><biblScope>II 1058</biblScope></bibl>).</etym>
