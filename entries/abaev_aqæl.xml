    <text>
        <body>
            <entry xml:id="entry_aqæl" xml:lang="os">
                <form type="lemma"><orth>aqæl</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>рассудительность</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>discretion</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>(хороший) нрав</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>good temper</q>
                        </abv:tr></sense></sense>
                <re>
                    <form type="lemma"><orth>ænaqæl</orth></form>
                    <sense><sense><abv:tr xml:lang="ru">
                                <q>безрассудный</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>reckless</q>
                            </abv:tr></sense><sense><abv:tr xml:lang="ru">
                                <q>дурного нрава</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>bad tempered</q>
                            </abv:tr></sense></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>mæ bæx ewgaron xwærzaqæl næj, æma ku ǧazon, wæd alke æxe baǧæwaj
                            kænæd</quote>
                        <abv:tr xml:lang="ru">
                            <q>мой конь не хорошего нрава, и когда я буду джигитовать, пусть каждый
                                побережется</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>my horse is not of good temper, so take care, everyone, when I engage
                                in trick riding</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Диг._сказ."
                            /><biblScope>4</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ʽaql</w>
                        <gloss><q>ум</q></gloss></mentioned>, <mentioned xml:lang="ar"
                        extralang="fa"><w/>
                        <gloss><q>рассудок</q></gloss></mentioned>, <mentioned xml:lang="ar"
                        extralang="fa"><w/>
                        <gloss><q>благоразумие</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ʽaql</w>
                        <gloss><q>intelligence</q></gloss></mentioned>, <mentioned xml:lang="ar"
                        extralang="fa"><w/>
                        <gloss><q>mind</q></gloss></mentioned>, <mentioned xml:lang="ar"
                        extralang="fa"><w/>
                        <gloss><q>prudence</q></gloss></mentioned>.</etym>
