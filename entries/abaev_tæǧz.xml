      <entry xml:id="entry_tæǧz" xml:lang="os" abv:completeness="incomplete">
        <?oxy_comment_start author="oleg" timestamp="20230207T130941+0300" comment="Russian checked"?>
        <form xml:id="form_d3479e66" type="lemma" xml:lang="os-x-iron"><orth>tæǧz</orth></form><?oxy_comment_end?>
        <form xml:lang="os-x-digor" type="dialectal"><orth>tæǧzæ</orth></form>
        <sense>
          <sense xml:id="sense_d3479e69">
            <abv:tr xml:lang="ru">
              <q>пористая кость</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>porous bone</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>болезненная часть копыта под твердым роговым слоем</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>sore part of the hoof under the hard stratum corneum</q>
            </abv:tr>
          </sense>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <note xmlns="http://www.tei-c.org/ns/1.0" xml:lang="ru" type="comment">(при ковке лошади:) </note>
          <note xmlns="http://www.tei-c.org/ns/1.0" xml:lang="en" type="comment">(while forging a
            horse:) </note>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">bæx ysqīwy, ævæccægæn zægæl
                <oRef>tæǧzy</oRef> bacydī</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">лошадь дергается, должно быть гвоздь вошел в
                  <oRef>tæǧz</oRef></q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">the horse is twitching, the nail must have
                entered the <oRef>tæǧz</oRef></q>
            </tr>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Не ясно.</etym>
        <etym xml:lang="en">It is unclear.</etym>
      </entry>
