<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_būqq"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>būqq</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>boqq</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>отрыжка</q>
        </cit><cit type="translation" xml:lang="en">
            <q>belching</q>
        </cit></sense>
    <re><form type="lemma"><orth>būqq kænyn</orth><form type="variant"><orth>būqqytæ
                kænyn</orth></form></form><sense><cit type="translation" xml:lang="ru">
                <q>рыгать</q>
            </cit><cit type="translation" xml:lang="en">
                <q>belch</q>
            </cit></sense><note type="comment" xml:lang="ru">д. <ref type="xr" target="#entry_rewun"
            /> id.</note><note type="comment" xml:lang="en">Digor <ref type="xr"
                target="#entry_rewun"/> id</note></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæsgon ældary būqqytæ kæny</quote>
            <cit type="translation" xml:lang="ru">
                <q>он рыгает, как кабардинский князь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he belches like a Kabardian prince</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>æfsesaxsæn boqqitæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>отрыжка сытого желудка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>belching of a full-fed stomach</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>286%l3%l</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>cardæj būqqytæ kodtoj</quote>
            <cit type="translation" xml:lang="ru">
                <q>от (избытка) жизни у них была отрыжка (они пресыщены)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>they had an belching because of (the excess) of life (they are oversaturated)</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>102</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæsgon ældary būqqytæ kæny</quote>
            <cit type="translation" xml:lang="ru">
