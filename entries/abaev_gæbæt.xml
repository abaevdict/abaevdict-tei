<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_gæbæt"
    xml:lang="os-x-digor">
    <form type="lemma"><orth>gæbæt</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>бурдюк</q>
        </cit><cit type="translation" xml:lang="en">
            <q>wineskin</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>næ farsbæl mudi gæbæt</quote>
            <cit type="translation" xml:lang="ru">
                <q>на нашей стене бурдючок меду</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>there is a wineskin of honey on our wall</q>
            </cit>
            <note type="comment" xml:lang=""> ()</note>
            <note type="comment" xml:lang=""> ()</note>
            <bibl><ref target="#src_ОЭ"/><biblScope>III 182</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
