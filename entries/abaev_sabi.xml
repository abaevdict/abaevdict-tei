      <entry xml:id="entry_sabi" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d4192e66" type="lemma"><orth>sabi</orth></form>
        <sense xml:id="sense_d4192e69">
          <abv:tr xml:lang="ru">
            <q>дитя</q>
            <?oxy_comment_start author="админ" timestamp="20220925T172710+0300" comment="рендерится только 1 кавычка"?>
            <q rendition="#rend_singlequotes">ребенок<?oxy_comment_end?></q>
          </abv:tr>
          <note type="comment">; син. <ref type="xr" target="#entry_sywællon"/></note>
          <?oxy_comment_start author="админ" timestamp="20220925T164407+0300" comment="2 точки"?>
        </sense><?oxy_comment_end?>
        <abv:exampleGrp xml:id="exampleGrp_d4192e75">
          <abv:example xml:id="example_d4192e77">
            <quote>mæ sabitæ de wazæg, Qasaj </quote>
            <abv:tr xml:lang="ru">
              <q xmlns="">(да будут) мои дети под твоим покровительством
                (<?oxy_comment_start author="админ" timestamp="20220923T113032+0300" comment="нужны кавычки"?>твоими
                гостями<?oxy_comment_end?>), Касай</q>
            </abv:tr>
            <note type="comment"><bibl>(из песни о Тотрадзе Берозове)</bibl></note>
          </abv:example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">xūr! ... batav sabity, batav ræzgæty</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">солнце! согрей малых деток, согрей растущих</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Niger"/>
              <biblScope>146</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">sabi sæ læǧy ranmæ næ qomyl kodta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">дети у них не вырастали в мужчин</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Čerm."/>
              <biblScope>123</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">qæwty ma bazzadysty ærmæst īwyl zærdæjy
              rūxsæn k˳y xaston</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">ведь я вскармливала ваших детей, ваших слабых
                на радость сердца</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Темыр."/>
              <biblScope>19</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">wydtæn wæd sabi, ḱysyl</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">я был тогда малым ребенком</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Xar._Zærdæ"/>
              <biblScope>67</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">sabitæ stonǵyjæ nīwync, kæwync</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">дети хнычут, плачут от голода</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Мысост"/>
              <biblScope>32</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">næn adtæj giccil sabi</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">у них был маленький ребенок</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Munk."/>
              <biblScope>200</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">nurmæ sabijtæ kænioncæ bašiltæ, nur
              rejdædtoncæ næ šiltæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">до сих пор дети готовили (себе) новогодние
                печенья, теперь начали (это делать) наши женщины</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Gurdž._Æduli"/>
              <biblScope>81</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">næ sabi ʒi babæddæn (avdæni)</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">привяжем там (в колыбели) нашего ребенка</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MSt."/>
              <biblScope>1ᵢᵢ</biblScope></bibl>
          </example>
        </abv:exampleGrp>
        <etym xml:lang="ru">Из <mentioned xml:lang="ar"><lang/>
            <w>sabi</w>
            <q>ребенок</q></mentioned>. Сюда же <mentioned xml:lang="trk"><lang/> (анат.)
              <w>sabi</w>
            <note type="comment">(<bibl><author>Магазаник.</author><title>Тур.-русск. сл.
                  </title><pubPlace>М.</pubPlace>, <time>1931</time>, <biblScope>стр.
                  876</biblScope></bibl>)</note></mentioned>, <mentioned xml:lang="kbd"><lang/>
            <w>sabij</w>
            <note type="comment">(<bibl><title>Русско-каб. сл.</title><pubPlace> М.</pubPlace>;
                  <time>1995</time>, <biblScope>стр. 173</biblScope><note type="comment">,
                    <q>дитя</q></note></bibl>)</note></mentioned>, <mentioned xml:lang="abq"><lang/>
            <w>sabi</w>
            <note type="comment">(<bibl><title>Русско-абаз. сл.</title>
                <pubPlace>М.</pubPlace>, <time>1956</time>, <biblScope>стр.
              114</biblScope></bibl>)</note></mentioned>, <mentioned xml:lang="ab"><lang/> a-sabi
              <note type="comment">(<bibl><author>Бгажба</author>. <title>Русско-абх. сл.</title>
                <pubPlace>Сухуми</pubPlace>, <time>1964</time>, <biblScope>стр.
                177</biblScope></bibl>)</note></mentioned>.<note type="bibl"><bibl><author> Вс.
                Миллер. </author><title>Gr. </title><biblScope>9.</biblScope></bibl><note
              type="bibl"> — <bibl><ref type="bibl" target="#ref_Hübschmann._Oss.">Hübschmann.
                  Oss.</ref><biblScope>130. </biblScope></bibl></note></note>— <note type="comment"
                ><bibl><ref type="bibl" target="#src_ОЯФ_I"/><biblScope>
            93</biblScope></bibl></note>.</etym>
      </entry>
