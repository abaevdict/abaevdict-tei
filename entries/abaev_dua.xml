<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_dua"
    xml:lang="os">
    <form type="lemma">
        <orth>dua</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>молитва</q>
        </cit>
        <note type="comment" xml:lang="ru"> (у мусульман)</note>
        <note type="comment" xml:lang="en"> (Islamic)</note>
        <cit type="translation" xml:lang="en">
            <q>prayer</q>
        </cit>
        <note type="comment" xml:lang="ru"> (у мусульман)</note>
        <note type="comment" xml:lang="en"> (Islamic)</note>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>mæg˳yry dua qabyl ū</quote>
            <cit type="translation" xml:lang="ru">
                <q>молитва бедняка угодна (богу)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the poor man's prayer is pleasing (to God)</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>Mæxæmæt-molloj duatæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>молитвы муллы Махамата</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>prayers of Mahamat the mullah</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>58</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>mæg˳yry dua qabyl ū</quote>
            <cit type="translation" xml:lang="ru">
                <q>молитва бедняка угодна (богу)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the poor man's prayer is pleasing (to God)</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>Mæxæmæt-molloj duatæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>молитвы муллы Махамата</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>prayers of Mahamat the mullah</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>58</biblScope>
            </bibl>
        </cit>
    </cit>
    <etym xml:lang="ru">Из <mentioned xml:lang="ar"
                ><lang/><w>du%b‘%ba</w><gloss><q>молитва</q></gloss></mentioned>; сюда же <mentioned
            xml:lang="kbd"><lang/><w>du‘a</w><gloss><q>заповедь</q></gloss></mentioned>, <mentioned
            xml:lang="x-vaynakh"><lang/><w>duha</w></mentioned>, <mentioned xml:lang="av"
                ><lang/><w>%bdu‘a%b</w><gloss><q>молитва</q></gloss></mentioned>.</etym>
    <etym xml:lang="en">From <mentioned xml:lang="ar"
            ><lang/><w>duʽa</w><gloss><q>prayer</q></gloss></mentioned>; also here <mentioned
            xml:lang="kbd"><lang/><w>duʽa</w><gloss><q>commandment</q></gloss></mentioned>,
            <mentioned xml:lang="x-vaynakh"><lang/><w>duha</w></mentioned>, <mentioned xml:lang="av"
                ><lang/><w>duʽa</w><gloss><q>prayer</q></gloss></mentioned>.</etym>
</entry>
