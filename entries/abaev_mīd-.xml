            <gloss><q>between</q></gloss></mentioned>, <mentioned xml:lang="cu"><w>mežda</w>
            <gloss><q>boundary</q></gloss></mentioned> , <mentioned xml:lang="ru"><lang/>
            <w>meždu</w>
            <gloss><q>between</q></gloss></mentioned>, <mentioned xml:lang="ru"><w>meža</w>
