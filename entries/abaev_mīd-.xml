            xml:lang="sgh"><lang/>
            <w>miδēn</w>
            <gloss><q>among</q></gloss>, <gloss><q>between</q></gloss></mentioned>, <mentioned
            xml:lang="sgh"><w>miδēnā</w>
            <gloss><q>the middle</q></gloss></mentioned>, <mentioned xml:lang="sgh"><w>miδēnǰ</w>
            <gloss><q>middle</q></gloss></mentioned> (<bibl><ref type="bibl"
