<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_bylgæron" xml:lang="os-x-iron">
    <form type="lemma">
        <orth>bylgæron</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>край (обрыва, крыши и т. п.)</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>edge (of the steep, roof etc.)</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>tækkæ bylgæron dūryl aværdta jæ kæxtæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>у самого края (скалы) он стал на камень</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he stood on the stone near the very edge (of the cliff)</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>79</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>tækkæ bylgæron dūryl aværdta jæ kæxtæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>у самого края (скалы) он стал на камень</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he stood on the stone near the very edge (of the cliff)</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>79</biblScope>
            </bibl>
        </cit>
    </cit>
    <etym xml:lang="ru">Из <ref type="xr" target="#entry_byl"/> ‘край’ и <ref type="xr"
            target="#entry_kæron"/> ‘конец’.</etym>
    <etym xml:lang="en">From <ref type="xr" target="#entry_byl"/> ‘edge’ and <ref type="xr"
            target="#entry_kæron"/> ‘end’.</etym>
</entry>
