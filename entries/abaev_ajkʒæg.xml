            <entry xml:id="entry_ajkʒæg" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>ajkʒæg</orth><form type="variant"
                            ><orth>æjkʒæg</orth></form><form type="variant"
                        ><orth>ikʒæg</orth></form></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>æccæg</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>яичник</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>ovary</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Из <ref type="xr" target="#entry_ajk"/> ‘яйцо’ и <ref type="xr"
                        target="#entry_ʒag"/> ‘полный’ с ослаблением гласных.</etym>
                <etym xml:lang="en">From <ref type="xr" target="#entry_ajk"/> ‘egg’ and <ref
                        type="xr" target="#entry_ʒag"/> ‘full’ with the weakening of the
                    vowels.</etym>
