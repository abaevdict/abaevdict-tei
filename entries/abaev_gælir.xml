            <entry xml:id="entry_gælir" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>gælir</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>gæleræ</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>покоробленный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>warped</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>неровный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>uneven</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>с зазубринами</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>serrated</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Из <mentioned xml:lang="ka"><lang/>
                        <w>xrili</w>
                        <gloss><q>изогнутый</q></gloss></mentioned> (от <mentioned xml:lang="ka"><lang/>
                        <w>xr-</w>
                        <gloss><q>сгибать</q></gloss></mentioned>); для перебоя %iх%i || %ig%i ср.
                    %ixari%i || <ref type="xr" target="#entry_gal"/> ‘бык’, %ičxinǯ-%i ||
                    %icæginʒ-%i (см. <ref type="xr" target="#entry_cæǵynʒ"/>) ‘столб’.%n<bibl><ref
                            type="bibl" target="#ref_ОЯФ_I"
                    /><biblScope>527</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ka"><lang/>
                        <w>xrili</w>
                        <gloss><q>curved</q></gloss></mentioned> (from <mentioned xml:lang="ka"><lang/>
                        <w>xr-</w>
                        <gloss><q>bend</q></gloss></mentioned>); for the alternation %iх%i || %ig%i
                    cf. %ixari%i || <ref type="xr" target="#entry_gal"/> ‘bull’, %ičxinǯ-%i ||
                    %icæginʒ-%i (see <ref type="xr" target="#entry_cæǵynʒ"/>) ‘post’.%n<bibl><ref
                            type="bibl" target="#ref_ОЯФ_I"
                    /><biblScope>527</biblScope></bibl>.</etym>
