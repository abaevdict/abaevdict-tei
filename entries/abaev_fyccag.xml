<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
        xml:id="entry_fyccag" xml:lang="os">
        <form xml:lang="os-x-iron" type="lemma"><orth>fyccag</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>ficcag</orth></form>
        <sense><sense><cit type="translation" xml:lang="ru">
                                <q>первый</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>first</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>начальный</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>initial</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>прежний</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>previous</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>первоначально</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>originally</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>сперва</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>firstly</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>ранее</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>before</q>
                        </cit></sense><sense><cit type="translation" xml:lang="ru">
                                <q>впереди</q>
                        </cit><cit type="translation" xml:lang="en">
                                <q>ahead</q>
                        </cit></sense></sense>
        <re><form type="lemma"><orth>aj fyccag</orth></form><sense><sense><cit type="translation"
                                        xml:lang="ru">
                                        <q>перед этим</q>
                                </cit><cit type="translation" xml:lang="en">
                                        <q>before this</q>
                                </cit></sense><sense><cit type="translation" xml:lang="ru">
                                        <q>давеча</q>
                                </cit><cit type="translation" xml:lang="en">
                                        <q>recently</q>
                                </cit></sense><sense><cit type="translation" xml:lang="ru">
                                        <q>на днях</q>
                                </cit><cit type="translation" xml:lang="en">
                                        <q>the other day</q>
                                </cit></sense></sense></re>
        <cit type="exampleGroup">
                <cit type="example">
                        <quote>wæ fyccag saxatæj wæ fæstag ræstægmæ æddagon card kodtat wæ binonty
                                astæw</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>от первого вашего часа до последнего времени вы жили как чужие
                                        среди своей семьи</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>from your first hour until the last time, you lived like
                                        strangers among your family</q>
                        </cit>
                        <bibl><ref target="#src_Æfx._Xæs."/><biblScope>105</biblScope></bibl>
                </cit>
                <cit type="example" xml:lang="os-x-digor">
                        <usg type="dialect"><lang/></usg>
                        <quote>Woræzmæg xestær adtæj, ma ficcagi dær zæǧuni baræ oj adtæj</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>Уоразмаг был старший, и право говорить первым принадлежало
                                        ему</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>Worazmag was the eldest, and the right to speak first belonged to
                                        him</q>
                        </cit>
                        <bibl><ref target="#src_СОПам."/><biblScope>II 38</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>Tembot ærbaficcag æj</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>Тембот пришел первым</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>Tembot came first</q>
                        </cit>
                        <bibl><ref target="#src_AK"/><biblScope>III 47</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>fyccag ræstceǧy</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>в прежнее время</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>in the old days</q>
                        </cit>
                </cit>
                <cit type="example">
                        <quote>k˳yd wydī jæ fyccag? k˳yd razyld mæ sær?</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>как было ее (любви) начало? как вскружилась моя голова?</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>what was its beginnig (the beginning of love)? how was my head
                                        turned?</q>
                        </cit>
                        <bibl><ref target="#src_Коста"/><biblScope>58</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>fyccag sæmbældī ūrs wæjygyl</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>сперва он встретил белого великана</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>first he met a white giant</q>
                        </cit>
                </cit>
                <cit type="example" xml:lang="os-x-digor">
                        <usg type="dialect"><lang/></usg>
                        <quote>ficcag sæ læqwæni kud adtæncæ, otæ festadæncæ</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>они стали такими, как были прежде, в юности</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>they became what they were before, in their youth</q>
                        </cit>
                        <bibl><ref target="#src_MSt."/><biblScope>15%l3–4%l</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>æz k˳y rbacæwyn, wæd ændær īsḱī nyxxīzy mæ fyccag</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>когда же я прихожу, другой уже сходит прежде меня</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>but while I am coming, another steppeth down before me</q>
                        </cit>
                        <bibl><ref target="#src_Иоанн"/><biblScope>%i5%i 7</biblScope></bibl>
                </cit>
                <cit type="example" xml:lang="os-x-digor">
                        <usg type="dialect"><lang/></usg>
                        <quote>jæ ficcag sæ tærun bajdædta</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>он стал их гнать перед собой</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>he began to drive them in front of him</q>
                        </cit>
                        <bibl><ref target="#src_MSt."/><biblScope>34%l9%l</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>dæ fyccag mæ х˳уcaw fækænæd!</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>пусть бог (пошлет мне смерть) раньше тебя!</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>may God (send me death) before you!</q>
                        </cit>
                </cit>
        </cit>
