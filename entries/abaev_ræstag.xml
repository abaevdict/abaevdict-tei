      <entry xml:id="entry_ræstag" xml:lang="os" abv:completeness="complete">
        <form xml:id="form_d1204e68" type="lemma"><orth>ræstag</orth></form>
        <sense xml:id="sense_d1204e71"><sense xml:id="sense_d1204e72"><abv:tr xml:lang="ru">
              <q>правый</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>right</q>
            </abv:tr></sense><sense xml:id="sense_d1204e81"><abv:tr xml:lang="ru">
              <q>невинный</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>innocent</q>
            </abv:tr></sense></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict" xml:id="exampleGrp_d1204e91">
          <example xml:id="example_d1204e93">
            <quote xmlns="http://www.tei-c.org/ns/1.0">ḱī kæna xax˳yr <oRef>ræstag</oRef> lægyl, ḱī
              kala īskæwyl cʼyf…</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">кто будет клеветать на невинного человека, кто
                будет обливать другого грязью…</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">who will slander an innocent person, who will
                pour mud on another one…</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <biblScope>1957 III 54</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_rast"/>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_rast"/>.</etym>
      </entry>
