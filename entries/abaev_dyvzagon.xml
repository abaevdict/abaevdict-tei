            <entry xml:id="entry_dyvzagon" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>dyvzagon</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>duvzagon</orth></form>
                <sense xml:id="sense_d4856e72"><sense xml:id="sense_d4856e73"><abv:tr xml:lang="ru">
                            <q>двуязычный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>bilibgual</q>
                        </abv:tr></sense><sense xml:id="sense_d4856e82"><abv:tr xml:lang="ru">
                            <q>ненадежный, двуличный, фальшивый человек</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>unreliable, two-faced, fake person</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Ср. <mentioned xml:id="mentioned_d4856e94" xml:lang="sog"><lang/>
                        <w>δyβzβ'k</w></mentioned> id. См. <ref type="xr" target="#entry_yvzag"/>
                    ‘язык’.</etym>
                <etym xml:lang="en">Cf. <mentioned xml:id="mentioned_d4856e104" xml:lang="sog"><lang/>
                        <w>δyβzβ'k</w></mentioned> id. See <ref type="xr" target="#entry_yvzag"/>
                    ‘language’.</etym>
