<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_æmyr"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>æmyr</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>æmir</orth>
        <form type="variant">
            <orth>imir</orth>
        </form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>плотный</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>tight, solid</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>плотно закрытый</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>tightly closed</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>fyccaǵydær ūj ḱl zaǧta, ūman jæ ʒyx bamyr wa!</quote>
            <cit type="translation" xml:lang="ru">
                <q>кто первый это сказал, пусть у того замкнутся уста!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>whoever said this first, let his mouth shut!</q>
            </cit>
            <bibl>
                <ref target="#src_Брит."/>
                <biblScope>48</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>de xsæv dyl bamyr is</quote>
            <cit type="translation" xml:lang="ru">
                <q>твоя ночь на тебе сомкнулась</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your night closed on you</q>
            </cit>
            <bibl>
                <ref target="#src_Æfx._Xæs."/>
                <biblScope>99</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>festadæncæ æmir cavddortæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>обратились в немых истуканов</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>turned into silent idols</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>39</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>i æstæg ænxæst næma isimir æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>кость (барашка) была еще недостаточно плотной</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the (lamb) bone was still not dense</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>II 98</biblScope>
            </bibl>
        </cit>
    </cit>
