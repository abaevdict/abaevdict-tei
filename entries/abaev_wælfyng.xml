      <entry xml:id="entry_wælfyng" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d1419e66" type="lemma" xml:lang="os-x-iron"><orth>wælfyng</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>wælfingæ</orth></form>
        <sense xml:id="sense_d1419e69">
          <abv:tr xml:lang="ru">
            <q>за столом</q>
          </abv:tr>
        </sense>
        <abv:exampleGrp xml:id="exampleGrp_d1419e75">
          <abv:example xml:id="example_d1419e77" xml:lang="os-x-digor">
            <usg><lang/></usg>
            <quote>i Ket studta wælfingæ bereket </quote>
            <abv:tr xml:lang="ru">
              <q>«(жители аула) Кет славили за столом изобилие</q>
            </abv:tr>
            <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>63</biblScope></bibl>
          </abv:example>
        </abv:exampleGrp>
        <etym xml:lang="ru">Сложение из <ref type="xr" target="#entry_wæl-">wæl- </ref>и <ref
            type="xr" target="#entry_fyng">fyng | fingæ</ref>
          <gloss><q>стол</q></gloss>. По образованию и значению ср. <ref type="xr"
            target="#entry_wælaryng">wælaryng</ref>
          <gloss><q>за мучным корытом</q></gloss>. </etym>
      </entry>
