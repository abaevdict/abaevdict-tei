      <entry xml:id="entry_pæux" xml:lang="os" abv:completeness="incomplete">
        <form type="lemma"><orth>pæux</orth><form type="variant"><orth>pox</orth></form></form>
        <sense><note xml:lang="ru" type="comment">название масти у лошадей </note><note
            xml:lang="en" type="comment">the name of the horse coat color </note>, <abv:tr
            xml:lang="en">
            <q>with a white stripe on his forehead</q>
            <note xml:lang="ru" type="comment"> (?)</note>
            <note xml:lang="en" type="comment"> (?)</note>
          </abv:tr><abv:tr xml:lang="ru">
            <q>темно-гнедой</q>
            <note xml:lang="ru" type="comment"> (?)</note>
            <note xml:lang="en" type="comment"> (?)</note>
          </abv:tr>, <abv:tr xml:lang="en">
            <q>dark bay</q>
            <note xml:lang="ru" type="comment"> (?)</note>
            <note xml:lang="en" type="comment"> (?)</note>
          </abv:tr><note xml:lang="ru" type="comment">; по объяснению одного информатора: <example
              xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="os">
              <quote xmlns="http://www.tei-c.org/ns/1.0" xml:lang="os">nædær surx, nædær bor, nædær
                sawgon</quote>
              <tr xml:lang="ru">
                <q rendition="#rend_doublequotes" xmlns="http://www.tei-c.org/ns/1.0">ни гнедая, ни
                  буланая, ни вороная</q>
              </tr>
            </example></note><note xml:lang="en" type="comment">; according to the explanation of
            one informant: <example xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="os">
              <quote xmlns="http://www.tei-c.org/ns/1.0" xml:lang="os">nædær surx, nædær bor, nædær
                sawgon</quote>
              <tr xml:lang="ru">
                <q rendition="#rend_doublequotes" xmlns="http://www.tei-c.org/ns/1.0">ни гнедая, ни
                  буланая, ни вороная</q>
              </tr>
              <tr xml:lang="en">
                <q rendition="#rend_doublequotes" xmlns="http://www.tei-c.org/ns/1.0">neither bay,
                  nor bun, nor black</q>
              </tr>
            </example></note></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">afonmæ рæих dūǧon me skʼæty xūssajrag xos
              mers kæny</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">должно быть, теперь мой скакун <hi
                  rendition="#rend_smallcaps">pæux</hi> в моей конюшне хрупает сено с южного склона
                (т. е. лучшее сено)</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">my <hi rendition="#rend_smallcaps">pæux</hi>
                race horse must be in my stable now, chomping hay from the south slope (i. e. the
                best hay)</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Брит."/>
              <biblScope>38</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">je ʽnsuvær ew рох bæxbæl otæ ærbaqærttæj
              duwwæ egarej хæccæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">тем временем его брат подоспел на лошади
                (масти) <hi rendition="#rend_smallcaps">pox</hi> с двумя гончими псами</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">meanwhile, his brother arrived on a <hi
                  rendition="#rend_smallcaps">pox</hi> horse with two greyhouds</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_AK"/>
              <biblScope>III 33</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Ср. <mentioned xml:lang="kbd"><lang/>
            <w>pexw</w>
            <gloss><q>вороной</q></gloss>
            <note type="bibl">(<bibl><ref type="bibl" target="#ref_СМК"/>
                <biblScope>XII <hi rendition="#rend_italic">2</hi> 21</biblScope></bibl>)</note>,
                <gloss><q>белоносый</q></gloss>
            <note xml:lang="ru" type="comment">(о животных)</note><note xml:lang="en" type="comment"
              >(о животных)</note><note xml:lang="en" type="comment">(о животных)</note><note
              xml:lang="en" type="comment">(о животных)</note>
            <note type="bibl">(<bibl><title>Каб.-русск. сл.</title>
                <biblScope>М., 1957, стр. 291</biblScope></bibl>)</note></mentioned>, <mentioned
            xml:lang="abq"><lang/>
            <w>pok˳</w>
            <gloss><q>масть лошади</q></gloss></mentioned>.</etym>
        <etym xml:lang="en">Cf. <mentioned xml:lang="kbd"><lang/>
            <w>pexw</w>
            <gloss><q>black</q></gloss>
            <note type="bibl">(<bibl><ref type="bibl" target="#ref_СМК"/>
                <biblScope>XII <hi rendition="#rend_italic">2</hi> 21</biblScope></bibl>)</note>,
                <gloss><q>white-nosed</q></gloss>
            <note xml:lang="ru" type="comment">(о животных)</note><note xml:lang="en" type="comment"
              >(about animals)</note>
            <note type="bibl">(<bibl><title>Kabardian-Russian dictionary</title>
                <biblScope>M. 1957, p. 291</biblScope></bibl>)</note></mentioned>, <mentioned
            xml:lang="abq"><lang/>
            <w>pok˳</w>
            <gloss><q>horse coat color</q></gloss></mentioned>.</etym>
      </entry>
