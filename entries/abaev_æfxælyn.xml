    <text>
        <body>
            <entry xml:id="entry_æfxælyn" xml:lang="os">
                <form xml:id="form_d587e66" xml:lang="os-x-iron" type="lemma"><orth>æfxælyn</orth><form xml:id="form_d587e68" type="participle"><orth>æfxæld</orth></form></form>
                <form xml:id="form_d587e71" xml:lang="os-x-digor" type="dialectal"><orth>æfxælun</orth><form xml:id="form_d587e73" type="participle"><orth>æfxæld</orth></form></form>
                <sense xml:id="sense_d587e76"><abv:tr xml:lang="ru">
                        <q>повреждаться (о коже, организме)</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>to be damaged (of skin, organism)</q>
                    </abv:tr></sense>
                <abv:exampleGrp xml:id="exampleGrp_d587e86">
                    <abv:example xml:id="example_d587e88">
                        <quote>mæ bæxæn jæ raǧ afxældī</quote>
                        <abv:tr xml:lang="ru">
                            <q>у моей лошади натерло спину</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>my horse has rubbed his back</q>
                        </abv:tr>
                    </abv:example>
                    <abv:example xml:id="example_d587e103" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>kʼæxtæ c’ifæj æfxwæluncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>ноги от грязи разъедаются</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>feet are corroded by dirt</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Sozur"/><biblScope>68</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d587e125" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>ræstæg nifxældæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>погода испортилась</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>the weather turned bad</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_AK"/><biblScope>I 8</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <re xml:id="re_d587e148">
                    <form xml:id="form_d587e150" xml:lang="os-x-iron" type="lemma"><orth>æfxælc</orth></form>
                    <form xml:id="form_d587e153" xml:lang="os-x-digor" type="dialectal"><orth>æfxælcæ</orth></form>
                    <sense xml:id="sense_d587e156"><sense xml:id="sense_d587e157"><abv:tr xml:lang="ru">
                                <q>выкидыш</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>misbirth</q>
                            </abv:tr></sense><sense xml:id="sense_d587e166"><note type="comment" xml:lang="ru">в д.
                                также</note><note type="comment" xml:lang="en">in Digor
                                also</note><abv:tr xml:lang="ru">
                                <q>смятение</q>
                            </abv:tr><note type="comment" xml:lang="ru">в д. также</note><note type="comment" xml:lang="en">in Digor also</note><abv:tr xml:lang="en">
                                <q>confusion</q>
                            </abv:tr></sense><sense xml:id="sense_d587e183"><abv:tr xml:lang="ru">
                                <q>путаница</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>mess</q>
                            </abv:tr></sense></sense>
                </re>
                <etym xml:lang="ru">Medium к <ref type="xr" target="#entry_æfxalyn"/> | <hi rendition="#rend_italic">æyxalun</hi>
                    q. v. См. также <ref type="xr" target="#entry_xælyn_1"/> | <hi rendition="#rend_italic">ixælun</hi>.</etym>
                <etym xml:lang="en">Medium to <ref type="xr" target="#entry_æfxalyn"/> | <hi rendition="#rend_italic">æyxalun</hi>
                    q. v. See also <ref type="xr" target="#entry_xælyn_1"/> | <hi rendition="#rend_italic">ixælun</hi>.</etym>
            </entry>
