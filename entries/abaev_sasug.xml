      <entry xml:id="entry_sasug" xml:lang="os-x-digor" abv:completeness="incomplete">
        <form xml:id="form_d5336e66" type="lemma"><orth>sasug</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d5336e69">
          <abv:tr xml:lang="ru">
            <q>головной платок</q>
          </abv:tr>
          <abv:tr xml:lang="en">
            <q>headscarf</q>
          </abv:tr>
        </sense>
        <note xml:lang="ru" type="comment"> (<bibl><ref type="bibl" target="#ref_ОЯФ_I"/>
            <biblScope>462₇₁₃</biblScope>); мало употребительно</bibl></note>
        <note xml:lang="en" type="comment"> (<bibl><ref type="bibl" target="#ref_ОЯФ_I"/>
            <biblScope>462₇₁₃</biblScope>); the word is rarely used</bibl></note>
        <etym xml:lang="ru">Ср. <mentioned xml:lang="ar" extralang="fa"><lang/>
            <w>šāš</w>
            <gloss><q>тюрбан</q></gloss>, <note type="bibl">(<bibl><ref type="bibl"
                  target="#ref_Ягелло"/></bibl>)</note>
            <gloss><q>тонкая ткань</q></gloss>, <gloss><q>муслин</q></gloss>
            <note xml:lang="ru" type="comment">(<bibl><ref type="bibl" target="#ref_Гаффаров"
                />)</bibl></note><note xml:lang="en" type="comment">(<bibl><ref type="bibl"
                  target="#ref_Гаффаров"/>)</bibl></note></mentioned>, <mentioned xml:lang="ku"><lang/>
            <w>šašək</w>
            <gloss><q>тюрбан</q></gloss></mentioned> (<bibl><ref type="bibl" target="#ref_Bedir_Xan"
            /></bibl>).</etym>
        <etym xml:lang="en">Cf. <mentioned xml:lang="ar" extralang="fa"><lang/>
            <w>šāš</w>
            <gloss><q>turban</q></gloss>, <note type="bibl">(<bibl><ref type="bibl"
                  target="#ref_Ягелло"/></bibl>)</note>
            <gloss><q>thin fabric</q></gloss>, <gloss><q>muslin</q></gloss>
            <note xml:lang="ru" type="comment">(<bibl><ref type="bibl" target="#ref_Гаффаров"
                />)</bibl></note><note xml:lang="en" type="comment">(<bibl><ref type="bibl"
                  target="#ref_Гаффаров"/>)</bibl></note></mentioned>, <mentioned xml:lang="ku"><lang/>
