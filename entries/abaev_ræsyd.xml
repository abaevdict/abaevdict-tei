        <form type="lemma" xml:lang="os-x-iron"><orth>ræsyd</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>ræsud</orth></form>
        <sense xml:id="sense_d360e74"><sense xml:id="sense_d360e75"><abv:tr xml:lang="ru">
              <q>опухоль</q>
            </abv:tr><abv:tr xml:lang="en">
