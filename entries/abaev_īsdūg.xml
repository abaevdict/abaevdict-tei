<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_īsdūg"
    xml:lang="os-x-iron">
    <form type="lemma">
        <orth>īsdūg</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>сперва</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>at first</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>поначалу</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>firstly</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>некоторое время</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>somewhile</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ældar īsdūgmæ næ razy kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>алдар сперва не соглашался</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Aldar did not agree at first</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>I 154</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>mīdæmæ xīzyn dæ īsdūg næ waʒʒysty</quote>
            <cit type="translation" xml:lang="ru">
                <q>войти внутрь тебе сперва не позволят</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>at first you will not be allowed to go inside</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>77</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>isdugmæ ew, dungi æzmælunmæ gæsgæ, adæni ʒæfqu fetingdær widæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>на некоторое время, по движению ветра, усиливался гомон людей</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>for some time, in the wind, people's noise intensified</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>100</biblScope>
            </bibl>
        </cit>
    </cit>
