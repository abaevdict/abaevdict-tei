    <sense>
        <cit type="translation" xml:lang="ru">
            <q>сильно стягивать</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>to tighten strongly</q>
        </cit>
    </sense>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>тащить</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to pull</q>
            </cit>
        </sense>
        <sense>
            <note type="comment" xml:lang="ru">д.</note>
            <note type="comment" xml:lang="en">Digor</note>
            <cit type="translation" xml:lang="ru">
                <q>бросать</q>
            </cit>
            <note type="comment" xml:lang="ru">д.</note>
            <note type="comment" xml:lang="en">Digor</note>
            <cit type="translation" xml:lang="en">
                <q>to throw</q>
            </cit>
        </sense>
        <note type="comment" xml:lang="ru">также</note>
        <note type="comment" xml:lang="en">also</note>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>æxtængtæ bantʼуxta</quote>
            <cit type="translation" xml:lang="ru">
                <q>он сильно стянул подпруги</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he strongly pulled saddle girths</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>kædimajdima xucaw ærintʼoxidæ amond</quote>
            <cit type="translation" xml:lang="ru">
                <q>авось бог приволочет счастье</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>perhaps god will drag happiness</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>101</biblScope>
