      <entry xml:id="entry_ūjas" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d5430e66" type="lemma" xml:lang="os-x-iron"><orth>ūjas</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>ojasæ</orth></form>
        <sense>
          <sense xml:id="sense_d5430e69">
            <abv:tr xml:lang="ru">
              <q>столько</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>настолько</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>так много</q>
            </abv:tr>
          </sense>
        </sense>
        <abv:exampleGrp xml:id="exampleGrp_d5430e75">
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">k˳yd færæzta ūjas!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">как она была в состоянии столько (делать)!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>53</biblScope></bibl>
          </example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">kæd ojasæ xwarz læg dæ, wædta avd wæjugej
              zænxæbæl xumæ ærkænæ</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">если ты такой молодец, то вспаши землю семи
                великанов</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_MSt."/>
              <biblScope>7<hi rendition="#rend_subscript">12</hi></biblScope></bibl>
          </example>
        </abv:exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_ūj"><w>ūj</w></ref> и <ref type="xr"
            target="#entry_as"><w>as</w></ref>. Ср. <ref type="xr" target="#entry_ūjbærc"
              ><w>ūjbærc</w></ref>. <bibl><ref type="bibl" target="#ref_Миллер_Вс._ОЭ"
