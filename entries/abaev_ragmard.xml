      <entry xml:id="entry_ragmard" xml:lang="os" abv:completeness="complete">
        <form type="lemma"><orth>ragmard</orth></form>
        <sense><sense><abv:tr xml:lang="ru">
              <q>ранняя, преждевременная смерть</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>early, untimely death</q>
            </abv:tr></sense><sense><abv:tr xml:lang="ru">
              <q>рано умерший</q>
            </abv:tr><abv:tr xml:lang="en">
              <q>early deceased</q>
            </abv:tr></sense></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">arfæj xæssæ æ nomi, ʒæǧgæj
                <oRef>ragmardæn</oRef>
              <q rendition="#rend_doublequotes">roxsag</q></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">с благодарностью неси его имя, говоря рано
                умершему «<hi rendition="#rend_smallcaps">roxsag</hi>»</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">bear his name with gratitude, saying <q><hi
                    rendition="#rend_smallcaps">roxsag</hi></q> to the early deceased</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Gurdž._Wadž."/>
              <biblScope>11</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_rag"/> и <ref type="xr"
            target="#entry_mard"/>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_rag"/> and <ref type="xr"
            target="#entry_mard"/>.</etym>
      </entry>
