            <entry xml:id="entry_arvan" xml:lang="os">
                <form type="lemma"><orth>arvan</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>скоба для закрепления обода к колесу</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>clip for fixing rims to a wheel</q>
                        </abv:tr></sense><sense><note type="comment" xml:lang="ru">также</note><note
                            type="comment" xml:lang="en">also</note><abv:tr xml:lang="ru">
                            <q>втулка в барабане мельничного волчка</q>
                        </abv:tr><note type="comment" xml:lang="ru">также</note><note type="comment"
                            xml:lang="en">also</note><abv:tr xml:lang="en">
                            <q>bushing in mill drum</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="ru"><lang/>
                        <w>арвант</w><note type="comment" xml:lang="ru"> (диалект терских
                            казаков)</note><note type="comment" xml:lang="en"> (dialect of Terek
                            Cossacks)</note>
                        <gloss><q>железное кольцо, охватывающее обод колеса</q></gloss></mentioned>
                            (<bibl><ref type="bibl" target="#ref_СМК"/><biblScope>XXXVII
                            88</biblScope></bibl>).</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="ru"><lang/>
                        <w>арвант</w><note type="comment" xml:lang="ru"> (диалект терских
                            казаков)</note><note type="comment" xml:lang="en"> (dialect of Terek
                            Cossacks)</note>
                        <gloss><q>iron ring around the wheel rim</q></gloss></mentioned> (<bibl><ref
                            type="bibl" target="#ref_СМК"/><biblScope>XXXVII
                    88</biblScope></bibl>).</etym>
            </entry>
