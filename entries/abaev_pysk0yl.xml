      <entry xml:id="entry_pysk0yl" xml:lang="os-x-iron" abv:completeness="incomplete">
        <form type="lemma"><orth>pyskₒyl</orth></form>
        <usg><lang/></usg>
        <sense><abv:tr xml:lang="ru">
            <q>тряпка</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>rag</q>
          </abv:tr></sense>
        <etym xml:lang="ru">Ср. <mentioned xml:lang="krc-x-karachay"><lang/>
            <w>pusxul</w>, <w>busxul</w>
            <gloss><q>тряпка</q>, <q>пеленка</q></gloss></mentioned>, <mentioned xml:lang="tr"><lang/>
            <w>buskul</w>
            <gloss><q>пеленка</q></gloss>
            <note type="bibl">(<bibl><ref type="bibl" target="#ref_Eren, Studia caucasica 1963">#<hi
                    rendition="#rend_smallcaps">Eren</hi>, Studia caucasica 1963</ref>
                <biblScope>I 101</biblScope></bibl>)</note></mentioned>. Ср. <ref type="xr"
            target="#entry_pysūl"/>.</etym>
        <etym xml:lang="en">Cf. <mentioned xml:lang="krc-x-karachay"><lang/>
            <w>pusxul</w>, <w>busxul</w>
            <gloss><q>rag</q>, <q>swaddle</q></gloss></mentioned>, <mentioned xml:lang="tr"><lang/>
            <w>buskul</w>
            <gloss><q>swaddle</q></gloss>
            <note type="bibl">(<bibl><ref type="bibl" target="#ref_Eren, Studia caucasica 1963">#<hi
                    rendition="#rend_smallcaps">Eren</hi>, Studia caucasica 1963</ref>
                <biblScope>I 101</biblScope></bibl>)</note></mentioned>. Cf. <ref type="xr"
            target="#entry_pysūl"/>.</etym>
