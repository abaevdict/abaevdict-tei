            <entry xml:id="entry_ʒaxana" xml:lang="os-x-digor">
                <form type="lemma"><orth>ʒaxana</orth><form type="variant"
                            ><orth>ʒazan(a)-maxan(a)</orth></form></form>
                <usg><lang/></usg>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>ад</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>hell</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>преисподняя</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>underworld</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>Soslan Satanaj ʒaxan-maxani cadæj fæjjervæzun kodta</quote>
                        <abv:tr xml:lang="ru">
                            <q>Сослан спас Сатану из адского озера</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Soslan saved Shatana from the lake of hell</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰahannam</w></mentioned> id. У всех мусульманских народов; ср. <mentioned
                        xml:lang="tr"><lang/>
                        <w>ǰehennem</w></mentioned>, <mentioned xml:lang="lez"><lang/>
                        <w>ǯehènnem</w></mentioned>, <mentioned xml:lang="ab"><lang/>
                        <w>ǯaħanǝ̀m</w></mentioned> и др.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰahannam</w></mentioned> id. It is found in all Muslim people's
                    languages; cf. <mentioned xml:lang="tr"><lang/>
                        <w>ǰehennem</w></mentioned>, <mentioned xml:lang="lez"><lang/>
                        <w>ǯehènnem</w></mentioned>, <mentioned xml:lang="ab"><lang/>
                        <w>ǯaħanǝ̀m</w></mentioned> etc.</etym>
            </entry>
