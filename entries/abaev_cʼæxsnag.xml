<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_cʼæxsnag" xml:lang="os">
    <form type="lemma"><orth>cʼæxsnag</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>звонкий</q>
            </cit><cit type="translation" xml:lang="en">
                <q>sonorous</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>резкий (голос)</q>
            </cit><cit type="translation" xml:lang="en">
                <q>sharp (voice)</q>
            </cit></sense></sense>
    <re><form type="lemma"><orth>cʼæxsnag wīs</orth></form><sense><cit type="translation"
                xml:lang="ru">
                <q>прут, свистящий при взмахе</q>
            </cit><cit type="translation" xml:lang="en">
                <q>whistling rod</q>
            </cit></sense></re>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>wo, næ zærbatug, dæ zard cʼæxsnag zællangæj kænis</quote>
            <cit type="translation" xml:lang="ru">
                <q>о наша ласточка, ты ведешь свою песню звонкой трелью</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>oh our swallow, you lead your song with a sonorous trill</q>
            </cit>
            <bibl><ref target="#src_Qazb."/><biblScope>37</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
