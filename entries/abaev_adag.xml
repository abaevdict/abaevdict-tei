<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_adag"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>adag</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>adagæ</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>овраг</q>
            </cit><cit type="translation" xml:lang="en">
                <q>gully</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>балка</q>
            </cit><cit type="translation" xml:lang="en">
                <q>beam</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Swadag %i(из%i saw-adag%i)%i</quote>
            <cit type="translation" xml:lang="ru">
                <q>Черная балка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Black beam</q>
            </cit>
            <note type="comment" xml:lang=""> ()</note>
            <note type="comment" xml:lang=""> ()</note>
        </cit>
        <cit type="example">
            <quote>x˳yskʼ adag</quote>
            <cit type="translation" xml:lang="ru">
                <q>сухая балка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>dry beam</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>wyngæg adagyl læsæn racyd</quote>
            <cit type="translation" xml:lang="ru">
                <q>по узкому оврагу спустился обвал</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a landslide descended along a narrow gully</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>173</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>lappū adag-adag nywwad æmæ xærīsǵyny nynnyǧ˳yld</quote>
            <cit type="translation" xml:lang="ru">
                <q>мальчик спустился вдоль по оврагу и затаился в ивняке</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the boy went down along the gully and hid in the willow</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>185</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>dæ furt Soslan ali adagæ fidiʒagæj ku dardta</quote>
            <cit type="translation" xml:lang="ru">
                <q>твой сын Сослан держал все овраги полными мяса (убитых зверей)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your son Soslan kept all the gullies full of meat (of killed animals)</q>
            </cit>
            <bibl><ref target="#src_Диг._сказ."/><biblScope>36</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>jeʽrfguti ʽxsæn tærnixi car adægtæ-adægtæ rabadtæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>на коже его лба между бровей пролегли борозды</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>there are furrows between his eyebrows on the skin of his forehead</q>
            </cit>
            <bibl><ref target="#src_Bes._Ka_ke"/><biblScope>63</biblScope></bibl>
        </cit>
    </cit>
