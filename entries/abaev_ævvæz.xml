<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ævvæz"
    xml:lang="os">
    <form type="lemma">
        <orth>ævvæz</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>выровненный</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>evened</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>ровный по поверхности</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>with an even surface</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>одинаковый по уровню</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>of the same level</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>ævvæz kænyn</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>выравнивать (поверхность)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to even (a surface)</q>
            </cit>
        </sense>
    </re>
    <etym xml:lang="ru">Из %iæm%i + %ifæz%i; см. <ref type="xr" target="#entry_æm-"/> и <ref
