            <w>nim</w></mentioned>, <mentioned xml:id="mentioned_d1624e812" xml:lang="sgy-x-zebak"><lang/>
            <w>nēm</w></mentioned>, <mentioned xml:id="mentioned_d1624e817" xml:lang="wbl"><lang/>
            <w>nōŋg</w><note type="bibl"> (<bibl><ref type="bibl" target="#ref_Lorimer"/>
                <biblScope>II 156</biblScope></bibl>)</note></mentioned>, <mentioned xml:id="mentioned_d1624e829" xml:lang="yai"><lang/>
            <w>nām</w> (<w>nom</w>)</mentioned>, <mentioned xml:id="mentioned_d1624e837" xml:lang="sog"><lang/>
