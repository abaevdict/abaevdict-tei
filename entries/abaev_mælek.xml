      <entry xml:id="entry_mælek" xml:lang="os" abv:completeness="complete">
        <form type="lemma"><orth>mælek</orth><form type="variant"><orth>mælīk</orth></form></form>
        <sense><abv:tr xml:lang="ru">
            <q>ангел</q>
            <note xml:lang="ru" type="comment"> (у мусульман)</note>
            
          </abv:tr><abv:tr xml:lang="en">
            <q>angel</q>
            <note type="comment"> (among Muslims)</note>
          </abv:tr></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">qurænttæ, <oRef>mælekti</oRef> stæn, Xæmic
              læg æj, ænæ dawæ læg</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">клянусь Коранами (и) малеками, Хамиц — мужчина,
                безупречный мужчина</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">I swear by the Qurans (and) the angels, Khæmits
                is a man, a man without reproach</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Bes._Ka_ke"/>
              <biblScope>37</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Из <mentioned xml:lang="ar"><lang/>
            <w>malak</w>, мн. ч. <w>malaīk</w>
            <gloss><q>вестник</q></gloss>, <gloss><q>ангел</q></gloss></mentioned>, <mentioned
            xml:lang="fa"><lang/>
            <w>malak</w>, <w>malaīk</w></mentioned>, <mentioned xml:lang="trk"><lang/>
            <w>melek</w></mentioned>, отсюда и <mentioned><mentioned xml:lang="kbd"><lang/>
              <w>melaək</w>
              <note type="bibl">(<bibl><ref type="bibl" target="#ref_СМК"/>
                  <biblScope>XII 1</biblScope></bibl>)</note></mentioned>, <mentioned xml:lang="av"><lang/>
              <w>malajik</w></mentioned>, <mentioned xml:lang="ce" extralang="inh"><lang/>
              <w>mælejk</w></mentioned>
            <gloss><q>ангел</q></gloss></mentioned> и др.</etym>
        <etym xml:lang="en">From <mentioned xml:lang="ar"><lang/>
            <w>malak</w>, pl. <w>malaīk</w>
            <gloss><q>messenger</q></gloss>, <gloss><q>angel</q></gloss></mentioned>, <mentioned
            xml:lang="fa"><lang/>
            <w>malak</w>, <w>malaīk</w></mentioned>, <mentioned xml:lang="trk"><lang/>
            <w>melek</w></mentioned>, whence also <mentioned><mentioned xml:lang="kbd"><lang/>
              <w>melaək</w>
              <note type="bibl">(<bibl><ref type="bibl" target="#ref_СМК"/>
                  <biblScope>XII 1</biblScope></bibl>)</note></mentioned>, <mentioned xml:lang="av"><lang/>
              <w>malajik</w></mentioned>, <mentioned xml:lang="ce" extralang="inh"><lang/>
              <w>mælejk</w></mentioned>
            <gloss><q>angel</q></gloss></mentioned> etc.</etym>
      </entry>
