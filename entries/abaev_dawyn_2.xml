    <text>
        <body>
            <entry xml:id="entry_dawyn_2" n="2" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>dawyn</orth><form type="participle"
                            ><orth>dawd</orth></form></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>dawun</orth><form
                        type="participle"><orth>dawd</orth></form></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>гладить</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>stroke</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>проводить рукой</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>pass the hand over</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>тереть</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>rub</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>полировать</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>polish</q>
                        </abv:tr></sense></sense>
                <sense><abv:tr xml:lang="ru">
                        <q>точить</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>sharpen</q>
                    </abv:tr></sense>
                <sense><abv:tr xml:lang="ru">
                        <q>обметать хлеб</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>%bdust bread%b</q>
                    </abv:tr></sense>
                <re>
                    <form type="lemma"><orth>ardwan</orth></form>
                    <sense><sense><abv:tr xml:lang="ru">
                                <q>большой напильник</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>big file</q>
                            </abv:tr></sense><sense><abv:tr xml:lang="ru">
                                <q>рашпиль</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>rasp file</q>
                            </abv:tr></sense></sense>
                </re>
                <re>
                    <form type="lemma"><orth>kard yssonyl dawyn</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>точить нож на оселке</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>sharpen a knife on a whetstone</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>Salǵenyq jæ bæxy fcæg dawy æmæ zæǧy…</quote>
                        <abv:tr xml:lang="ru">
                            <q>Салдженук гладит шею своей лошади и говорит…</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Salgenouk strokes his horse's neck and says …</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Čerm."
                            /><biblScope>104</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>jæ kʼūxæj bæxy raǧ adawdta</quote>
                        <abv:tr xml:lang="ru">
                            <q>он провел рукой по спине лошади</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>he ran his hand over the horse's back</q>
                        </abv:tr>
                    </abv:example>
                    <abv:example>
                        <quote>ysdawʒynæn æz acy kard</quote>
                        <abv:tr xml:lang="ru">
                            <q>я отточу эту шашку</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>I will sharpen this sabre</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Брит."/><biblScope>92</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>(cævægbæl) fætæn sawdor ærdawdditæ kænuj</quote>
                        <abv:tr xml:lang="ru">
                            <q>проводит (по косе) широким оселком</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>[he] passes a whetstone (over the scythe)</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Bes._Ka_ke"
                            /><biblScope>7</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>Ʒanxot jæ zaḱʼe adawdta</quote>
                        <abv:tr xml:lang="ru">
                            <q>Дзанхот погладил бороду</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Zanhot stroked his beard</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Сека"/><biblScope>126</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>Misirbij æ rexæ radawdta æ kʼoxæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>Мисирби погладил бороду рукой</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Misirbi stroked his beard with his hand</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>126</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>ræsuǧdi xæccæ æmvarstæj badʒinan fændonæj, dawgæj kæræʒej næ
                            kastæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>с красавицей бок о бок мы будем сидеть вволю, лаская друг друга
                                взглядами</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>with a beauty, side by side, we will sit to our heart's content,
                                caressing each other with glances</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Gurdž._Æduli"
                            /><biblScope>88</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>ungæg kombæl radawdta je ʼngas</quote>
                        <abv:tr xml:lang="ru">
                            <q>по узкому ущелью он провел взглядом</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>he glanced down the narrow gorge</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MD"/><biblScope>1936 II
                            34</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Основа %idhāv%i- со значением ‘тереть’, ‘чистить’ и пр. хорошо
                    представлена в индоиранских языках: <mentioned xml:lang="fa"><lang/>
                        <w>zidūdan : zidāy-(←*uz-dāv-)</w>
                        <gloss><q>очищать</q></gloss></mentioned>, <mentioned xml:lang="fa"><lang/>
                        <w>andūdan : andāy-(←*han-dāv-)</w>
                        <gloss><q>намазывать</q></gloss></mentioned>, <mentioned xml:lang="ps"><lang/>
                        <w>zdōyəl(←*uz-dāv-)</w>
                        <gloss><q>тереть</q></gloss></mentioned>, <mentioned xml:lang="ps"><w/>
                        <gloss><q>протирать</q></gloss></mentioned>, <mentioned xml:lang="ps"><w/>
                        <gloss><q>полировать</q></gloss></mentioned>, <mentioned
                        xml:lang="sgh-x-oroshori"><lang/>
                        <w>lawák</w><note type="comment" xml:lang="ru"> (из *%idawak%i)</note><note
                            type="comment" xml:lang="en"> (from *%idawak%i)</note>
                        <gloss><q>растирание</q></gloss></mentioned>, <mentioned
                        xml:lang="sgh-x-oroshori"><w/>
                        <gloss><q>полирование</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Зарубин._Орош."/><biblScope>41</biblScope></bibl>),
                        <mentioned xml:lang="ydg"><lang/>
                        <w>dā-um : dawd-um</w>
                        <gloss><q>натирать</q></gloss></mentioned>, <mentioned xml:lang="ydg"><w/>
                        <gloss><q>намазывать</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Morgenstierne._IIFL"/><biblScope>II
                    204</biblScope></bibl>), <mentioned xml:lang="yai"><lang/>
                        <w>daw-</w>
                        <gloss><q>мазать</q></gloss></mentioned>, <mentioned xml:lang="yai"><w/>
                        <gloss><q>штукатурить</q></gloss></mentioned>, <mentioned xml:lang="sog"><lang/>
                        <w>*δāw-</w>
                        <gloss><q>тереть</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>чистить</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>подметать</q></gloss></mentioned>, <mentioned xml:lang="sog"><lang/>
                        <w>anδāw-('nδ''w-)</w>
                        <gloss><q>намазывать</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>штукатурить</q></gloss></mentioned>, <mentioned xml:lang="ae"><lang/>
                        <w>dav-</w><note type="comment" xml:lang="ru"> (в %ifra-dav%i-)</note><note
                            type="comment" xml:lang="en"> (in %ifra-dav-%i)</note>
                        <gloss><q>вытирать</q></gloss></mentioned>, <mentioned xml:lang="ae"><w/>
                        <gloss><q>очищать</q></gloss></mentioned>, <mentioned xml:lang="sa"><lang/>
                        <w>dhāv-</w>
                        <gloss><q>вытирать</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>очищать</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>полировать</q></gloss></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>III 151</biblScope></bibl>,
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."/><biblScope>30,
                            65</biblScope></bibl>. — <bibl><ref type="bibl" target="#ref_Reichelt"
                            /><biblScope>ZII 1926 IV 245</biblScope></bibl>. — <bibl><ref
                            type="bibl" target="#ref_Benveniste._TSP"
                        /><biblScope>196</biblScope></bibl>.</etym>
                <etym xml:lang="en">The stem %idhāv%i- with the meaning ‘rub’, ‘clean’ etc. is well
                    presented in Indo-Iranian languages: <mentioned xml:lang="fa"><lang/>
                        <w>zidūdan : zidāy-(←*uz-dāv-)</w>
                        <gloss><q>clean</q></gloss></mentioned>, <mentioned xml:lang="fa"><lang/>
                        <w>andūdan : andāy-(←*han-dāv-)</w>
                        <gloss><q>spread on</q></gloss></mentioned>, <mentioned xml:lang="ps"><lang/>
                        <w>zdōyəl(←*uz-dāv-)</w>
                        <gloss><q>rub</q></gloss></mentioned>, <mentioned xml:lang="ps"><w/>
                        <gloss><q>wipe clean</q></gloss></mentioned>, <mentioned xml:lang="ps"><w/>
                        <gloss><q>polish</q></gloss></mentioned>, <mentioned
                        xml:lang="sgh-x-oroshori"><lang/>
                        <w>lawák</w><note type="comment" xml:lang="ru"> (из *%idawak%i)</note><note
                            type="comment" xml:lang="en"> (from *%idawak%i)</note>
                        <gloss><q>rubbing</q></gloss></mentioned>, <mentioned
                        xml:lang="sgh-x-oroshori"><w/>
                        <gloss><q>polishing</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Зарубин._Орош."/><biblScope>41</biblScope></bibl>),
                        <mentioned xml:lang="ydg"><lang/>
                        <w>dā-um : dawd-um</w>
                        <gloss><q>polish</q></gloss></mentioned>, <mentioned xml:lang="ydg"><w/>
                        <gloss><q>spread on</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Morgenstierne._IIFL"/><biblScope>II
                    204</biblScope></bibl>), <mentioned xml:lang="yai"><lang/>
                        <w>daw-</w>
                        <gloss><q>smear</q></gloss></mentioned>, <mentioned xml:lang="yai"><w/>
                        <gloss><q>plaster</q></gloss></mentioned>, <mentioned xml:lang="sog"><lang/>
                        <w>*δāw-</w>
                        <gloss><q>rub</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>clean</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>sweep</q></gloss></mentioned>, <mentioned xml:lang="sog"><lang/>
                        <w>anδāw-('nδ''w-)</w>
                        <gloss><q>spread</q></gloss></mentioned>, <mentioned xml:lang="sog"><w/>
                        <gloss><q>plaster</q></gloss></mentioned>, <mentioned xml:lang="ae"><lang/>
                        <w>dav-</w><note type="comment" xml:lang="ru"> (в %ifra-dav%i-)</note><note
                            type="comment" xml:lang="en"> (in %ifra-dav-%i)</note>
                        <gloss><q>sweep</q></gloss></mentioned>, <mentioned xml:lang="ae"><w/>
                        <gloss><q>clean</q></gloss></mentioned>, <mentioned xml:lang="sa"><lang/>
                        <w>dhāv-</w>
                        <gloss><q>wipe</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>clean</q></gloss></mentioned>, <mentioned xml:lang="sa"><w/>
                        <gloss><q>polish</q></gloss></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>III 151</biblScope></bibl>,
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."/><biblScope>30,
                            65</biblScope></bibl>. — <bibl><ref type="bibl" target="#ref_Reichelt"
                            /><biblScope>ZII 1926 IV 245</biblScope></bibl>. — <bibl><ref
                            type="bibl" target="#ref_Benveniste._TSP"
                        /><biblScope>196</biblScope></bibl>.</etym>
            </entry>
        </body>
