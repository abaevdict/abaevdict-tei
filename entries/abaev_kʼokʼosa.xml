            <entry xml:id="entry_kʼokʼosa" xml:lang="os-x-iron">
                <form type="lemma"><orth>kʼokʼosa</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d4357e72"><abv:tr xml:lang="ru">
                        <q>шишка</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>bump</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d4357e84">
                        <quote>mæ nyx nyccavton æmæ ʽskʼokʼosa ī</quote>
                        <abv:tr xml:lang="ru">
                            <q>я ушиб лоб, и на нем вскочила шишка</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>I bruised my forehead and a bump came up on it</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:id="mentioned_d4357e102" xml:lang="ka"><lang/>
                        <w>kʼokʼosa</w>
                        <gloss><q>сосуд из тыквы</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4357e110" xml:lang="ka"><w>kʼokʼola</w>
                        <gloss><q>шишка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4357e116" xml:lang="sva"><lang/>
                        <w>kokweč</w>
                        <gloss><q>хохолок</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned xml:id="mentioned_d4357e128" xml:lang="ka"><lang/>
                        <w>kʼokʼosa</w>
                        <gloss><q>vessel made of pumpkin</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4357e136" xml:lang="ka"><w>kʼokʼola</w>
                        <gloss><q>bump</q></gloss></mentioned>, <mentioned xml:id="mentioned_d4357e142" xml:lang="sva"><lang/>
                        <w>kokweč</w>
                        <gloss><q>pappus</q></gloss></mentioned>.</etym>
            </entry>
