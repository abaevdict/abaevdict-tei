            <entry xml:id="entry_ʒūtt" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>ʒūtt</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>ʒiwitt</orth></form>
                <sense xml:id="sense_d2092e72"><sense xml:id="sense_d2092e73"><abv:tr xml:lang="ru">
                            <q>еврей</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jew</q>
                        </abv:tr></sense><sense xml:id="sense_d2092e82"><abv:tr xml:lang="ru">
                            <q>еврейский народ</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jewish people</q>
                        </abv:tr></sense></sense>
                <re xml:id="re_d2092e92">
                    <form xml:lang="os-x-iron" type="lemma"><orth>ʒuttag</orth></form>
                    <form xml:lang="os-x-digor" type="dialectal"><orth>ʒiwittag</orth></form>
                    <sense xml:id="sense_d2092e100"><abv:tr xml:lang="ru">
                            <q>еврей</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jew</q>
                        </abv:tr></sense>
                </re>
                <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d2092e113" xml:lang="ar"><lang/>
                        <w>jahūd</w></mentioned>, <mentioned xml:id="mentioned_d2092e118" xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰuhūd</w></mentioned>, <mentioned xml:id="mentioned_d2092e123" xml:lang="trk"><lang/>
                        <w>ǯivut</w></mentioned>, <mentioned xml:id="mentioned_d2092e128" xml:lang="trk"><w>čivut</w></mentioned>, <mentioned xml:id="mentioned_d2092e131" xml:lang="trk"><w>čufut</w></mentioned>.
                    Сюда же <mentioned xml:id="mentioned_d2092e135" xml:lang="kbd"><lang/>
                        <w>žut</w>
                        <gloss><q>еврей</q></gloss></mentioned>.%n<bibl><ref type="bibl" target="#ref_Вс._Миллер._ОЭ"/><biblScope>II
                    79</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:id="mentioned_d2092e151" xml:lang="ar"><lang/>
                        <w>jahūd</w></mentioned>, <mentioned xml:id="mentioned_d2092e156" xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰuhūd</w></mentioned>, <mentioned xml:id="mentioned_d2092e161" xml:lang="trk"><lang/>
                        <w>ǯivut</w></mentioned>, <mentioned xml:id="mentioned_d2092e166" xml:lang="trk"><w>čivut</w></mentioned>, <mentioned xml:id="mentioned_d2092e169" xml:lang="trk"><w>čufut</w></mentioned>.
                    Also here <mentioned xml:id="mentioned_d2092e173" xml:lang="kbd"><lang/>
                        <w>žut</w>
                        <gloss><q>jew</q></gloss></mentioned>.%n<bibl><ref type="bibl" target="#ref_Вс._Миллер._ОЭ"/><biblScope>II
                    79</biblScope></bibl>.</etym>
