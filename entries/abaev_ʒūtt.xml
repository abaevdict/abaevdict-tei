            <entry xml:id="entry_ʒūtt" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>ʒūtt</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>ʒiwitt</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>еврей</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jew</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>еврейский народ</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jewish people</q>
                        </abv:tr></sense></sense>
                <re>
                    <form xml:lang="os-x-iron" type="lemma"><orth>ʒuttag</orth></form>
                    <form xml:lang="os-x-digor" type="dialectal"><orth>ʒiwittag</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>еврей</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>jew</q>
                        </abv:tr></sense>
                </re>
                <etym xml:lang="ru">Из <mentioned xml:lang="ar"><lang/>
                        <w>jahūd</w></mentioned>, <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰuhūd</w></mentioned>, <mentioned xml:lang="trk"><lang/>
                        <w>ǯivut</w></mentioned>, <mentioned xml:lang="trk"
                    ><w>čivut</w></mentioned>, <mentioned xml:lang="trk"><w>čufut</w></mentioned>.
                    Сюда же <mentioned xml:lang="kbd"><lang/>
                        <w>žut</w>
                        <gloss><q>еврей</q></gloss></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>II
                    79</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ar"><lang/>
                        <w>jahūd</w></mentioned>, <mentioned xml:lang="ar" extralang="fa"><lang/>
                        <w>ǰuhūd</w></mentioned>, <mentioned xml:lang="trk"><lang/>
                        <w>ǯivut</w></mentioned>, <mentioned xml:lang="trk"
                    ><w>čivut</w></mentioned>, <mentioned xml:lang="trk"><w>čufut</w></mentioned>.
                    Also here <mentioned xml:lang="kbd"><lang/>
                        <w>žut</w>
                        <gloss><q>jew</q></gloss></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>II
