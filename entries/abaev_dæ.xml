    <form type="lemma"><orth>dæ</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>тебя</q>
            </cit><note type="comment" xml:lang="ru"> (энклит.)</note><note type="comment"
                xml:lang="en"> (enclitic)</note><cit type="translation" xml:lang="en">
                <q>thou (ACC)</q>
            </cit><note type="comment" xml:lang="ru"> (энклит.)</note><note type="comment"
                xml:lang="en"> (enclitic)</note></sense><sense><cit type="translation" xml:lang="ru">
                <q>твой</q>
            </cit><cit type="translation" xml:lang="en">
                <q>your</q>
            </cit></sense></sense>
    <re><re><form type="lemma"><orth>dæ-xædæg</orth></form><sense><cit type="translation"
                    xml:lang="ru">
                    <q>ты сам</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>thyself</q>
                </cit></sense></re><re><form type="lemma"
                        ><orth>dæ-xi</orth></form><sense><sense><cit type="translation"
                        xml:lang="ru">
                        <q>тебя самого</q>
                    </cit><cit type="translation" xml:lang="en">
                        <q>thyself (acc)</q>
                    </cit></sense><sense><cit type="translation" xml:lang="ru">
                        <q>твой собственный</q>
                    </cit><cit type="translation" xml:lang="en">
                        <q>thy own</q>
                    </cit></sense></sense></re></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ragæj dæ kædton mæ wyndæj tærsyn</quote>
            <cit type="translation" xml:lang="ru">
                <q>давно я пугал тебя своим видом</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>for a long time I scared thou with my appearance</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>23</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>am dæ kalati cættæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>здесь твоя клетка готова</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>here thy cage is ready</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>122</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>kūræm dæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>мы просим у тебя</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>we are asking thou</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>122</biblScope></bibl>
        </cit>
    </cit>
