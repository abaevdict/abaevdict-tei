            <entry xml:id="entry_fælqæzæn" xml:lang="os-x-digor">
                <form type="lemma"><orth>fælqæzæn</orth></form>
                <usg><lang/></usg>
                <sense><abv:tr xml:lang="ru">
                        <q>волна</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>wave</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>...ʒikkotæ ... fælqæzænti ... felawuncce</quote>
                        <abv:tr xml:lang="ru">
                            <q>косы колышутся в волнах</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>braids sway in the waves</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>41</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>fælqæzæn fælqæzænbæl ... kælidæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>волна на волну набегала</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a wave ran into a wave</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Gurdž._Wadž."
