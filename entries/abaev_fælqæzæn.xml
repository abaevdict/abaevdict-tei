        <body>
            <entry xml:id="entry_fælqæzæn" xml:lang="os-x-digor">
                <form xml:id="form_d4025e66" type="lemma"><orth>fælqæzæn</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d4025e72"><abv:tr xml:lang="ru">
                        <q>волна</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>wave</q>
                    </abv:tr></sense>
                <abv:exampleGrp xml:id="exampleGrp_d4025e82">
                    <abv:example xml:id="example_d4025e84">
                        <quote>…ʒikkotæ … fælqæzænti … felawuncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>косы колышутся в волнах</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>braids sway in the waves</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>41</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d4025e103">
                        <quote>fælqæzæn fælqæzænbæl … kælidæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>волна на волну набегала</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a wave ran into a wave</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Gurdž._Wadž."/><biblScope>35</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
