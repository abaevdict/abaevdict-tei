            <entry xml:id="entry_ændarg" xml:lang="os-x-digor">
                <form type="lemma"><orth>ændarg</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d4509e72"><sense xml:id="sense_d4509e73"><abv:tr xml:lang="ru">
                            <q>окостеневший</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (?)</note><note type="comment" xml:lang="en"> (?)</note><abv:tr xml:lang="en">
                            <q>stiff</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (?)</note><note type="comment" xml:lang="en"> (?)</note></sense><sense xml:id="sense_d4509e90"><abv:tr xml:lang="ru">
                            <q>застывший</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (?)</note><note type="comment" xml:lang="en"> (?)</note><abv:tr xml:lang="en">
                            <q>stark (frozen?)</q>
                        </abv:tr><note type="comment" xml:lang="ru"> (?)</note><note type="comment" xml:lang="en"> (?)</note></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d4509e110">
                        <quote>med kʼox fedar nindarg æj kard</quote>
                        <abv:tr xml:lang="ru">
                            <q>в руке крепко окостенела шашка</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a sabre became stiff in the hand</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Iræf"/><biblScope>?
                            76</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Может быть, к %idar%i- ˈдержатьˈ. [См. <ref type="xr" target="#entry_daryn"/>]</etym>
                <etym xml:lang="en">May be related to %idar%i- ‘to hold’. [Cf. <ref type="xr" target="#entry_daryn"/>]</etym>
            </entry>
