    <text>
        <body>
            <entry xml:id="entry_kʼado" xml:lang="os-x-digor">
                <form type="lemma"><orth>kʼado</orth></form>
                <usg><lang/></usg>
                <sense><abv:tr xml:lang="ru">
                        <q>ветка</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>branch</q>
                    </abv:tr></sense>
                <re>
                    <form type="lemma"><orth>bælasi kʼado</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>ветка дерева</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>branch of a tree</q>
                        </abv:tr></sense>
                    <note type="bibl">(<bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                                20</biblScope></bibl>)</note>
                </re>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>wærccæ… bælasi kʼadojæj raxawdtæj</quote>
                        <abv:tr xml:lang="ru">
                            <q>перепелка свалилась с ветки дерева</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a quail fell from a tree branch</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                            118</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="ka"><lang/>
                        <w>k’adoni</w>
                        <gloss><q>брус</q></gloss></mentioned>, <mentioned xml:lang="ka"><w/>
                        <gloss><q>перекладина</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Саба"/></bibl>, <bibl><ref type="bibl"
                            target="#ref_Чубинов"/></bibl>), <mentioned xml:lang="ka"><lang/>
                        <w>k’ado</w>
                        <gloss><q>столб, к которому привязывают скотину</q></gloss></mentioned>
                            (<bibl><ref type="bibl" target="#ref_Беридзе"/></bibl>).</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="ka"><lang/>
                        <w>k’adoni</w>
                        <gloss><q>beam</q></gloss></mentioned>, <mentioned xml:lang="ka"><w/>
                        <gloss><q>crossbar</q></gloss></mentioned> (<bibl><ref type="bibl"
                            target="#ref_Саба"/></bibl>, <bibl><ref type="bibl"
                            target="#ref_Чубинов"/></bibl>), <mentioned xml:lang="ka"><lang/>
                        <w>k’ado</w>
                        <gloss><q>post to which cattle is tied</q></gloss></mentioned> (<bibl><ref
                            type="bibl" target="#ref_Беридзе"/></bibl>).</etym>
            </entry>
