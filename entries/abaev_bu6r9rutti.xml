    xml:id="entry_bu6r9rutti" xml:lang="os-x-digor">
    <form type="lemma">
        <orth>bu(r)rutti</orth>
        <form type="variant">
            <orth>brotti</orth>
        </form>
    </form>
    <sense>
        <def xml:lang="ru">выражение недовольства, гнева, каприза</def>
        <def xml:lang="en">an expression of discontent, anger, caprice</def>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Majra eci burruttitæj xæʒari zelæn kænun bajdædta</quote>
            <cit type="translation" xml:lang="ru">
                <q>Майра, такая же гневная, стала возиться по хозяйству</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Majra, outraged as well, started to busy herself with the housekeeping</q>
            </cit>
            <bibl>
                <ref target="#src_LQ"/>
                <biblScope>III 39</biblScope>
            </bibl>
        </cit>
    </cit>
