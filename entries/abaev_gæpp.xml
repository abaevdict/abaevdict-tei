<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_gæpp"
    xml:lang="os">
    <form type="lemma">
        <orth>gæpp</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>прыжок</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>jump</q>
        </cit>
    </sense>
    <re>
        <form type="lemma">
            <orth>gæpp kænyn</orth>
            <form type="variant">
                <orth>gæppytæ kænyn</orth>
            </form>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>прыгать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>jump</q>
            </cit>
        </sense>
    </re>
    <re>
        <form xml:lang="os-x-iron" type="lemma">
            <orth>gæppævvong</orth>
        </form>
        <form xml:lang="os-x-digor" type="dialectal">
            <orth>gæppærvong</orth>
        </form>
        <sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>наготове</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>(at the) ready</q>
                </cit>
            </sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>готовый к прыжку</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>ready to jump</q>
                </cit>
            </sense>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>agæpp æm kænon</quote>
            <cit type="translation" xml:lang="ru">
                <q>прыгну-ка я туда</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I'll jump there</q>
            </cit>
            <bibl>
                <ref target="#src_Коста"/>
                <biblScope>79</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>dæ bæx ma hæwylty agæpp kænyn kæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>заставь-ка свою лошадь прыгнуть через это</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>make your horse jump over it</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>26</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>īwmæ bagæpp kænūt nymæty astæwmæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>прыгайте вместе на середину войлока</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>jump together in the middle of the felt</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>72</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>Sozyryqo fægæpp lasta jæ bynatæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>Созрыко вскочил с места</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Sozryko jumped up</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>æ bæx mæsugi gorenmæ fezdaxta ma ibæl bagæpp kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>он повернул своего коня к ограде башни и перепрыгнул через нее</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he turned his horse to the wall of the tower and jumped over it</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>14%l4%l</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>mistæ ba fændurbæl ragæpp-bagæpp kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>мышка прыгала туда-сюда по фандуру</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the mouse jumped back and forth on the fandour</q>
            </cit>
            <bibl>
                <ref target="#src_MSt."/>
                <biblScope>17%l9–10%l</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>beræǧaw næmæ gæppærvongæj læwwuncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>они стоят, готовые наброситься на нас, как волки</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>they stand ready to pounce on us like wolves</q>
            </cit>
            <bibl>
                <ref target="#src_FS"/>
                <biblScope>V 5</biblScope>
            </bibl>
        </cit>
    </cit>
