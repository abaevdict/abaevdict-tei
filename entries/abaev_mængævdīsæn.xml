      <entry xml:id="entry_mængævdīsæn" xml:lang="os" abv:completeness="complete">
        <form xml:id="form_d1015e66" type="lemma"><orth>mængævdīsæn</orth></form>
        <sense xml:id="sense_d1015e69"><abv:tr xml:lang="ru">
            <q>лжесвидетель</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>false witness</q>
          </abv:tr></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:id="example_d1015e81">
            <quote xmlns="http://www.tei-c.org/ns/1.0">fæstagmæ ærbacydysty dywwæ
                <oRef>mængævdīsæny</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">наконец пришли два лжесвидетеля</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">at the last came two false witnesses</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Матфей"/>
              <biblScope><hi rendition="#rend_italic">26</hi> 60</biblScope></bibl>
          </example>
          <example xml:id="example_d1015e108">
            <quote xmlns="http://www.tei-c.org/ns/1.0"><oRef>mængævdīsæn</oRef> ma læw</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">не лжесвидетельствуй</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">do not bear false witness</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Марк"/>
              <biblScope><hi rendition="#rend_italic">10</hi> 19</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_mæng"/> и <ref type="xr" target="#entry_ævdīsæn"/>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_mæng"/> and <ref type="xr" target="#entry_ævdīsæn"/>.</etym>
