    <text>
        <body>
            <entry xml:id="entry_armukʼa" xml:lang="os">
                <form xml:id="form_d1519e66" type="lemma"><orth>armukʼa</orth></form>
                <sense xml:id="sense_d1519e69"><sense xml:id="sense_d1519e70"><abv:tr xml:lang="ru">
                            <q>большой рынок</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>big market</q>
                        </abv:tr></sense><sense xml:id="sense_d1519e79"><abv:tr xml:lang="ru">
                            <q>ярмарка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>fair</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d1519e91" xml:lang="ru"><lang/>
                        <w>ярмарка</w></mentioned> (← <mentioned xml:id="mentioned_d1519e96" xml:lang="de"><lang/>
                        <w>Jahrmarkt</w></mentioned>); оттуда же <mentioned xml:id="mentioned_d1519e101" xml:lang="inh"><lang/>
                        <w>jarmæke</w></mentioned>, <mentioned xml:id="mentioned_d1519e106" xml:lang="av"><lang/>
                        <w>yarmunka</w></mentioned>, <mentioned xml:id="mentioned_d1519e111" xml:lang="kbd-x-crc"><lang/>
                        <w>ermelək</w></mentioned>.%n<bibl><ref type="bibl" target="#ref_Вс._Миллер._ОЭ"/><biblScope>II 69</biblScope></bibl>;
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."/><biblScope>11</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:id="mentioned_d1519e128" xml:lang="ru"><lang/>
                        <w>jarmarka</w></mentioned> (← <mentioned xml:id="mentioned_d1519e133" xml:lang="de"><lang/>
                        <w>Jahrmarkt</w></mentioned>); the following words are also from it:
                        <mentioned xml:id="mentioned_d1519e138" xml:lang="inh"><lang/>
                        <w>jarmæke</w></mentioned>, <mentioned xml:id="mentioned_d1519e143" xml:lang="av"><lang/>
                        <w>yarmunka</w></mentioned>, <mentioned xml:id="mentioned_d1519e148" xml:lang="kbd-x-crc"><lang/>
                        <w>ermelək</w></mentioned>.%n<bibl><ref type="bibl" target="#ref_Вс._Миллер._ОЭ"/><biblScope>II 69</biblScope></bibl>;
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."/><biblScope>11</biblScope></bibl>.</etym>
            </entry>
