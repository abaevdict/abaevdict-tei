            <entry xml:id="entry_armukʼa" xml:lang="os">
                <form type="lemma"><orth>armukʼa</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>большой рынок</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>big market</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>ярмарка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>fair</q>
                        </abv:tr></sense></sense>
                <etym xml:lang="ru">Из <mentioned xml:lang="ru"><lang/>
                        <w>ярмарка</w></mentioned> (← <mentioned xml:lang="de"><lang/>
                        <w>Jahrmarkt</w></mentioned>); оттуда же <mentioned xml:lang="inh"><lang/>
                        <w>jarmæke</w></mentioned>, <mentioned xml:lang="av"><lang/>
                        <w>yarmunka</w></mentioned>, <mentioned xml:lang="x-crc"><lang/>
                        <w>ermelək</w></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>II 69</biblScope></bibl>;
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."
                            /><biblScope>11</biblScope></bibl>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="ru"><lang/>
                        <w>ярмарка</w></mentioned> (← <mentioned xml:lang="de"><lang/>
                        <w>Jahrmarkt</w></mentioned>); the following words are also from it:
                        <mentioned xml:lang="inh"><lang/>
                        <w>jarmæke</w></mentioned>, <mentioned xml:lang="av"><lang/>
                        <w>yarmunka</w></mentioned>, <mentioned xml:lang="x-crc"><lang/>
                        <w>ermelək</w></mentioned>.%n<bibl><ref type="bibl"
                            target="#ref_Вс._Миллер._ОЭ"/><biblScope>II 69</biblScope></bibl>;
                            <bibl><ref type="bibl" target="#ref_Вс._Миллер._Gr."
                            /><biblScope>11</biblScope></bibl>.</etym>
