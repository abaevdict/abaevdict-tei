    <text>
        <body>
            <entry xml:id="entry_ænʒyg" xml:lang="os">
                <form xml:id="form_d2458e66" xml:lang="os-x-iron" type="lemma"><orth>ænʒyg</orth></form>
                <form xml:id="form_d2458e69" xml:lang="os-x-digor" type="dialectal"><orth>ænʒug</orth></form>
                <sense xml:id="sense_d2458e72"><sense xml:id="sense_d2458e73"><abv:tr xml:lang="ru">
                            <q>скорченный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>writhed</q>
                        </abv:tr></sense><sense xml:id="sense_d2458e82"><abv:tr xml:lang="ru">
                            <q>сведенный</q>
                        </abv:tr><note type="comment" xml:lang="ru">(от холода и пр.)</note><note type="comment" xml:lang="en">(because of cold etc)</note><abv:tr xml:lang="en">
                            <q>grown numb</q>
                        </abv:tr><note type="comment" xml:lang="ru">(от холода и пр.)</note><note type="comment" xml:lang="en">(because of cold
                    etc)</note></sense></sense>
                <abv:exampleGrp xml:id="exampleGrp_d2458e100">
                    <abv:example xml:id="example_d2458e102">
                        <quote>mæ cong banʒyg ī</quote>
                        <abv:tr xml:lang="ru">
                            <q>моя рука закоченела</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>my hand grew numb</q>
                        </abv:tr>
                    </abv:example>
                    <abv:example xml:id="example_d2458e117">
                        <quote>banʒyg wyʒænī dæ g˳yr</quote>
                        <abv:tr xml:lang="ru">
                            <q>твой стан скорчится</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>your figure will writhe</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Niger"/><biblScope>76</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d2458e136" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>kʼæxti næ waʒuj ænʒug</quote>
                        <abv:tr xml:lang="ru">
                            <q>не дает застояться ногам</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>[it] does not let legs stand too long</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Qazb."/><biblScope>27</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Не ясно.</etym>
                <etym xml:lang="en">Etymology is not clear.</etym>
            </entry>
