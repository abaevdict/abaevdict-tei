            <entry xml:id="entry_ærtdor" xml:lang="os-x-digor">
                <form xml:id="form_d1291e66" type="lemma"><orth>ærtdor</orth><form
                        xml:id="form_d1291e68" type="variant"><orth>ætdor</orth></form><form
                        xml:id="form_d1291e70" type="variant"><orth>ættor</orth></form></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d1291e76">
                    <abv:tr xml:lang="ru">
                        <q>кремень</q>
                    </abv:tr>
                    <note type="comment" xml:lang="ru">(и. <ref type="xr" target="#entry_xson"
                        />)</note>
                    <note type="comment" xml:lang="en">(Iron <ref type="xr" target="#entry_xson"
                        />)</note>
                    <abv:tr xml:lang="en">
                        <q>flint</q>
                    </abv:tr>
                    <note type="comment" xml:lang="ru">(и. <ref type="xr" target="#entry_xson"
                        />)</note>
                    <note type="comment" xml:lang="en">(Iron <ref type="xr" target="#entry_xson"
                        />)</note>
                </sense>
                <etym xml:lang="ru">Из <hi rendition="#rend_italic">art-dor</hi>, буквально
                    „огонь-камень“; см. под <ref type="xr" target="#entry_ætdor"/>.</etym>
                <etym xml:lang="en">From <hi rendition="#rend_italic">art-dor</hi>, literally
                    'fire-stone'; see <ref type="xr" target="#entry_ætdor"/>.</etym>
            </entry>
