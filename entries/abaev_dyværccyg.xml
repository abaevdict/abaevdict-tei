<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dyværccyg" xml:lang="os-x-iron">
    <form type="lemma">
        <orth>dyværccyg</orth>
        <form type="variant">(<orth>dyværsyg</orth>)</form>
        <form type="variant">(<orth>dyværsag</orth>)</form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>двойной</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>double</q>
            </cit>
        </sense>
        <sense>
            <note type="comment" xml:lang="ru">в часности</note>
            <note type="comment" xml:lang="en">particularly</note>
            <cit type="translation" xml:lang="ru">
                <q>двойной плетень, внутри заполненный соломой или другим материалом</q>
            </cit>
            <note type="comment" xml:lang="ru">в часности</note>
            <note type="comment" xml:lang="en">particularly</note>
            <cit type="translation" xml:lang="en">
                <q>double wattle, filled with straw or other material inside</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>annætæ kafync dyværccygæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>остальные пляшут в два яруса (одни на плечах других)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>other people are dancing in two tiers (one on the shoulders of the other)</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
                <biblScope>93</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>dyværsag æfsæn dwar</quote>
            <cit type="translation" xml:lang="ru">
                <q>двустворчатая железная дверь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>double folding iron door</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>III 53</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>annætæ kafync dyværccygæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>остальные пляшут в два яруса (одни на плечах других)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>other people are dancing in two tiers (one on the shoulders of the other)</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
                <biblScope>93</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>dyværsag æfsæn dwar</quote>
            <cit type="translation" xml:lang="ru">
                <q>двустворчатая железная дверь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>double folding iron door</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>III 53</biblScope>
            </bibl>
        </cit>
    </cit>
