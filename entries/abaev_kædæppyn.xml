            <entry xml:id="entry_kædæppyn" xml:lang="os-x-iron">
                <form type="lemma"><orth>kædæppyn</orth></form>
                <usg><lang/></usg>
                <sense><abv:tr xml:lang="ru">
                        <q>авось</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>perhaps</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>īw araqq yn raxæss, kædæppyn næ nywwaʒid</quote>
                        <abv:tr xml:lang="ru">
                            <q>вынеси ему один (бокал) араки, авось (тогда) он оставит нас в
                                покое</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>bring him one (glass) of araka, maybe (then) he will leave us
                                alone</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Брит."
                            /><biblScope>136</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">См. <ref type="xr" target="#entry_kæd"/> и <ref type="xr"
                        target="#entry_æppyn6dær9"/>.</etym>
                <etym xml:lang="en">See <ref type="xr" target="#entry_kæd"/> and <ref type="xr"
                        target="#entry_æppyn6dær9"/>.</etym>
            </entry>
