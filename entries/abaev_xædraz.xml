      <entry xml:id="entry_xædraz" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d1597e66" type="lemma" xml:lang="os-x-iron"><orth>xædraz</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>xwædrazi</orth></form>
        <sense xml:id="sense_d1597e69">
          <abv:tr xml:lang="ru">
            <q>непосредственно перед</q>
          </abv:tr>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">īw ūs dæ <oRef>xædrazy</oRef>…</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">прямо перед тобой одна женщина…</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Коста"/>
              <biblScope>7</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">don lægætæn jæ <oRef/> cæwy</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">речка течет перед самой пещерой</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Сека"/>
              <biblScope>97</biblScope></bibl>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0"><q>Xor-xory</q> bon ma zydtoj ærægvæzzæǵy
              dær, fæzzygændty tawyny <oRef>xædrazmæ</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">праздник <q>Xop-xop</q> справляли еще поздней
                осенью, непосредственно перед севом озимых</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <date>1967</date>
              <biblScope>X 73</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_xæd-"/> и <ref type="xr"
            target="#entry_raz"/>; ср. <ref type="xr" target="#entry_xædfæstæ"/>.</etym>
      </entry>
