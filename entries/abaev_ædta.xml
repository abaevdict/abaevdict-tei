    <text>
        <body>
            <entry xml:id="entry_ædta" xml:lang="os-x-digor">
                <form type="lemma"><orth>ædta</orth></form>
                <usg><lang/></usg>
                <sense><abv:tr xml:lang="ru">
                        <q>тогда</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>then</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>ædta e ba æz æxwædæg dæn</quote>
                        <abv:tr xml:lang="ru">
                            <q>так я и есть он самый</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>so I am him</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MSt."
                            /><biblScope>8%l15%l</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>ædta dæ næ warzuj næ</quote>
                        <abv:tr xml:lang="ru">
                            <q>должно быть, он не любит тебя, нет</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>he must not love you, no</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MSt."
                            /><biblScope>11%l4%l</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>ædta со</quote>
                        <abv:tr xml:lang="ru">
                            <q>в таком случае иди</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>in that case, go</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MSt."
                            /><biblScope>16%l12%l</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из %iwæd-ta%i; для отпадения начального %iw%i ср. <ref type="xr"
                        target="#entry_yssæʒ"/>, <ref type="xr" target="#entry_ævz-"/>, <ref
                        type="xr" target="#entry_aryn"/>, <ref type="xr" target="#entry_Acyrūxs"/>.
                    См. <ref type="xr" target="#entry_wæd"/>.</etym>
                <etym xml:lang="en">From %iwæd-ta%i; for elision of the initial %iw%i cf. <ref
                        type="xr" target="#entry_yssæʒ"/>, <ref type="xr" target="#entry_ævz-"/>,
                        <ref type="xr" target="#entry_aryn"/>, <ref type="xr"
                        target="#entry_Acyrūxs"/>. See <ref type="xr" target="#entry_wæd"/>.</etym>
            </entry>
