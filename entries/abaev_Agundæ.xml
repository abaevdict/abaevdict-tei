            <entry xml:id="entry_Agundæ" xml:lang="os" abv:completeness="complete">
                <?oxy_comment_start author="oleg" timestamp="20231223T130157+0300" comment="checked"?>
                <form xml:id="form_d5658e66" type="lemma"><orth>Agundæ</orth></form><?oxy_comment_end?>
                <sense xml:id="sense_d5658e69">
                    <def xml:lang="ru">имя красавицы, героини нартовского эпоса</def>
                    <def xml:lang="en">name of a beauty, a heroine of the Nart epics</def>
                </sense>
                <abv:exampleGrp xml:id="exampleGrp_d5658e75">
                    <abv:example xml:id="example_d5658e77" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>gurvedawcæ ma naræg ærfgutæ Sajnægi kizgæ <oRef>Agundæ</oRef>
                            ræsuǧd</quote>
                        <abv:tr xml:lang="ru">
                            <q>с изящным станом и узкими бровями дочь Сайнага красавица Агунда</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q><?oxy_comment_start author="oleg" timestamp="20231223T130043+0300" comment="gurvedawcæ стройный, элегантный"?>elegant<?oxy_comment_end?>
                                and (with) narrow eyebrows, the daughter of Saynæg, beautiful
                                Agundæ</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_ОЭп."/>
                            <biblScope>61<hi rendition="#rend_subscript"
                            >276—277</hi></biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned corresp="#mentioned_d5658e147"
                        xml:id="mentioned_d5658e102" xml:lang="krc-x-balkar"><lang/>
                        <w>Agunda</w></mentioned>, <mentioned corresp="#mentioned_d5658e152"
                        xml:id="mentioned_d5658e107" xml:lang="ka-x-khevsur"><lang>хевсурское</lang>
                        <gloss>женское имя</gloss>
                        <w>Agunda</w></mentioned>, <mentioned corresp="#mentioned_d5658e157"
                        xml:id="mentioned_d5658e112" xml:lang="ab"><lang/>
                        <w>Gṷənda</w></mentioned> — героиня нартовского эпоса. Вероятно, к
                        <mentioned corresp="#mentioned_d5658e162" xml:id="mentioned_d5658e117"
                        xml:lang="xpr"><lang/>
                        <w>yʼkwnd</w>
                        <gloss><q>ὑάκινθος</q></gloss></mentioned> (<bibl><author>Henning</author>
                        <ref type="bibl" target="#ref_BSOS"/>
                        <biblScope>IX 89</biblScope></bibl> ), <mentioned
                        corresp="#mentioned_d5658e176" xml:id="mentioned_d5658e131" xml:lang="fa"><lang/>
                        <w>yākand</w></mentioned>, <mentioned corresp="#mentioned_d5658e181"
                        xml:id="mentioned_d5658e136" xml:lang="ka"><lang/>
                        <w>iagundi</w>
                        <gloss><q>яхонт</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned corresp="#mentioned_d5658e102"
                        xml:id="mentioned_d5658e147" xml:lang="krc-x-balkar"><lang/>
                        <w>Agunda</w></mentioned>, <mentioned corresp="#mentioned_d5658e107"
                        xml:id="mentioned_d5658e152" xml:lang="ka-x-khevsur"><lang>Khevsurian</lang>
                        <gloss>female name</gloss>
                        <w>Agunda</w></mentioned>, <mentioned corresp="#mentioned_d5658e112"
                        xml:id="mentioned_d5658e157" xml:lang="ab"><lang/>
                        <w>Gṷənda</w></mentioned> — a heroine of Nart epos. Probably related to
                        <mentioned corresp="#mentioned_d5658e117" xml:id="mentioned_d5658e162"
                        xml:lang="xpr"><lang/>
                        <w>yʼkwnd</w>
                        <gloss><q>ὑάκινθος</q></gloss></mentioned> (<bibl><author>Henning</author>
                        <ref type="bibl" target="#ref_BSOS"/>
                        <biblScope>IX 89</biblScope></bibl> ), <mentioned
                        corresp="#mentioned_d5658e131" xml:id="mentioned_d5658e176" xml:lang="fa"><lang/>
                        <w>yākand</w></mentioned>, <mentioned corresp="#mentioned_d5658e136"
                        xml:id="mentioned_d5658e181" xml:lang="ka"><lang/>
                        <w>iagundi</w>
                        <gloss><q>sapphire</q></gloss></mentioned>.</etym>
            </entry>
