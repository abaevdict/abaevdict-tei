<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dælgom" xml:lang="os">
    <form type="lemma"><orth>dælgom</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>лежащий ничком</q>
            </cit><cit type="translation" xml:lang="en">
                <q>prostrate</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>вверх дном</q>
            </cit><cit type="translation" xml:lang="en">
                <q>upside-down</q>
            </cit></sense></sense>
    <re><form type="lemma"><orth>dælgommæ</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>ничком</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>facedown</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>вверх дном</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>upside-down</q>
                </cit></sense></sense></re>
    <re><form type="lemma"><orth>dælgom kænyn</orth></form><sense><cit type="translation"
                xml:lang="ru">
                <q>опрокидывать</q>
            </cit><cit type="translation" xml:lang="en">
                <q>overturn</q>
            </cit></sense></re>
    <re><form type="lemma"><orth>dælgommæ gæsgæjæ</orth></form><sense><sense><cit type="translation"
                    xml:lang="ru">
                    <q>исподтишка</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>on the quiet</q>
                </cit></sense><sense><cit type="translation" xml:lang="ru">
                    <q>втихомолку</q>
                </cit><cit type="translation" xml:lang="en">
                    <q>secretly</q>
                </cit></sense></sense></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Æfxærdty Xæsanæ, Solæmany saq fyrt, dælgommæ x˳yssydi wærdony g˳yffæjy</quote>
            <cit type="translation" xml:lang="ru">
                <q>Афхардов Хасана, доблестный сын Соламана, лежал ничком в кузове арбы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Afkhardov Hasana, the brave son of Solaman, lay facedown in the back of the
                    cart</q>
            </cit>
            <bibl><ref target="#src_Æfx._Xæs."/><biblScope>112—113</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>darǧ stoli nixbæl dælgommæj ræxcuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>на краю длинного стола он дремлет лицом вниз</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he dozes facing down on the edge of a long table</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>283%l3%l</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>adæm kald bælæstaw adælgom ysty</quote>
            <cit type="translation" xml:lang="ru">
                <q>люди пали ничком, как срубленные деревья</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>people fell facedown like felled trees</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>183</biblScope></bibl>
        </cit>
    </cit>
