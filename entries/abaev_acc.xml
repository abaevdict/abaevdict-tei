    <form xml:lang="os-x-iron" type="lemma"><orth>acc</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>accæ</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>дикая утка</q>
        </cit><cit type="translation" xml:lang="en">
            <q>wild duck</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ūs fūrdmæ bašydi æmæ aččaw dony fæk˳yssi</quote>
            <cit type="translation" xml:lang="ru">
                <q>женщина подошла к морю и подобно дикой утке нырнула в воду</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the woman went to the sea and dived into the water like a wild duck</q>
            </cit>
            <bibl><ref target="#src_ЮОПам."/><biblScope>I 9</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>Barsægy šalx fætarsti, jæxī ačč festyn kodta æmæ fætæxy</quote>
            <cit type="translation" xml:lang="ru">
                <q>колесо Барсага испугалось, обратилось в дикую утку и улетело</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the wheel of Barsag got scared, turned into a wild duck and flew away</q>
            </cit>
            <bibl><ref target="#src_ЮОПам."/><biblScope>I 84</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>accæ-ʒawæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>охота за дикими утками</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>hunting for wild ducks</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>accitæ maruj</quote>
            <cit type="translation" xml:lang="ru">
                <q>убивает диких уток</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he kills wild ducks</q>
            </cit>
            <bibl><biblScope xml:lang="ru">со слов Г. Малиева</biblScope><biblScope xml:lang="en">G.
                    Maliev p.c.</biblScope></bibl>
        </cit>
    </cit>
