            <entry xml:id="entry_ǵelbet" xml:lang="os">
                <form xml:id="form_d581e66" xml:lang="os-x-iron" type="lemma"><orth>ǵelbet</orth></form>
                <form xml:id="form_d581e69" xml:lang="os-x-digor" type="dialectal"><orth>kelbet</orth></form>
                <sense xml:id="sense_d581e72"><sense xml:id="sense_d581e73"><abv:tr xml:lang="ru">
                            <q>достоинство</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>dignity</q>
                        </abv:tr></sense><sense xml:id="sense_d581e82"><abv:tr xml:lang="ru">
                            <q>внушающий уважение облик</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>respectable appearance</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp xml:id="exampleGrp_d581e92">
                    <abv:example xml:id="example_d581e94" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>cardæj issirdtaj kelbet</quote>
                        <abv:tr xml:lang="ru">
                            <q>(своей) жизнью ты нашел достоинство</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>you found dignity with (your) life</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Gurdž._Wadž."/><biblScope>9</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
