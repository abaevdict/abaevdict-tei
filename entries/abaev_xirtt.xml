      <entry xml:id="entry_xirtt" xml:lang="os-x-iron" abv:completeness="russian-questions">
        <form xml:id="form_d3620e66" type="lemma"><orth>xirtt</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d3620e69">
          <abv:tr xml:lang="ru">
            <q>сморканье</q>
          </abv:tr>
        </sense>
        <re>
          <form type="lemma"><orth>xirtt kænyn</orth></form>
          <sense>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">сморкаться</q>
              </tr>
            </sense>
            <sense>
              <abv:tr xml:lang="ru">
                <q>фыркать носом</q>
              </abv:tr>
            </sense>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">dæ fynʒ <oRef>nyxxirtt</oRef> kæn!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">высморкай нос!</q>
            </tr>
          </example>
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">Qazi jæ fynʒ mæsty <oRef>xirtt</oRef>
              nykkodta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">Кази сердито фыркнул носом</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Нафи"/>
              <biblScope>74</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Звукоподражательное.</etym>
