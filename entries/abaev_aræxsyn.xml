    <sense><sense><note type="comment" xml:lang="">() </note><note type="comment" xml:lang="">()
                </note><cit type="translation" xml:lang="ru">
                <q>подходить</q>
            </cit><note type="comment" xml:lang="">() </note><note type="comment" xml:lang="">()
                </note><cit type="translation" xml:lang="en">
                <q>match</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>соответствовать</q>
            </cit><cit type="translation" xml:lang="en">
                <q>correspond</q>
            </cit></sense><sense><note type="comment" xml:lang="">() </note><note type="comment"
                xml:lang="">() </note><cit type="translation" xml:lang="ru">
                <q>справляться</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>управляться (с делом)</q>
            </cit><cit type="translation" xml:lang="en">
                <q>get things done</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>уметь</q>
            </cit><cit type="translation" xml:lang="en">
                <q>be able</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>быть умелым</q>
            </cit><cit type="translation" xml:lang="en">
                <q>be skilful</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>nīcæmæ aræxsy</quote>
            <cit type="translation" xml:lang="ru">
                <q>ни к чему не имеет сноровки</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>has no skill for anything</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>Cæraj wyd... aræxsǵyn alcæmæ dær</quote>
            <cit type="translation" xml:lang="ru">
                <q>Царай был искусен (умел) во всем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Tsarai was skilled (able) in everything</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>129</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>fædīsontæj ḱī fæcaræxst axæmtæ dær wyd</quote>
            <cit type="translation" xml:lang="ru">
                <q>из тех, кто прибежал на тревогу, нашлись такие, которые действовали умело (не
                    растерялись)</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>there were those who acted skillfully (did not get confused) among those who came
                    running to the alarm</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>183</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>xæsty xabartæ sæ særæj sæ kæronmæ aræxsgæ raʒyrdta Bjaslan</quote>
            <cit type="translation" xml:lang="ru">
                <q>события войны сначала до конца умело рассказал Биаслан</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Biaslan skillfully told the events of the war from the beginning to the end</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>135</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>Dossanæ ræsuǧdi aræxsgaj bæxbæl isævardtoncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>красавицу Доссану осторожно посадили на коня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the beautiful Dossana was carefully put on a horse</q>
            </cit>
            <bibl><ref target="#src_MD"/><biblScope>1940 III 57</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>ærcynʒæj, xæskardæj xærzaræxst ḱyzgaw</quote>
            <cit type="translation" xml:lang="ru">
                <q>иглой, ножницами (я владел) искусно, как девушка</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(I managed with) a needle, scissors skillfully like a girl</q>
            </cit>
            <bibl><ref target="#src_Коста"/><biblScope>57</biblScope></bibl>
        </cit>
    </cit>
