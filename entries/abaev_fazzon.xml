<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_fazzon" xml:lang="os">
    <form type="lemma"><orth>fazzon</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>близнец</q>
        </cit><cit type="translation" xml:lang="en">
            <q>twin</q>
        </cit></sense>
    <re><form xml:lang="os-x-iron" type="lemma"><orth>fazzættæ</orth></form><form
            xml:lang="os-x-digor" type="dialectal"><orth>fazzænttæ</orth></form><sense><cit
                type="translation" xml:lang="ru">
                <q>близнецы</q>
            </cit><cit type="translation" xml:lang="en">
                <q>twins</q>
            </cit></sense></re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Wærxægæn rajg˳yrdīs dywwæ læppūjy, fazzættæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>у Уархага родились два сына, близнецы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Ouarhag had two sons, twins</q>
            </cit>
            <bibl><ref target="#src_Нарт._сказ."/><biblScope>3</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>dywwæ fazzony næ fenyn kodtoj madæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>двух близнецов не показали матери</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>two twins were not shown to their mother</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>IV 47</biblScope></bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ew sæǧæ nizzadæj fazænttæ duwwæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>одна коза родила двух близнецов</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q/>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>271%l2%l</biblScope></bibl>
        </cit>
    </cit>
