<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ævzyjyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>ævzyjyn</orth><form type="inflected"
            subtype="ptcp"><orth>ævzyd</orth></form></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>ævzujun</orth><form type="inflected"
            subtype="ptcp"><orth>ævzud</orth></form></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>выпадать, лезть(о волосах)</q>
            </cit><cit type="translation" xml:lang="en">
                <q>come out, fall out (of hair)</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>чахнуть</q>
            </cit><cit type="translation" xml:lang="en">
                <q>pine</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>rnæ saryq˳yntæ gasæj bavzydysty</quote>
            <cit type="translation" xml:lang="ru">
                <q>мои волосы совсем повыпали</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my hair completely fell out</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>ævzud ærfguti bunti misti cæstitæ...</quote>
            <cit type="translation" xml:lang="ru">
                <q>под облезлыми бровями мышиные глазки</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>there are mouse eyes under shabby eyebrows</q>
            </cit>
            <bibl><ref target="#src_Тайм._Мӕ_зӕрд."/><biblScope>49</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>bonæj bonmæ (Salu) ævzujgæ, tajgæ cæuj</quote>
            <cit type="translation" xml:lang="ru">
                <q>день ото дня (Салу) чахнет, тает</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Salu is pinning, wasting away from day to day</q>
            </cit>
            <bibl><ref target="#src_Bes._Ka_ke"/><biblScope>86</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>rnæ saryq˳yntæ gasæj bavzydysty</quote>
            <cit type="translation" xml:lang="ru">
                <q>мои волосы совсем повыпали</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my hair completely fell out</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
