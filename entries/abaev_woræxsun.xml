      <entry xml:id="entry_woræxsun" xml:lang="os-x-digor" abv:completeness="incomplete">
        <form xml:id="form_d4941e66" type="lemma"><orth>woræxsun</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d4941e69">
          <sense>
            <abv:tr xml:lang="ru">
              <q>вести себя беспокойно</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>behave restlessly</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>метаться</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>rush</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>вертеться</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>swirl</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>ворочаться с боку на бок</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>toss and turn from side to side</q>
            </abv:tr>
          </sense>
        </sense>
