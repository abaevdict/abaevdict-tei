    <text>
        <body>
            <entry xml:id="entry_g0yrʒ" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>g˳yrʒ</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>igurʒæ</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>зачаток</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>rudiment</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>зародыш</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>germ</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>эмбрион</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>embryo</q>
                        </abv:tr></sense></sense>
                <re xml:lang="os-x-digor">
                    <lang/>
                    <form type="lemma"><orth>igurcæ</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>рождение</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>birth</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>dæxecæj tuxgindær dæbæl fembaldæj, ævi igurcæj fæddæ qæræw?</quote>
                        <abv:tr xml:lang="ru">
                            <q>повстречался ли тебе (кто-нибудь) сильней тебя, или ты от рождения
                                одноглазый?</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Have you met anyone who is stronger than you or are you one-eyed from
                                birth?</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_MSt."
                            /><biblScope>26%l4—5%l</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">От <ref type="xr" target="#entry_g0yryn"/> | %iigurun%i
                    ‘рождаться’ с помощью форманта -%iʒ%i | -%iʒæ %i(-%ic%i | -%icæ%i).%n<bibl><ref
                            type="bibl" target="#ref_ОЯФ_I"
                    /><biblScope>573</biblScope></bibl></etym>
                <etym xml:lang="en">From <ref type="xr" target="#entry_g0yryn"/> | %iigurun%i ‘be
                    born’ using the formant -%iʒ%i | -%iʒæ %i(-%ic%i | -%icæ%i).%n<bibl><ref
                            type="bibl" target="#ref_ОЯФ_I"
                    /><biblScope>573</biblScope></bibl></etym>
            </entry>
