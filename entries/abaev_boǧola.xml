<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_boǧola" xml:lang="os">
    <form type="lemma"><orth>boǧola</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>бука</q>
        </cit><note type="comment" xml:lang="ru"> (в „детском“ языке)</note><note type="comment"
            xml:lang="en"> (in “child” language)</note><cit type="translation" xml:lang="en">
            <q>bogeyman</q>
        </cit><note type="comment" xml:lang="ru"> (в „детском“ языке)</note><note type="comment"
            xml:lang="en"> (in “child” language)</note></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ma ku! boǧola dæ baxærʒæn!<note type="comment" xml:lang="ru"> (говорят
                    детям)</note><note type="comment" xml:lang="en"> (to children)</note></quote>
            <cit type="translation" xml:lang="ru">
                <q>не плачь, бука тебя съест!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>do not cry, a bogeyman will eat you!</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ma ku! boǧola dæ baxærʒæn!<note type="comment" xml:lang="ru"> (говорят
                    детям)</note><note type="comment" xml:lang="en"> (to children)</note></quote>
            <cit type="translation" xml:lang="ru">
                <q>не плачь, бука тебя съест!</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>do not cry, a bogeyman will eat you!</q>
            </cit>
        </cit>
    </cit>
    <etym xml:lang="ru">Ср. <mentioned xml:lang="av"><lang/><w>boholo</w></mentioned>, <mentioned
            xml:lang="ka"><lang/><w>Beǧela</w><gloss><q>чудовище</q></gloss></mentioned><mentioned
            xml:lang="ka"><w/><gloss><q>царь дэвов</q></gloss></mentioned>, .</etym>
    <etym xml:lang="en">Cf. <mentioned xml:lang="av"><lang/><w>boholo</w></mentioned>, <mentioned
            xml:lang="ka"><lang/><w>Beǧela</w><gloss><q>monster</q></gloss></mentioned><mentioned
            xml:lang="ka"><w/><gloss><q>king of the Divs</q></gloss></mentioned>, .</etym>
</entry>
