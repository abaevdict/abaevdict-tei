    <text>
        <body>
            <entry xml:id="entry_cʼyrt" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>cʼyrt</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>cʼirt</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>брызга</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>splash</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>маленькая короткая струйка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>a small short jet</q>
                        </abv:tr></sense></sense>
                <re>
                    <form type="lemma"><orth>rūxsy cyrt</orth></form>
                    <sense><abv:tr xml:lang="ru">
                            <q>лучик света</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>light raylet</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>tūǵy cʼyrtt ʒy ne ʽrtoǧd</quote>
                        <abv:tr xml:lang="ru">
                            <q>ни капли крови с него не упало</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>not a drop of blood fell from him</q>
                        </abv:tr>
                    </abv:example>
                    <abv:example>
                        <quote>īw ranæj rūxsy cʼyrtt cyd</quote>
                        <abv:tr xml:lang="ru">
                            <q>в одном месте светился огонь</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>there was fire gleaming in one place</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Qazaq"/><biblScope>3</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Звукоподражательное слово.</etym>
                <etym xml:lang="en">An onomatopoetic word.</etym>
            </entry>
