            <entry xml:id="entry_ædyx" xml:lang="os" abv:completeness="complete">
                <?oxy_comment_start author="oleg" timestamp="20240128T125359+0300" comment="checked"?>
                <form xml:id="form_d4132e66" xml:lang="os-x-iron" type="lemma"
                    ><orth>ædyx</orth></form><?oxy_comment_end?>
                <form xml:id="form_d4132e69" xml:lang="os-x-digor" type="dialectal"
                        ><orth>æduxæ</orth></form>
                <sense xml:id="sense_d4132e72">
                    <abv:tr xml:lang="ru">
                        <q>бессильный</q>
                    </abv:tr>
                    <abv:tr xml:lang="en">
                        <q>powerless</q>
                    </abv:tr>
                </sense>
                <abv:exampleGrp xml:id="exampleGrp_d4132e82">
                    <abv:example xml:id="example_d4132e84">
                        <quote>X˳ycawmæ īw nyxas dær <oRef>ædyxæj</oRef> næ bazzajʒæn</quote>
                        <abv:tr xml:lang="ru">
                            <q>у Бога не останется бессильным никакое слово</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>for with God nothing shall be impossible</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Лука"/>
                            <biblScope><w>1</w> 37</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">См. <ref type="xr" target="#entry_æ-"/> и <ref type="xr"
                        target="#entry_tyx"><w>tyx</w>
                        <gloss><q>сила</q></gloss></ref>.</etym>
                <etym xml:lang="en">See <ref type="xr" target="#entry_æ-"/> and <ref type="xr"
                        target="#entry_tyx"><w>tyx</w>
                        <gloss><q>power</q></gloss></ref>.</etym>
            </entry>
