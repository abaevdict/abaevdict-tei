            <entry xml:id="entry_ædyx" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>ædyx</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>æduxæ</orth></form>
                <sense><abv:tr xml:lang="ru">
                        <q>бессильный</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>powerless</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>x˳ycawmæ īw nyxas dær ædyxæj næ bazzajʒæn</quote>
                        <abv:tr xml:lang="ru">
                            <q>у бога не останется бессильным никакое слово</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>For with God nothing shall be impossible</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Лука"/><biblScope>%i1%i
                            37</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
