    xml:id="entry_kʼodax" xml:lang="os">
    <form type="lemma">
        <orth>kʼodax</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>пень</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stump</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>колода</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>log</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>чурбан</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>block</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kʼodæx jæ fyccag cæfæj fæbæræg væjjy</quote>
            <cit type="translation" xml:lang="ru">
                <q>пень можно определить с первого удара</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stump can be rated from the first stroke</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>III 244</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>kʼodax ficcag cæfbæl bæræg æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>пень можно определить с первого удара</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>stump can be rated from the first stroke</q>
            </cit>
            <bibl>
                <ref target="#src_Sozur"/>
                <biblScope>9</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>patcax syn radta dynǵyr kærz kʼodax</quote>
            <cit type="translation" xml:lang="ru">
                <q>в качестве царя дал он им (Зевес лягушкам) большую ясеневую колоду</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he (Zeus) gave them (the frogs) a large ash log as a king</q>
            </cit>
            <bibl>
                <ref target="#src_Сека._Ир._фийау"/>
                <biblScope>40</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>nīcy æmbary, q˳yddag ne ʽvzary, kʼodax ū, bælas</quote>
            <cit type="translation" xml:lang="ru">
                <q>ничего не понимает, дела не разбирает, он — колода, дерево</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he doesn’t understand anything, he doesn’t do his chores, he is a log, a tree</q>
            </cit>
            <bibl>
