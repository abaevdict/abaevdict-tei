    <form xml:lang="os-x-iron" type="lemma"><orth>byryn</orth><form type="inflected" subtype="ptcp"
                ><orth>byrd</orth></form></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>burun</orth><form type="inflected"
            subtype="ptcp"><orth>burd</orth></form></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>ползти</q>
            </cit><cit type="translation" xml:lang="en">
                <q>crawl</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>скользить</q>
            </cit><note type="comment" xml:lang="">; </note><note type="comment" xml:lang="">;
                </note><cit type="translation" xml:lang="en">
                <q>slip</q>
            </cit><note type="comment" xml:lang="">; </note><note type="comment" xml:lang="">;
            </note></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kalm sūr zæxxmæ rabyrydīs</quote>
            <cit type="translation" xml:lang="ru">
                <q>змей выполз на сушу</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a snake crawled out to the land</q>
            </cit>
            <bibl><ref target="#src_Munk."/><biblScope>32</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>mæ sykʼatyl ysbyr</quote>
            <cit type="translation" xml:lang="ru">
                <q>влез на мои рога</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[he/it] crawled on my horns</q>
            </cit>
            <bibl><ref target="#src_Munk."/><biblScope>102</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>zymæg xæxty særæj ærbyrydi æfcǵytæm</quote>
            <cit type="translation" xml:lang="ru">
                <q>зима с вершин гор сползла к перевалам</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>winter crawled from the hills down to passes</q>
            </cit>
            <bibl><ref target="#src_Čerm."/><biblScope>76</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>īxyl fæbyrydtæn æmæ mæ zæng asast</quote>
            <cit type="translation" xml:lang="ru">
                <q>я поскользнулся на льду и сломал ногу</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I slipped on ice and broke my leg</q>
            </cit>
        </cit>
    </cit>
