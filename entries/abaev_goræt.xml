            <entry xml:id="entry_goræt" xml:lang="os">
                <form type="lemma"><orth>goræt</orth></form>
                <sense xml:id="sense_d5726e69"><abv:tr xml:lang="ru">
                        <q>город</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>town</q>
                    </abv:tr></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d5726e81">
                        <quote>Dottīmæ xorz næ kastī goræty ʒæǧæly læwwyn</quote>
                        <abv:tr xml:lang="ru">
                            <q>Дотти не нравилось без дела оставаться в городе</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Dotty didn't like staying in town idle</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Arsen"/><biblScope>50</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d5726e103" xml:lang="ru"><lang/>
                        <w>город (горат)</w></mentioned>; ср. <ref type="xr" target="#entry_saxar"/>.</etym>
                <etym xml:lang="en">From <mentioned xml:id="mentioned_d5726e113" xml:lang="ru"><lang/>
                        <w>gorod (gorat)</w>
                        <gloss><q>town</q></gloss></mentioned>; cf. <ref type="xr" target="#entry_saxar"/>.</etym>
            </entry>
