<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_fældajun" xml:lang="os-x-digor">
    <form type="lemma">
        <orth>fældajun</orth>
        <form type="inflected" subtype="ptcp">
            <orth>fældatt</orth>
        </form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>мочить</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>make wet</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>смачивать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>moisten</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>увлажнять</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>humidify</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Sona ... æ særbæddæn æmæ æ xwærujnægti barkʼi tasi niffældadta</quote>
            <cit type="translation" xml:lang="ru">
                <q>Сона вымочила в тазу свою косынку и свой мешочек для провизии</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Sona soaked her kerchief and her food bag in the basin</q>
            </cit>
            <bibl>
                <ref target="#src_AK"/>
                <biblScope>I 43</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Sona ... æ særbæddæn æmæ æ xwærujnægti barkʼi tasi niffældadta</quote>
            <cit type="translation" xml:lang="ru">
