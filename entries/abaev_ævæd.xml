<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ævæd"
    xml:lang="os">
    <form type="lemma">
        <orth>ævæd</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>без следа</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>without a trace</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>бесследный</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>traceless</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>без потомства</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>without posterity</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>ævæd rnīt</orth>
        </form>
        <sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>снег без следов</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>snow without traces</q>
                </cit>
            </sense>
            <sense>
                <cit type="translation" xml:lang="ru">
                    <q>девственный снег, по которому нет ничьих следов</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>virgin snow which does not have any traces</q>
                </cit>
            </sense>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>fydvædæj ævæd x˳yzdær</quote>
            <cit type="translation" xml:lang="ru">
                <q>лучше не иметь потомства, чем дурное потомство</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>it is better to have no posterity than to have bad one</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>fydvædæj ævæd x˳yzdær</quote>
            <cit type="translation" xml:lang="ru">
                <q>лучше не иметь потомства, чем дурное потомство</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>it is better to have no posterity than to have bad one</q>
            </cit>
        </cit>
    </cit>
    <etym xml:lang="ru">Сложение негативной частицы <ref type="xr" target="#entry_æ-"/> with <ref
            type="xr" target="#entry_fæd"/> 'след'.</etym>
    <etym xml:lang="en">The composition of the negative particle <ref type="xr" target="#entry_æ-"/>
        with <ref type="xr" target="#entry_fæd"/> 'trace'.</etym>
</entry>
