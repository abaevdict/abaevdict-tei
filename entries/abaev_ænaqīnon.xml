    <text>
        <body>
            <entry xml:id="entry_ænaqīnon" xml:lang="os">
                <form xml:id="form_d3148e66" type="lemma"><orth>ænaqīnon</orth></form>
                <sense xml:id="sense_d3148e69"><sense xml:id="sense_d3148e70"><abv:tr xml:lang="ru">
                            <q>несуразный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>awkward</q>
                        </abv:tr></sense><sense xml:id="sense_d3148e79"><abv:tr xml:lang="ru">
                            <q>уродливый</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>ugly</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d3148e91">
                        <quote>aw, æmæ æcæg aftæ xæld, ænaqīnon adæjmag dæn?</quote>
                        <abv:tr xml:lang="ru">
                            <q>неужели я нравственный урод?</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>Am I a moral freak?</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Qazaq"/><biblScope>12</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d3148e110">
                        <quote>ræstæg ænaqīnonæj… læsydī</quote>
                        <abv:tr xml:lang="ru">
                            <q>время ползло несуразно</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>time was creeping awkwardly</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Сека"/><biblScope>95</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
