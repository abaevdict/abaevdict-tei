            <gloss><q>now</q></gloss>
            <note type="bibl"><bibl><author>Ivanow</author>, <title type="j">Rivista degli studi
                  orientali</title>
                <biblScope>1939 XVIII <hi rendition="#rend_italic">1</hi> 18,
              23</biblScope></bibl>)</note></mentioned>, <mentioned xml:lang="x-balochi"><lang/>
