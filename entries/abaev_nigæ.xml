      <entry xml:id="entry_nigæ" xml:lang="os-x-digor" abv:completeness="english-questions">
        <form xml:id="form_d5799e66" type="lemma"><orth>nigæ</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d5799e72"><abv:tr xml:lang="ru">
            <q>прибрежная полоса у реки, поросшая травой</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>grassy riverside</q>
          </abv:tr></sense>
        <note xml:lang="ru" type="comment">; по значению ср. <ref type="xr" target="#entry_astym"/>.</note>
        <note xml:lang="en" type="comment">; semantically, cf. <ref type="xr" target="#entry_astym"/></note>
        <etym xml:lang="ru">Из <mentioned xml:id="mentioned_d5799e94" xml:lang="kbd" extralang="kbd-x-crc"><lang/>
            <w>nәgä</w>
            <gloss><q>пойма реки</q></gloss>, <gloss><q>заливной луг</q></gloss></mentioned>.</etym>
        <etym xml:lang="en">From <mentioned xml:id="mentioned_d5799e108" xml:lang="kbd" extralang="kbd-x-crc"><lang/>
            <w>nәgä</w>
            <gloss><q>floodplain</q></gloss>, <gloss><q>water meadow</q></gloss></mentioned>.</etym>
      </entry>
