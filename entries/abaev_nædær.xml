      <entry xml:id="entry_nædær" xml:lang="os" abv:completeness="russian-questions">
        <form type="lemma"><orth>nædær... nædær...</orth></form>
        <sense><abv:tr xml:lang="ru">
            <q>ни... ни...</q>
          </abv:tr></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ūсу bonæj ta kænæ saxatæj nīḱī zony,
                <oRef>nædær</oRef> wælarvon zædtæ, <oRef>nædær</oRef> fyrt, fælæ aermæst fyd</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">о дне же том, или часе, никто не знает, ни
                ангелы небесные, ни сын, но только отец</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Марк"/>
              <biblScope><hi rendition="#rend_italic">13</hi> 32</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_næ_1"/> и <ref type="xr"
            target="#entry_dær"/>.</etym>
      </entry>
