    xml:id="entry_fīdyjyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>fīdyjyn</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>fedujun</orth>
        <form type="inflected" subtype="ptcp">
            <orth>fedut</orth>
        </form>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>иссякать</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>run out</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>dæ surx xwari gorn ænæ fedujgæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>твоя житница с красным зерном неиссякаема</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>your red grain barn is inexhaustible</q>
            </cit>
            <bibl>
                <ref target="#src_MD"/>
                <biblScope>1936 II 55</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>znagbæl toxgæj togvedut fæddæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>сражаясь с врагом, ты истек кровью</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>you bled out while fighting the enemy</q>
            </cit>
            <bibl>
                <ref target="#src_MD"/>
                <biblScope>1936 II 36</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>ærʒæn isfedujæn næ es</quote>
            <cit type="translation" xml:lang="ru">
                <q>природа неистощима</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>nature is inexhaustible</q>
            </cit>
            <bibl>
                <ref target="#src_SD"/>
                <biblScope>292%l3%l</biblScope>
            </bibl>
        </cit>
    </cit>
