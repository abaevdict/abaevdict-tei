      <entry xml:id="entry_toppæxst" xml:lang="os" abv:completeness="russian-questions">
        <?oxy_comment_start author="oleg" timestamp="20230302T151206+0300" comment="Russian checked"?>
        <form xml:id="form_d1815e66" type="lemma"><orth>toppæxst</orth></form><?oxy_comment_end?>
        <sense>
          <sense xml:id="sense_d1815e69">
            <abv:tr xml:lang="ru">
              <q>ружейная стрельба</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>выстрел</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>расстояние ружейного выстрела</q>
            </abv:tr>
          </sense>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0"><oRef>toppæxstæj</oRef> bīræ fyldær ærbarazæj
              bajrag</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">жеребенок опередил (других) намного больше, чем
                на ружейный выстрел</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_ИАА"/>
