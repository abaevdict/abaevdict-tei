            <entry xml:id="entry_golmanʒy" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>golmanʒy</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>golmanʒi</orth></form>
                <sense><def xml:lang="ru">название съедобной травы и приготовляемого из нее
                        кушанья</def><def xml:lang="en">the name of an edible grass and the dishes
                        prepared from it</def></sense>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="sva"><lang/>
                        <w>kwalmānč</w></mentioned> (<bibl><biblScope xml:lang="ru">Арсен %sОниан%s.
                            Сборник сванских названий деревьев и растений. Пгр.,
                            1917</biblScope><biblScope xml:lang="en">Arsen %sOnian%s. Collection of
                            Svan names of trees and plants. Pg., 1917</biblScope></bibl>).</etym>
                <etym xml:lang="en">Compare <mentioned xml:lang="sva"><lang/>
                        <w>kwalmānč</w></mentioned> (<bibl><biblScope xml:lang="ru">Арсен %sОниан%s.
                            Сборник сванских названий деревьев и растений. Пгр.,
                            1917</biblScope><biblScope xml:lang="en">Arsen %sOnian%s. Collection of
                            Svan names of trees and plants. Pg., 1917</biblScope></bibl>).</etym>
