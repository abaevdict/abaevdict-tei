<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_kæmdær" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>kæmdær</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>kæmidær</orth></form>
    <sense n="1" xml:id="sense_kæmdær.1"><cit type="translation" xml:lang="ru">
            <q>где-то</q>
        </cit><cit type="translation" xml:lang="en">
            <q>somewhere</q>
        </cit></sense>
    <sense n="2" xml:id="sense_kæmdær.2"><cit type="translation" xml:lang="ru">
            <q>оказывается</q>
        </cit><cit type="translation" xml:lang="en">
            <q>it turns out</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæmdær fesæft</quote>
            <cit type="translation" xml:lang="ru">
                <q>где-то пропал</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(he) disappeared somewhere</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>læppū kæmdær ragæj warzta Tamary</quote>
            <cit type="translation" xml:lang="ru">
                <q>юноша, оказывается, давно любил Тамару</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>it turns out that the young man has loved Tamara for a long time</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæmdær fesæft</quote>
            <cit type="translation" xml:lang="ru">
                <q>где-то пропал</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(he) disappeared somewhere</q>
            </cit>
        </cit>
    </cit>
    <cit type="exampleGroup">
