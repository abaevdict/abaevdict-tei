    <text>
        <body>
            <entry xml:id="entry_kæmdær" xml:lang="os">
                <form xml:id="form_d5624e66" xml:lang="os-x-iron" type="lemma"><orth>kæmdær</orth></form>
                <form xml:id="form_d5624e69" xml:lang="os-x-digor" type="dialectal"><orth>kæmidær</orth></form>
                <sense n="1" xml:id="sense_kæmdær.1"><abv:tr xml:lang="ru">
                        <q>где-то</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>somewhere</q>
                    </abv:tr></sense>
                <sense n="2" xml:id="sense_kæmdær.2"><abv:tr xml:lang="ru">
                        <q>оказывается</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>it turns out</q>
                    </abv:tr></sense>
                <abv:exampleGrp xml:id="exampleGrp_d5624e92">
                    <abv:example xml:id="example_d5624e94">
                        <quote>kæmdær fesæft</quote>
                        <abv:tr xml:lang="ru">
                            <q>где-то пропал</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>(he) disappeared somewhere</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <abv:exampleGrp xml:id="exampleGrp_d5624e111">
                    <abv:example xml:id="example_d5624e113">
                        <quote>læppū kæmdær ragæj warzta Tamary</quote>
                        <abv:tr xml:lang="ru">
                            <q>юноша, оказывается, давно любил Тамару</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>it turns out that the young man has loved Tamara for a long time</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">См. <ref type="xr" target="#entry_kæm"/>.</etym>
                <etym xml:lang="en">See <ref type="xr" target="#entry_kæm"/>.</etym>
            </entry>
