<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_cʼala"
    xml:lang="os">
    <form type="lemma">
        <orth>cʼala</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>долина</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>valley</q>
        </cit>
        <note type="comment" xml:lang="ru">(употребляется только в южноиронских и дигорских говорах) </note>
        <note type="comment" xml:lang="en">(in use only in South Iron and Digor dialects) </note>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>ystyr cʼala</quote>
            <cit type="translation" xml:lang="ru">
                <q>большая долина</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>big valley</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>192</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>jæ dymægyl badgæjæ cʼalamæ ærbyryd</quote>
            <cit type="translation" xml:lang="ru">
                <q>сидя он сполз в долину</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>sitting he climbed down in the valley</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>II 159</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>i сʼalaj buni me ʽrvadæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>внизу долины (находится) мой брат</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(there is) my brother below the valley</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>22</biblScope>
            </bibl>
        </cit>
    </cit>
