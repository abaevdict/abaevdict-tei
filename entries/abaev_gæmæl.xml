    <text>
        <body>
            <entry xml:id="entry_gæmæl" xml:lang="os">
                <form xml:id="form_d3703e66" type="lemma"><orth>gæmæl</orth></form>
                <sense xml:id="sense_d3703e69"><sense xml:id="sense_d3703e70"><abv:tr xml:lang="ru">
                            <q>навостривший уши</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>a person who perked up their ears</q>
                        </abv:tr></sense><sense xml:id="sense_d3703e79"><abv:tr xml:lang="ru">
                            <q>принявший боевой, воинственный вид</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>a person who took on a fighting, warlike look</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d3703e91">
                        <quote>jæxi fægæmæl kogta</quote>
                        <abv:tr xml:lang="ru">
                            <q>он принял боевой вид</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>he took on a fighting look</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Неясно.</etym>
                <etym xml:lang="en">Unclear.</etym>
            </entry>
