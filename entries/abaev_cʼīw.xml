    <text>
        <body>
            <entry xml:id="entry_cʼīw" xml:lang="os">
                <form xml:id="form_d5743e66" xml:lang="os-x-iron" type="lemma"><orth>cʼīw</orth></form>
                <form xml:id="form_d5743e69" xml:lang="os-x-digor" type="dialectal"><orth>cʼew</orth></form>
                <sense xml:id="sense_d5743e72"><sense xml:id="sense_d5743e73"><abv:tr xml:lang="ru">
                            <q>птичка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>little bird</q>
                        </abv:tr></sense><sense xml:id="sense_d5743e82"><abv:tr xml:lang="ru">
                            <q>цыпленок</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>baby chicken</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d5743e94">
                        <quote>he, nyr axst dæ, cʼīw</quote>
                        <abv:tr xml:lang="ru">
                            <q>теперь ты поймана, птичка</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>now you are caught, little bird</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Коста"/><biblScope>121</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d5743e113">
                        <quote>īwæj-īw læppyn cærgæsy karḱy cʼīw byrsy</quote>
                        <abv:tr xml:lang="ru">
                            <q>иного птенца-орла может одолеть и цыпленок</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>some baby eagle can be beaten even by a baby chicken</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_Коста"/><biblScope>107</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d5743e132" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>biccewi bafændadtæj axurgond cʼewi radavun</quote>
                        <abv:tr xml:lang="ru">
                            <q>мальчику захотелось украсть ученую птицу</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>a boy wanted to steal a learned bird</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_AK"/><biblScope>I 50</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:id="example_d5743e154" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>cʼewtæ zællang ǧæræj zaruncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>птички поют звонкими голосами</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>birds sing with clear voices</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_AK"/><biblScope>I 56</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Звукоподражательное слово (ср. <mentioned xml:id="mentioned_d5743e179" xml:lang="os"><lang/>
                        <w>cʼīw-cʼīw</w>
                        <gloss><q>щебетанье</q></gloss></mentioned>), повторяющееся, с небольшими
                    вариациями, во множестве языков: <mentioned xml:id="mentioned_d5743e187" xml:lang="ru"><lang/>
                        <w>цы́пка</w></mentioned>, <mentioned xml:id="mentioned_d5743e192" xml:lang="ru"><w>цы́вка</w></mentioned>, <mentioned xml:id="mentioned_d5743e195" xml:lang="ru"><w>че́вка</w>
                        <gloss><q>птичка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e201" xml:lang="ru"><w/>
                        <gloss><q>пташка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e208" xml:lang="ru"><w/>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e214" xml:lang="kv"><lang/>
                        <w n="0">чипан</w>
                        <gloss><q>курица</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e223" xml:lang="fi"><lang/>
                        <w>tipu</w>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e231" xml:lang="krl"><lang/>
                        <w>čiučoi</w>
                        <gloss><q>пташка</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e239" xml:lang="krl"><w/>
                        <gloss><q>воробей</q></gloss></mentioned> %b(FUF XIX 169)%b, <mentioned xml:id="mentioned_d5743e245" xml:lang="cv"><lang/>
                        <w>cipi</w></mentioned>, <mentioned xml:id="mentioned_d5743e251" xml:lang="tt-x-kazan"><lang/>
                        <w>čəbəš</w>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e259" xml:lang="tr-x-anat"><lang/>
                        <w>ǯivǯiw</w>
                        <gloss><q>птенчик</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e267" xml:lang="tr"><w/>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e273" xml:lang="tr"><w/>
                        <gloss><q>чириканье</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e279" xml:lang="fa"><lang/>
                        <w>ǰuǰa</w>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e287" xml:lang="ku"><lang/>
                        <w>cucik</w>
                        <gloss><q>воробей</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e296" xml:lang="ka"><lang/>
                        <w>cʼcʼila</w>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e304" xml:lang="ka" dialect="ka-x-imer"><lang/>
                        <w>čʼiutʼi</w></mentioned>, <mentioned xml:id="mentioned_d5743e309" xml:lang="lez"><lang/>
                        <w>cʼicʼib</w>
                        <gloss><q>цыпленок</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e317" xml:lang="bbl"><lang/>
                        <w>cʼivč</w></mentioned>, <mentioned xml:id="mentioned_d5743e322" xml:lang="krc-x-balkar"><lang/>
                        <w>čəvčək</w>
                        <gloss><q>воробей</q></gloss></mentioned> и др.</etym>
                <etym xml:lang="en">An onomatopoetic word (cf. <mentioned xml:id="mentioned_d5743e334" xml:lang="os"><lang/>
                        <w>cʼīw-cʼīw</w>
                        <gloss><q>chirruping</q></gloss></mentioned>), which is found with some
                    variations in many languages: <mentioned xml:id="mentioned_d5743e342" xml:lang="ru"><lang/>
                        <w>cýpka</w></mentioned>, <mentioned xml:id="mentioned_d5743e347" xml:lang="ru"><w>cývka</w></mentioned>,
                        <mentioned xml:id="mentioned_d5743e350" xml:lang="ru"><w>čévka</w>
                        <gloss><q>little bird</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e356" xml:lang="ru"><w/>
                        <gloss><q>birdie</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e363" xml:lang="ru"><w/>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e369" xml:lang="kv"><lang/>
                        <w>čipan</w>
                        <gloss><q>chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e377" xml:lang="fi"><lang/>
                        <w>tipu</w>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e385" xml:lang="krl"><lang/>
                        <w>čiučoi</w>
                        <gloss><q>birdie</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e393" xml:lang="krl"><w/>
                        <gloss><q>sparrow</q></gloss></mentioned> %b(FUF XIX 169)%b, <mentioned xml:id="mentioned_d5743e399" xml:lang="cv"><lang/>
                        <w>cipi</w></mentioned>, <mentioned xml:id="mentioned_d5743e405" xml:lang="tt-x-kazan"><lang/>
                        <w>čəbəš</w>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e413" xml:lang="tr-x-anat"><lang/>
                        <w>ǯivǯiw</w>
                        <gloss><q>nestling</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e421" xml:lang="tr"><w/>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e427" xml:lang="tr"><w/>
                        <gloss><q>chirping</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e433" xml:lang="fa"><lang/>
                        <w>ǰuǰa</w>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e441" xml:lang="ku"><lang/>
                        <w>cucik</w>
                        <gloss><q>sparrow</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e450" xml:lang="ka"><lang/>
                        <w>cʼcʼila</w>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e458" xml:lang="ka" dialect="ka-x-imer"><lang/>
                        <w>čʼiutʼi</w></mentioned>, <mentioned xml:id="mentioned_d5743e463" xml:lang="lez"><lang/>
                        <w>cʼicʼib</w>
                        <gloss><q>little chicken</q></gloss></mentioned>, <mentioned xml:id="mentioned_d5743e471" xml:lang="bbl"><lang/>
                        <w>cʼivč</w></mentioned>, <mentioned xml:id="mentioned_d5743e476" xml:lang="krc-x-balkar"><lang/>
                        <w>čəvčək</w>
                        <gloss><q>sparrow</q></gloss></mentioned> etc.</etym>
            </entry>
