            <entry xml:id="entry_gobat" xml:lang="os-x-digor">
                <form type="lemma"><orth>gobat</orth></form>
                <usg><lang/></usg>
                <sense xml:id="sense_d3484e72"><abv:tr xml:lang="ru">
                        <q>куча</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>heap</q>
                    </abv:tr></sense>
                <re xml:id="re_d3484e82">
                    <form type="lemma"><orth>gobat kænyn</orth></form>
                    <sense xml:id="sense_d3484e87"><abv:tr xml:lang="ru">
                            <q>окучивать</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>earth sth up</q>
                        </abv:tr></sense>
                </re>
                <etym xml:lang="ru">К группе <ref type="xr" target="#entry_gūbakk"/> и пр. См. под
                        <ref type="xr" target="#entry_tymbyl"/> ‘круглый’.</etym>
                <etym xml:lang="en">It is related to the group <ref type="xr" target="#entry_gūbakk"/> etc. See under <ref type="xr" target="#entry_tymbyl"/> ‘round’.</etym>
            </entry>
