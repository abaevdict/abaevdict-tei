    <cit type="exampleGroup">
        <cit type="example">
            <quote>styræj, ḱysylæj næ wīstæj næmync</quote>
            <cit type="translation" xml:lang="ru">
                <q>больших и малых секут розгами</q>
            </cit>
            <cit type="translation" xml:lang="en">
