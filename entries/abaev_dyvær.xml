    <form type="lemma">
        <orth>dyvær</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>двойной (по количеству)</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>double (by quantity)</q>
        </cit>
    </sense>
    <re>
        <form type="lemma">
            <orth>dyværæj</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>вдвойне</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>twice</q>
            </cit>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>jæ toppy dyvær īftyǧd awaǧta</quote>
            <cit type="translation" xml:lang="ru">
                <q>он вложил в ружье двойной заряд</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he put a double charge in the gun</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
