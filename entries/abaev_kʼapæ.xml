            <entry xml:id="entry_kʼapæ" xml:lang="os">
                <form xml:id="form_d2062e66" type="lemma"><orth>kʼapæ</orth></form>
                <sense xml:id="sense_d2062e69"><abv:tr xml:lang="ru">
                        <q>курдюк</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>fat tail</q>
                    </abv:tr></sense>
                <etym xml:lang="ru">Усвоено из языков адыгско-абхазской группы; ср. <mentioned corresp="#mentioned_d2062e103" xml:id="mentioned_d2062e81" xml:lang="kbd"><lang/>
                        <w>ḱ’apä</w></mentioned>, <mentioned corresp="#mentioned_d2062e108" xml:id="mentioned_d2062e86" xml:lang="ab"><lang/>
                        <w>a-қʼapä</w>
                        <gloss><q>курдюк</q></gloss></mentioned>, <mentioned corresp="#mentioned_d2062e116" xml:id="mentioned_d2062e94" xml:lang="ab"><w/>
                        <gloss><q>курдючное сало</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Acquired from Adyghe-Abkhaz language group; cf. <mentioned corresp="#mentioned_d2062e81" xml:id="mentioned_d2062e103" xml:lang="kbd"><lang/>
                        <w>ḱ’apä</w></mentioned>, <mentioned corresp="#mentioned_d2062e86" xml:id="mentioned_d2062e108" xml:lang="ab"><lang/>
                        <w>a-қʼapä</w>
                        <gloss><q>fat tail</q></gloss></mentioned>, <mentioned corresp="#mentioned_d2062e94" xml:id="mentioned_d2062e116" xml:lang="ab"><w/>
                        <gloss><q>fat-tail fat</q></gloss></mentioned>.</etym>
            </entry>
