            <usg type="dialect">
                <lang/>
            </usg>
            <quote>...fudænængi ke gælʒuncæ duwwæ zærdi kæræʒej</quote>
            <cit type="translation" xml:lang="ru">
                <q>...что на беду бросают друг друга два сердца</q>
            </cit>
            <cit type="translation" xml:lang="en">
