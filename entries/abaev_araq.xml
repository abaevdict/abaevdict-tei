<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_araq"
        xml:lang="os">
        <form type="lemma"><orth>araq</orth></form>
        <sense><cit type="translation" xml:lang="ru">
                        <q>водка</q>
                </cit><cit type="translation" xml:lang="en">
                        <q>vodka</q>
                </cit></sense>
        <cit type="exampleGroup">
                <cit type="example">
                        <quote>davæggag fydīmæ karz araq dædʒæn</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>он будет угощать крепкой аракой с краденым мясом</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>he will treat us with strong vodka and stolen meat</q>
                        </cit>
                        <bibl><ref target="#src_Коста"/><biblScope>65</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>araq, bægænyjæn kæron næ wydi</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>водке, пиву не было конца</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>there was no end to vodka and beer</q>
                        </cit>
                        <bibl><ref target="#src_Сека"/><biblScope>67</biblScope></bibl>
                </cit>
        </cit>
        <cit type="exampleGroup">
                <cit type="example">
                        <quote>davæggag fydīmæ karz araq dædʒæn</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>он будет угощать крепкой аракой с краденым мясом</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>he will treat us with strong vodka and stolen meat</q>
                        </cit>
                        <bibl><ref target="#src_Коста"/><biblScope>65</biblScope></bibl>
                </cit>
                <cit type="example">
                        <quote>araq, bægænyjæn kæron næ wydi</quote>
                        <cit type="translation" xml:lang="ru">
                                <q>водке, пиву не было конца</q>
                        </cit>
                        <cit type="translation" xml:lang="en">
                                <q>there was no end to vodka and beer</q>
                        </cit>
                        <bibl><ref target="#src_Сека"/><biblScope>67</biblScope></bibl>
                </cit>
        </cit>
        <etym xml:lang="ru">Из <mentioned xml:lang=""><lang/><w>ʽaraq</w></mentioned>, <mentioned
