      <entry xml:id="entry_tʼællang" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d3367e66" type="lemma"><orth>tʼællang</orth></form>
        <sense xml:id="sense_d3367e69">
          <abv:tr xml:lang="ru">
            <q>пронзительный, звонкий звук, крик</q>
          </abv:tr>
        </sense>
        <re>
          <form type="lemma"><orth>tʼællangæj zaryn</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">петь громко, звонко, во весь голос</q>
            </tr>
          </sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">X˳yppe <oRef>fætʼællang</oRef> lasta: ej
              Gino!</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">Хуппе пронзительно крикнул: эй, Гино!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_F."/>
              <biblScope>1976 VII 95</biblScope></bibl>
          </example>
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">zærdgin <oRef>tʼællangæj</oRef>, nifsgun
              zællangæj zaræ dæ fændon</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">от души, звонко, полная надежд, пой во весь
                голос</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Qazb."/>
              <biblScope>14</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Звукоподражательное (<bibl><ref type="bibl" target="#ref_ГО"/>
