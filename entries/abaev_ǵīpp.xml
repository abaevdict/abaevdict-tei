                    <q>отливать</q>
                </cit>
                <cit type="translation" xml:lang="en">
                    <q>cast (in a mould)</q>
                </cit>
            </sense>
            <sense>
                <note type="comment" xml:lang="ru">также</note>
                <note type="comment" xml:lang="en">also</note>
                <cit type="translation" xml:lang="ru">
                    <q>печатать</q>
                </cit>
                <note type="comment" xml:lang="ru">также</note>
                <note type="comment" xml:lang="en">also</note>
                <cit type="translation" xml:lang="en">
                    <q>print</q>
                </cit>
            </sense>
        </sense>
    </re>
    <re>
        <form type="lemma">
            <orth>ǵippy waǧd</orth>
        </form>
        <sense>
