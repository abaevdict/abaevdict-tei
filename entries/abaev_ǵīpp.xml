    <sense>
        <cit type="translation" xml:lang="ru">
            <q>форма для отливки чего-либо</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>cast</q>
        </cit>
    </sense>
    <re>
        <form type="lemma">
            <orth>ǵippy waʒyn</orth>
        </form>
        <sense>
            <sense>
