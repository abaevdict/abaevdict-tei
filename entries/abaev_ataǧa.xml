    <text>
        <body>
            <entry xml:id="entry_ataǧa" xml:lang="os-x-iron">
                <form type="lemma"><orth>ataǧa</orth></form>
                <usg><lang/></usg>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>прибрежный лес</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>coastal forest</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>лесистая речная долина</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>wooded river valley</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>læppu... baхaсcæ īw atagamæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>юноша достиг одной лесистой долины</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>the young man reached a wooded valley</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>III
                            60</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>Terḱy ataǧajy adærǧ lysty saǵy mærdtæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>в долине Терека распростерлись трупы оленей</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>deer corpses spread out in the valley of Terek</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>III
                            123</biblScope></bibl>
                    </abv:example>
                    <abv:example>
                        <quote>ḱī fīstægæj, q˳yzgæ-q˳yzgcæ ataǧajy fæmīdæg īs...</quote>
                        <abv:tr xml:lang="ru">
                            <q/>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>somebody climbed sneaking, on foot into the coastal forest...</q>
                        </abv:tr>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Ср. <mentioned xml:lang="inh"><lang/>
                        <w>atæghæ</w></mentioned>, <mentioned xml:lang="ce"><lang/>
                        <w>ataghi</w>
                        <gloss><q>долина</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">Cf. <mentioned xml:lang="inh"><lang/>
                        <w>atæghæ</w></mentioned>, <mentioned xml:lang="ce"><lang/>
                        <w>ataghi</w>
                        <gloss><q>valley</q></gloss></mentioned>.</etym>
            </entry>
