<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ataǧa"
    xml:lang="os-x-iron">
    <form type="lemma"><orth>ataǧa</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>прибрежный лес</q>
            </cit><cit type="translation" xml:lang="en">
                <q>coastal forest</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>лесистая речная долина</q>
            </cit><cit type="translation" xml:lang="en">
                <q>wooded river valley</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>læppu... bахасcæ īw atagamæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>юноша достиг одной лесистой долины</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the young man reached a wooded valley</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>III 60</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>Terḱy ataǧajy adærǧ lysty saǵy mærdtæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>в долине Терека распростерлись трупы оленей</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>deer corpses spread out in the valley of Terek</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>III 123</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>ḱī fīstægæj, q˳yzgæ-q˳yzgcæ ataǧajy fæmīdæg īs...</quote>
            <cit type="translation" xml:lang="ru">
                <q/>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>somebody climbed sneaking, on foot into the coastal forest...</q>
            </cit>
        </cit>
    </cit>
