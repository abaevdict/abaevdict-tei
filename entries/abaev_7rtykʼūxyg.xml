      <entry xml:id="entry_7rtykʼūxyg" xml:lang="os" abv:completeness="questions">
        <form type="lemma" xml:lang="os-x-iron"><orth>ᵆrtykʼūxyg</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>ærtikʼoxug</orth></form>
        <sense><abv:tr xml:lang="ru">
            <q>трехпалый</q>
          </abv:tr></sense>
        <re>
          <form type="lemma"><orth>ᵆrtykʼūxyg sagoj</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">трехпалые вилы</q>
            </tr></sense>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">...cuppærkʼoxug æfsæjnag sagænttæ,
                <oRef>ærtikʼoxgutæ</oRef>...</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">...четырехпалые железные вилы, трехпалые...</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_SD"/>
              <biblScope>245<hi rendition="#rend_subscript">2</hi></biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Сложение из <ref type="xr" target="#entry_7rtæ"><m xml:lang="os-x-iron"
              >ᵆrty-</m>
            <?oxy_comment_start author="Irene" timestamp="20220115T140746+0300" comment="NB поставила палку сама, она не поставилась автоматически"?>|<?oxy_comment_end?>
            <m xml:lang="os-x-digor">ærti-</m>
            <gloss><q>три</q></gloss></ref>  и <ref type="xr" target="#entry_kʼūx"><w
              xml:lang="os-x-iron">kʼūx</w><w xml:lang="os-x-digor">kʼox</w>
            <gloss><q>рука</q></gloss>, <gloss><q>палец</q></gloss></ref>, оформленное суффиксом <m
            xml:lang="os-x-iron">-yg</m>
          <?oxy_comment_start author="Irene" timestamp="20220115T140757+0300" comment="здесь тоже"?>|<?oxy_comment_end?>
