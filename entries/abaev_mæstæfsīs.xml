      <entry xml:id="entry_mæstæfsīs" xml:lang="os" abv:completeness="complete">
        <form xml:id="form_d2968e66" type="lemma" xml:lang="os-x-iron"><orth>mæstæfsīs</orth></form>
        <form xml:id="form_d2968e69" xml:lang="os-x-digor" type="dialectal"><orth>mæstæfses</orth></form>
        <sense xml:id="sense_d2968e72"><abv:tr xml:lang="ru">
            <q>пресыщенность горем</q>
          </abv:tr><abv:tr xml:lang="en">
            <q>satiety with grief</q>
          </abv:tr></sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example xml:id="example_d2968e84" xml:lang="os-x-digor">
            <usg xmlns="http://www.tei-c.org/ns/1.0"><lang/></usg>
            <quote xmlns="http://www.tei-c.org/ns/1.0">zawwat zærdi <oRef>mæstæfsesæj</oRef>
              issirdta</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">он нашел несчастное сердце пресыщенным
                горем</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">he found the unhappy heart satiated with
                grief</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Qazb."/>
              <biblScope>69</biblScope></bibl>
          </example>
        </exampleGrp>
        <note xml:lang="ru" type="comment">. — См. <ref type="xr" target="#entry_mæstælǧæd"/></note>
        <note xml:lang="en" type="comment">. — See <ref type="xr" target="#entry_mæstælǧæd"/></note>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_mast"/> и <ref type="xr" target="#entry_7fsīs"><w>fsīs</w></ref>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_mast"/> and <ref type="xr" target="#entry_7fsīs"><w>fsīs</w></ref>.</etym>
