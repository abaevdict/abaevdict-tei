            <entry xml:id="entry_ʒækkor" xml:lang="os-x-iron">
                <form type="lemma"><orth>ʒækkor</orth></form>
                <usg><lang/></usg>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>деревянный молот</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>wood hammer</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>колотушка</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>beater</q>
                        </abv:tr></sense></sense>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>axæssūt æfsæn ʒækkor æmæ jyn ʒy jæ fynʒy ærǧīwtæ cævut</quote>
                        <abv:tr xml:lang="ru">
                            <q>возьмите железную колотушку и бейте по хрящам его носа</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>take an iron beater and hit the cartilage of his nose</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_ЮОПам."/><biblScope>I
                            107</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Из <mentioned xml:lang="mn"><lang/>
                        <w>tsokūr</w>
                        <gloss><q>молот</q></gloss></mentioned>, <mentioned xml:lang="mn"><w/>
                        <gloss><q>колотушка</q></gloss></mentioned> от <mentioned xml:lang="mn"><lang/>
                        <w>čoki</w>
                        <gloss><q>ударять</q></gloss></mentioned>.</etym>
                <etym xml:lang="en">From <mentioned xml:lang="mn"><lang/>
                        <w>tsokūr</w>
                        <gloss><q>hammer</q></gloss></mentioned>, <mentioned xml:lang="mn"><w/>
                        <gloss><q>beater</q></gloss></mentioned> from <mentioned xml:lang="mn"><lang/>
                        <w>čoki</w>
                        <gloss><q>beat</q></gloss></mentioned>.</etym>
            </entry>
