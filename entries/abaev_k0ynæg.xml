    <text>
        <body>
            <entry xml:id="entry_k0ynæg" xml:lang="os">
                <form xml:lang="os-x-iron" type="lemma"><orth>k˳ynæg</orth></form>
                <form xml:lang="os-x-digor" type="dialectal"><orth>kunæg</orth></form>
                <sense><sense><abv:tr xml:lang="ru">
                            <q>малый (по количеству)</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>small (quantitatively)</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>мало</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>little</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>скудный</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>scarce</q>
                        </abv:tr></sense><sense><abv:tr xml:lang="ru">
                            <q>оскудевший</q>
                        </abv:tr></sense></sense>
                <re>
                    <re>
                        <form type="lemma"><orth>k˳ynægmæ cæwyn</orth></form>
                        <sense><abv:tr xml:lang="ru">
                                <q>оскудевать</q>
                            </abv:tr><abv:tr xml:lang="en">
                                <q>grow scarce</q>
                            </abv:tr></sense>
                    </re>
                    <re>
                        <form type="lemma"><orth>ysk˳ynæg wyn</orth></form>
                        <sense><sense><abv:tr xml:lang="ru">
                                    <q>оскудеть</q>
                                </abv:tr><abv:tr xml:lang="en">
                                    <q>become poor</q>
                                </abv:tr></sense><sense><abv:tr xml:lang="ru">
                                    <q>сойти на нет</q>
                                </abv:tr><abv:tr xml:lang="en">
                                    <q>come to naught</q>
                                </abv:tr></sense></sense>
                    </re>
                </re>
                <abv:exampleGrp>
                    <abv:example>
                        <quote>k˳ynæg mæsty ne systy ævsymærtæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>братья разгневались немало</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>the brothers were angry a lot</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>III
                            101</biblScope></bibl>
                    </abv:example>
                    <abv:example xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>qal læqwæntæ kunæg estitæ næ fæzzaruncæ</quote>
                        <abv:tr xml:lang="ru">
                            <q>мало ли что поют спесивые молодые люди</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>you never know what arrogant young people sing</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_СОПам."/><biblScope>II
                            33</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Закономерно восходит к иран. %i*kavna-ka%i; ср. <mentioned
                        xml:lang="sog"><lang/>
                        <w>*kaβn-</w><note type="comment" xml:lang="ru"> (%ikβn-%i, %ikβny-kβny%i
                            ‘мало-помалу’)</note><note type="comment" xml:lang="en"> (%ikβn-%i,
                            %ikβny-kβny%i ‘little by little’)</note></mentioned>, <mentioned
                        xml:lang="sog"><w>*kaβnak</w></mentioned> (<mentioned xml:lang="sog-x-chr"><lang/>
                        <w>qbnq</w></mentioned>), <mentioned xml:lang="ps"><lang/>
                        <w>konkai</w></mentioned>, <mentioned xml:lang="fa"><lang/>
                        <w>kam</w></mentioned>, <mentioned xml:lang="ae"><lang/>
                        <w>kamna-</w>
                        <gloss><q>малый</q></gloss></mentioned>.%n<bibl><author>Morgenstierne</author><ref
                            type="bibl" target="#ref_NTS"/><biblScope>XII 267</biblScope></bibl>. —
                            <bibl><ref type="bibl" target="#ref_Bailey._Asica"
                            /><biblScope>23</biblScope></bibl>.</etym>
                <etym xml:lang="en">It regularly originated from %i*kavna-ka%i; cf. <mentioned
                        xml:lang="sog"><lang/>
                        <w>*kaβn-</w><note type="comment" xml:lang="ru"> (%ikβn-%i, %ikβny-kβny%i
                            ‘мало-помалу’)</note><note type="comment" xml:lang="en"> (%ikβn-%i,
                            %ikβny-kβny%i ‘little by little’)</note></mentioned>, <mentioned
                        xml:lang="sog"><w>*kaβnak</w></mentioned> (<mentioned xml:lang="sog-x-chr"><lang/>
                        <w>qbnq</w></mentioned>), <mentioned xml:lang="ps"><lang/>
                        <w>konkai</w></mentioned>, <mentioned xml:lang="fa"><lang/>
                        <w>kam</w></mentioned>, <mentioned xml:lang="ae"><lang/>
                        <w>kamna-</w>
                        <gloss><q>little</q></gloss></mentioned>.%n<bibl><author>Morgenstierne</author><ref
                            type="bibl" target="#ref_NTS"/><biblScope>XII 267</biblScope></bibl>. —
                            <bibl><ref type="bibl" target="#ref_Bailey._Asica"
                            /><biblScope>23</biblScope></bibl>.</etym>
            </entry>
