            <entry xml:id="entry_bez" xml:lang="os">
                <form xml:id="form_d5562e66" xml:lang="os-x-iron" type="lemma"><orth>bez</orth><form xml:id="form_d5562e68" type="variant"><orth>bīʒ</orth></form></form>
                <form xml:id="form_d5562e71" xml:lang="os-x-digor" type="dialectal"><orth>begæ</orth></form>
                <sense xml:id="sense_d5562e74"><abv:tr xml:lang="ru">
                        <q>вощина</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>beeswax foundation</q>
                    </abv:tr></sense>
                <abv:exampleGrp xml:id="exampleGrp_d5562e84">
                    <abv:example xml:id="example_d5562e86" xml:lang="os-x-digor">
                        <usg><lang/></usg>
                        <quote>medægæj si rænǧitæj ævard adtæncæ ramkitæ, beʒæ æma mudæj se
                            ʽʒag</quote>
                        <abv:tr xml:lang="ru">
                            <q>внутри (улья) были помещены рамки, полные вощины и меда</q>
                        </abv:tr>
                        <abv:tr xml:lang="en">
                            <q>frames full of wax and honey were placed inside (the hive)</q>
                        </abv:tr>
                        <bibl><ref type="bibl" target="#src_AK"/><biblScope>III
                            66</biblScope></bibl>
                    </abv:example>
                </abv:exampleGrp>
                <etym xml:lang="ru">Этимология не известна.</etym>
                <etym xml:lang="en">The etymology is not known.</etym>
