    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>bez</orth><form type="variant"
            ><orth>bīʒ</orth></form></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>begæ</orth></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>вощина</q>
        </cit><cit type="translation" xml:lang="en">
            <q>beeswax foundation</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>medægæj si rænǧitæj ævard adtæncæ ramkitæ, beʒæ æma mudæj se ʽʒag</quote>
            <cit type="translation" xml:lang="ru">
                <q>внутри (улья) были помещены рамки, полные вощины и меда</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>frames full of wax and honey were placed inside (the hive)</q>
            </cit>
            <bibl><ref target="#src_AK"/><biblScope>III 66</biblScope></bibl>
        </cit>
    </cit>
