      <entry xml:id="entry_wælædaræn" xml:lang="os" abv:completeness="incomplete">
        <form xml:id="form_d4931e66" type="lemma" xml:lang="os-x-iron"><orth>wælædaræn</orth></form>
        <form xml:lang="os-x-digor" type="dialectal"><orth>wæledaræn</orth></form>
        <re>
          <note type="comment">в сочетании</note>
          <form type="lemma"><orth>wælædaræn nymæt</orth></form>
          <sense>
            <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">бурка</q>
            </tr>
          </sense>
          <re>
            <note type="comment"> , в отличие от</note>
            <form type="lemma" xml:lang="os-x-iron"><orth>binag <note type="comment"
                >(nymæt)</note></orth></form>
            <form xml:lang="os-x-digor" type="dialectal"><orth>bujnag</orth></form>
            <sense>
              <tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">войлок</q>
              </tr>
            </sense>
          </re>
        </re>
        <note type="comment">; ср. <ref type="xr" target="#entry_wæjlag">wæjlag</ref><note
            type="comment">(nymæt)</note></note>
        <abv:exampleGrp xml:id="exampleGrp_d4931e75">
          <abv:example xml:id="example_d4931e77">
            <quote>K˳yrttatæ sæ toppyxos wælædaræn nymættyl ærkodtoj</quote>
            <abv:tr xml:lang="ru">
              <q>куртатинцы насыпали свой порох на бурки</q>
            </abv:tr>
            <bibl><ref type="bibl" target="#src_ИАА"/><biblScope>III 248</biblScope></bibl>
          </abv:example>
          <example xmlns="http://ossetic-studies.org/ns/abaevdict">
            <quote xmlns="http://www.tei-c.org/ns/1.0">zydtoj ittæg xorz tynwafyn, wælædaræn nymættæ
              kænyn</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0"><note type="comment">(осетинки)</note> очень
                хорошо умели ткать сукно, изготовлять бурки</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Čerm."/>
              <biblScope>92</biblScope></bibl>
          </example>
        </abv:exampleGrp>
        <etym xml:lang="ru">Сложение из <ref type="xr" target="#entry_wælæ">wælæ</ref> и образования
          на -<m>æn</m> от <ref type="xr" target="#entry_daryn">daryn</ref>
          <gloss><q>носить</q></gloss>
          <note type="comment">(об одежде)</note><bibl><ref type="bibl" target="#ref_ГО"
              /><biblScope>§ 170 <hi>1</hi></biblScope></bibl>. </etym>
      </entry>
    </body>
