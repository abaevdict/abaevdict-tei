<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_gogon"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>gogon</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>gogojnæ</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>высокий металлический кувшин для воды</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>tall metal jug for water</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>qæwæj racæwy zargæ īw ḱyzg, gogon jæ congyl</quote>
            <cit type="translation" xml:lang="ru">
                <q>из аула идет напевая одна девушка, на руке у нее %sgogon%s</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one girl is coming from the aul, singing, a %sgogon%s is on her arm</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>IV 28</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>mæjroxs felawuj æ ors gogojnæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>лунный свет льется из ее белого %sgogon%s'а</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>moonlight is pouring out of her white %sgogon%s</q>
            </cit>
            <bibl>
                <ref target="#src_ОЭп."/>
                <biblScope>54%l11%l</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>qæwæj racæwy zargæ īw ḱyzg, gogon jæ congyl</quote>
            <cit type="translation" xml:lang="ru">
                <q>из аула идет напевая одна девушка, на руке у нее %sgogon%s</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>one girl is coming from the aul, singing, a %sgogon%s is on her arm</q>
            </cit>
            <bibl>
                <ref target="#src_СОПам."/>
                <biblScope>IV 28</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>mæjroxs felawuj æ ors gogojnæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>лунный свет льется из ее белого %sgogon%s'а</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>moonlight is pouring out of her white %sgogon%s</q>
            </cit>
            <bibl>
                <ref target="#src_ОЭп."/>
                <biblScope>54%l11%l</biblScope>
            </bibl>
        </cit>
    </cit>
    <etym xml:lang="ru">Из <mentioned xml:lang="trk"><lang/><w>gügün</w><gloss><q>медный сосуд для
                    воды</q></gloss></mentioned> (<bibl><ref target="#ref_Радлов"/><biblScope>II
                1633</biblScope></bibl>); ср. <mentioned xml:lang="krc-x-balkar"
            ><lang/><w>gögän</w></mentioned>, <mentioned xml:lang="ady"
            ><lang/><w>gogon</w></mentioned>, <mentioned xml:lang="av"><lang/><w>gurgun,
            gulgun</w></mentioned>, <mentioned xml:lang="lbe"><lang/><w>gungume</w><gloss><q>медный
                    кувшин для воды</q></gloss></mentioned> и др.</etym>
    <etym xml:lang="en">From <mentioned xml:lang="trk"><lang/><w>gügün</w><gloss><q>copper vessel
                    for water</q></gloss></mentioned> (<bibl><ref target="#ref_Радлов"
                /><biblScope>II 1633</biblScope></bibl>); compare <mentioned xml:lang="krc-x-balkar"
                ><lang/><w>gögän</w></mentioned>, <mentioned xml:lang="ady"
            ><lang/><w>gogon</w></mentioned>, <mentioned xml:lang="av"><lang/><w>gurgun,
            gulgun</w></mentioned>, <mentioned xml:lang="lbe"><lang/><w>gungume</w><gloss><q>copper
                    water jug</q></gloss></mentioned> and others.</etym>
</entry>
