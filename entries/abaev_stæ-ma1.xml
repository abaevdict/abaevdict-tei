      <entry xml:id="entry_stæ-ma1" xml:lang="os-x-iron" abv:completeness="incomplete">
        <form xml:id="form_d4444e66" type="lemma"><orth>stæ-ma!</orth></form>
        <usg><lang/></usg>
        <sense>
          <sense xml:id="sense_d4444e69">
            <abv:tr xml:lang="ru">
              <q>прочь!</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>пусти!</q>
            </abv:tr>
          </sense>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ciu cy? <oRef>stæ-ma!</oRef></quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">в чем дело? пустите!</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_Брит."/>
              <biblScope>46</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Вероятно, из <ref type="xr" target="#entry_æddæ"/>-<w>ma</w> (<mentioned
