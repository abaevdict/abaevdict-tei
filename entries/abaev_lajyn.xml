      <entry xml:id="entry_lajyn" xml:lang="os-x-south">
        <form xml:id="form_d2097e66" type="lemma"><orth>lajyn</orth></form>
        <usg><lang/></usg>
        <sense xml:id="sense_d2097e72"><def xml:lang="ru">название крашеной (хлопчатобумажной?) ткани</def><def xml:lang="en">name of a painted (cotton?) fabric</def></sense>
        <etym xml:lang="ru">Из <mentioned corresp="#mentioned_d2097e97" xml:id="mentioned_d2097e80" xml:lang="ka"><lang/><w>laini</w><gloss><q>крашеный
                холст</q></gloss></mentioned> (к <mentioned corresp="#mentioned_d2097e105" xml:id="mentioned_d2097e86" xml:lang="ar"><lang/><w>laīn</w><gloss><q>гладкий</q>,
          <q>мягкий</q></gloss></mentioned>?).</etym>
        <etym xml:lang="en">From <mentioned corresp="#mentioned_d2097e80" xml:id="mentioned_d2097e97" xml:lang="ka"><lang/>
            <w>laini</w>
            <gloss><q>painted canvas</q></gloss></mentioned> (to <mentioned corresp="#mentioned_d2097e86" xml:id="mentioned_d2097e105" xml:lang="ar"><lang/>
            <w>laīn</w>
            <gloss><q>smooth</q>, <q>soft</q></gloss></mentioned>?).</etym>
