      <entry xml:id="entry_xædræbyn" xml:lang="os" abv:completeness="incomplete">
        <?oxy_comment_start author="oleg" timestamp="20231024T190807+0300" comment="Russian checked"?>
        <form xml:id="form_d5117e66" type="lemma" xml:lang="os-x-iron"><orth>xædræbyn</orth></form><?oxy_comment_end?>
        <form xml:lang="os-x-digor" type="dialectal"><orth>xwædræbun</orth></form>
        <sense>
          <sense xml:id="sense_d5117e69">
            <abv:tr xml:lang="ru">
              <q>вплотную</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>up close</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>у подножия (горы)</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>at the foot of</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>у стены</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>at the wall</q>
            </abv:tr>
          </sense>
        </sense>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">ajnæg k’æʒæxy <oRef>xædræbyn</oRef> zyny qæw,
              jæ nom — K’ob</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">у самого подножия отвесной скалы виден аул,
                зовется он Коб</q>
            </tr>
            <tr xml:lang="en">
              <q xmlns="http://www.tei-c.org/ns/1.0">At the very foot of the steep cliff you can see
                an aul called Kob</q>
            </tr>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">См. <ref type="xr" target="#entry_xæd-"/> и <ref type="xr"
            target="#entry_ræbyn"/>.</etym>
        <etym xml:lang="en">See <ref type="xr" target="#entry_xæd-"/> and <ref type="xr"
            target="#entry_ræbyn"/>.</etym>
      </entry>
