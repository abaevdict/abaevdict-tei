<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_dydaǧ"
    xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>dydaǧ</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>dudaǧ</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>сложенный вдвое</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>double folded</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæmændær jæ bændæn īw daǧæj ne ʽxxæssydi æmæ jæ dydaǧ kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>у кого-то веревки не хватало в одинарном виде (несложенной), а он ее складывал
                    вдвое</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>someone lacked the rope in a single form (unfolded), but he folded it in half</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>III 211</biblScope>
            </bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kæmændær jæ bændæn īw daǧæj ne ʽxxæssydi æmæ jæ dydaǧ kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>у кого-то веревки не хватало в одинарном виде (несложенной), а он ее складывал
                    вдвое</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>someone lacked the rope in a single form (unfolded), but he folded it in half</q>
            </cit>
            <bibl>
                <ref target="#src_ЮОПам."/>
                <biblScope>III 211</biblScope>
            </bibl>
        </cit>
    </cit>
