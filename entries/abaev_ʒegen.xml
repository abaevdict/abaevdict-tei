<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ʒegen"
    xml:lang="os-x-digor">
    <form type="lemma"><orth>ʒegen</orth><form type="variant"><orth>ʒegejnæ</orth></form></form>
    <sense><cit type="translation" xml:lang="ru">
            <q>тростниковая циновка</q>
        </cit><cit type="translation" xml:lang="en">
            <q>reed mat</q>
        </cit></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>kizgæmæ baxastoncæ i biccewi... æma ʽj ʒegenbæl islæwwun kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>мальчика принесли к девушке, и она поставила его на циновку</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the boy was brought to the girl, and she put him on the mat</q>
            </cit>
            <bibl><ref target="#src_СОПам."/><biblScope>II</biblScope></bibl>
        </cit>
    </cit>
    <cit type="exampleGroup">
        <cit type="example">
