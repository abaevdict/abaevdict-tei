      <entry xml:id="entry_noǧaj" xml:lang="os" abv:completeness="russian-questions">
        <form type="lemma"><orth>noǧaj</orth></form>
        <sense><abv:tr xml:lang="ru">
            <q>ногайцы</q>
          </abv:tr></sense>
        <re>
          <form type="lemma"><orth>noǧajag</orth></form>
          <sense><sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
                <q xmlns="http://www.tei-c.org/ns/1.0">ногайский</q>
              </tr></sense><sense><abv:tr xml:lang="ru">
                <q>ногаец</q>
              </abv:tr></sense></sense>
        </re>
        <re>
          <form type="lemma"><orth>saw noǧaj</orth></form>
          <sense><tr xmlns="http://ossetic-studies.org/ns/abaevdict" xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">караногайцы</q>
            </tr></sense>
          <note type="comment"> (<q rendition="#rend_doublequotes">черные ногайцы</q>)</note>
        </re>
        <exampleGrp xmlns="http://ossetic-studies.org/ns/abaevdict">
          <example>
            <quote xmlns="http://www.tei-c.org/ns/1.0">wæzdan læg пe ʽssærdtoj kūsǵytæ Satanajæn,
              fælæ jæm īw dombaj aræztytæ saw <oRef>noǧajaǵy</oRef> ʽrbax˳ydtoj</quote>
            <tr xml:lang="ru">
              <q xmlns="http://www.tei-c.org/ns/1.0">благородного мужчину не нашли работники для
                Шатаны, но пригласили к ней одного богатырски сложенного караногайца</q>
            </tr>
            <bibl xmlns="http://www.tei-c.org/ns/1.0"><ref type="bibl" target="#src_СОПам."/>
              <biblScope>III 11</biblScope></bibl>
          </example>
        </exampleGrp>
        <etym xml:lang="ru">Из <mentioned xml:lang="trk"><lang/>
            <w>noγai</w>
            <gloss><q>ногаец</q></gloss><note type="bibl"> (<bibl><ref type="bibl"
                  target="#ref_Радлов"/>
                <biblScope>III 693</biblScope></bibl>)</note></mentioned>. В дальнейшем ср.
            <mentioned xml:lang="mn"><lang/>
            <w>noxoi</w></mentioned>, <mentioned xml:lang="xal"><lang/>
            <w>noqai</w>
            <gloss><q>собака</q></gloss><note type="bibl"> (<bibl><ref type="bibl"
                  target="#ref_Ramstedt"/>
                <biblScope>278</biblScope></bibl>)</note></mentioned>.</etym>
