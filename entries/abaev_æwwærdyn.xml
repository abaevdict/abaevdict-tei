<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_æwwærdyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>æwwærdyn</orth>
        <form type="inflected" subtype="ptcp">
            <orth>æwwærst</orth>
        </form>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>æwwærdun</orth>
        <form type="inflected" subtype="ptcp">
            <orth>æwwarst</orth>
        </form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>мять</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>crumple</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>растирать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>pound</q>
            </cit>
        </sense>
    </sense>
    <re>
        <form type="lemma">
            <orth>bæx æwwærdyn</orth>
        </form>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>тренировать лошадь</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to train a horse</q>
            </cit>
        </sense>
    </re>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>mæ kærc nyqqæbær, awwærdyn æj qæwy</quote>
            <cit type="translation" xml:lang="ru">
                <q>моя шуба затвердела, надо ее помять</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my furcoat has become hard, I need to squeeze it</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>Buri pysyratæj jæ kʼūxy ʒag ratydta, je ʽrtty sæ awwærsta</quote>
            <cit type="translation" xml:lang="ru">
                <q>Бури сорвал полную горсть крапивы, помял ее между ладонями</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Buri picked up a handfull of nettle, squeezed it in palms</q>
            </cit>
            <bibl>
                <ref target="#src_F."/>
                <biblScope>1957 III 26</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>don raīvyld, dūrtæ æwwærdy</quote>
            <cit type="translation" xml:lang="ru">
                <q>река разлилась, катает камни</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the river has overflowed, it is rolling stones</q>
            </cit>
            <bibl>
                <ref target="#src_F."/>
                <biblScope>1957 III 30</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>twallag ta dūǧmæ æwwærdʒæn jæ bæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>туалец опять будет тренировать к скачкам своего коня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the dval will train his horse for the races again</q>
            </cit>
            <bibl>
                <ref target="#src_Bar."/>
                <biblScope>75</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ævæǵiaw sæwwærsta jæ bæx</quote>
            <cit type="translation" xml:lang="ru">
                <q>он отлично вытренировал своего коня</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>he trained his horse perfectly</q>
            </cit>
            <bibl>
                <ref target="#src_Čerm."/>
                <biblScope>148</biblScope>
            </bibl>
        </cit>
    </cit>
