<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ḱʼīrppa" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>ḱʼīrppa</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>kʼerppa</orth><form type="variant"
                >(<orth>kʼeppa?</orth>)</form></form>
    <sense n="1" xml:id="sense_ḱʼīrppa.1"><cit type="translation" xml:lang="ru">
            <q>подхвостник к ослиному вьючному седлу</q>
        </cit><cit type="translation" xml:lang="en">
            <q>crupper for a donkey pack-saddle</q>
        </cit></sense>
    <sense n="2" xml:id="sense_ḱʼīrppa.2" xml:lang="os-x-digor"><usg><lang/></usg><cit
            type="translation" xml:lang="ru">
            <q>намордник из прутьев, надеваемый на теленка, чтобы он не сосал</q>
        </cit><cit type="translation" xml:lang="en">
            <q>a muzzle made of twigs, which is put on a calf so that it does not suck</q>
        </cit></sense>
    <sense n="3" xml:id="sense_ḱʼīrppa.3"><sense><cit type="translation" xml:lang="ru">
                <q>нарост на теле</q>
            </cit><cit type="translation" xml:lang="en">
                <q>body tumour</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>болячка</q>
            </cit><note type="comment" xml:lang=""> ()</note><note type="comment" xml:lang="">
                ()</note><cit type="translation" xml:lang="en">
                <q>sore</q>
            </cit><note type="comment" xml:lang=""> ()</note><note type="comment" xml:lang="">
                ()</note></sense></sense>
    <sense><cit type="translation" xml:lang="ru">
            <q>затвердевший на лице пот c грязью</q>
        </cit><note type="comment" xml:lang=""> ()</note><note type="comment" xml:lang="">
            ()</note><cit type="translation" xml:lang="en">
            <q>sweat and dirt hardened on the face</q>
        </cit><note type="comment" xml:lang=""> ()</note><note type="comment" xml:lang="">
        ()</note></sense>
    <cit type="exampleGroup">
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect"><lang/></usg>
            <quote>walæ xonxi ædkʼerppatæ xæræg</quote>
            <cit type="translation" xml:lang="ru">
                <q>вот на горе осел с подхвостником</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>On the mountain there is a donkey with a crupper</q>
            </cit>
            <bibl><note type="comment" xml:lang=""/><note type="comment" xml:lang=""/><ref
                    target="#src_СОПам."/><biblScope>II 176</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>Xasæqomæ xærægy ḱʼīrppatæm yssydtæn</quote>
            <cit type="translation" xml:lang="ru">
                <q>я пошел к Хасако за ослиным подхвостником</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>I went to Khasako to get a donkey crupper</q>
            </cit>
            <bibl><ref target="#src_Брит."/><biblScope>134</biblScope></bibl>
        </cit>
    </cit>
