    xml:id="entry_daǧytʼajlag" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma"><orth>daǧytʼajlag</orth></form>
    <form xml:lang="os-x-digor" type="dialectal"><orth>daǧitʼajlag</orth></form>
    <sense><sense><cit type="translation" xml:lang="ru">
                <q>бедственный</q>
            </cit><cit type="translation" xml:lang="en">
                <q>disastrous</q>
            </cit></sense><sense><cit type="translation" xml:lang="ru">
                <q>гиблый</q>
            </cit><cit type="translation" xml:lang="en">
                <q>ruinous</q>
            </cit></sense></sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>Mæxæ mæt æmæ Morat ba fæwuncæ fud daǧitʼajlagi rasug</quote>
            <cit type="translation" xml:lang="ru">
                <q>Махамат и Морат бывают погибельно пьяны</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Mahamat and Morat happen to be drunk to death</q>
            </cit>
            <bibl><ref target="#src_SD"/><biblScope>286%l3%l</biblScope></bibl>
        </cit>
        <cit type="example">
            <quote>k˳yst daǧytʼajlagmæ ærtardta</quote>
            <cit type="translation" xml:lang="ru">
                <q>довел работу до бедственного состояния</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[he] brought work to a disastrous state</q>
            </cit>
        </cit>
    </cit>
