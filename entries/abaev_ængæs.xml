<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0" xml:id="entry_ængæs"
    xml:lang="os">
    <form type="lemma">
        <orth>ængæs</orth>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>похожий</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>ressembling</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>хорошо выглядящий</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>good looking</q>
            </cit>
        </sense>
    </sense>
    <sense xml:lang="os-x-digor">
        <usg>
            <lang/>
        </usg>
        <cit type="translation" xml:lang="ru">
            <q>присмотр</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>supervision</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>асу læppū jæ fydy ʽngæs ū</quote>
            <cit type="translation" xml:lang="ru">
                <q>этот мальчик похож на своего отца</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>this boy looks like his father</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>næ rynḱyn ængæsdær ū</quote>
            <cit type="translation" xml:lang="ru">
                <q>наш больной лучше выглядит</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>our patient is looking better</q>
            </cit>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>дDzambolat doxturi xwasti fæste fengæsændær æj</quote>
            <cit type="translation" xml:lang="ru">
                <q>Дзамболат после лекарств доктора заметно оправился</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Dzambolat has got much better after taking doctor’s medicine</q>
            </cit>
            <bibl>
                <ref target="#src_Bes._Ka_ke"/>
                <biblScope>28</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>suvællænttæmæ ængæs dard пæ jes</quote>
            <cit type="translation" xml:lang="ru">
                <q>за детьми нет присмотра</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>children are left unattended</q>
            </cit>
            <bibl>
                <ref target="#src_AK"/>
                <biblScope>I 117</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ængæsdaræg</quote>
            <cit type="translation" xml:lang="ru">
                <q>наблюдатель</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>an observer</q>
            </cit>
            <bibl>
                <ref target="#src_SD"/>
                <biblScope>266%l4%l) </biblScope>
            </bibl>
        </cit>
    </cit>
