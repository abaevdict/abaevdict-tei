              <q>жасмин</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>jasmine</q>
            </abv:tr>
          </sense>
          <sense>
            <abv:tr xml:lang="ru">
              <q>Jasminum fruticans</q>
            </abv:tr>
            <abv:tr xml:lang="en">
              <q>Jasminum fruticans</q>
