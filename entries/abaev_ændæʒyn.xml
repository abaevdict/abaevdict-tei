<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_ændæʒyn" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>ændæʒyn</orth>
        <form type="inflected" subtype="ptcp">
            <orth>ændæǧd</orth>
        </form>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>ændæʒun</orth>
        <form type="inflected" subtype="ptcp">
            <orth>ændaǧd</orth>
        </form>
    </form>
    <sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>прилипать</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to stick (intr.)</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>слипаться</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to stick together (intr.)</q>
            </cit>
        </sense>
        <sense>
            <cit type="translation" xml:lang="ru">
                <q>скрепляться</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>to be attached together</q>
            </cit>
        </sense>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>xyssæ aryngyl nyndæǧdī</quote>
            <cit type="translation" xml:lang="ru">
                <q>тесто прилипло к корыту</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>dough is stuck to the tub</q>
            </cit>
        </cit>
        <cit type="example">
            <quote>Ʒimseræn jæ cæst ḱyzgyl andæǧd, jæ zærdæ artaw yssyǧdīs</quote>
            <cit type="translation" xml:lang="ru">
                <q>взгляд Дзимсера упал на девушку, сердце его загорелось огнем</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>Dzimser’s glance fell upon a girl, his heart caught fire</q>
            </cit>
            <bibl>
                <ref target="#src_Сека"/>
                <biblScope>20</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>(ræsuǧd) stadæj zænxi ænæ baz je ʽndærǧæ ændaǧdæj</quote>
            <cit type="translation" xml:lang="ru">
                <q>(красавица) лежала на земле без подушки, распростертая во весь рост</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>(the beautiful woman) stretched to her full height was lying on the ground
                    without a pillow</q>
            </cit>
            <bibl>
                <ref target="#src_Gurdž._Saxi_ræs."/>
                <biblScope>15</biblScope>
            </bibl>
        </cit>
    </cit>
