            <entry xml:id="entry_kʼori" xml:lang="os">
                <form xml:id="form_d143e66" type="lemma"><orth>kʼori</orth></form>
                <sense xml:id="sense_d143e69"><abv:tr xml:lang="ru">
                        <q>шар</q>
                    </abv:tr><abv:tr xml:lang="en">
                        <q>ball</q>
                    </abv:tr></sense>
                <re xml:id="re_d143e79">
                    <form xml:id="form_d143e81" type="lemma"><orth>zæxxy kʼori</orth></form>
                    <sense xml:id="sense_d143e84"><abv:tr xml:lang="ru">
                            <q>земной шар</q>
                        </abv:tr><abv:tr xml:lang="en">
                            <q>terrestrial globe</q>
                        </abv:tr></sense>
                </re>
                <abv:exampleGrp>
                    <abv:example xml:id="example_d143e97">
                        <quote>kʼorijy x˳yzæn tymbyl læppū</quote>
                        <abv:tr xml:lang="ru">
