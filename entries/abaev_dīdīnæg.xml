<?xml version="1.0" encoding="UTF-8"?>
<entry xmlns:functx="http://www.functx.com" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="entry_dīdīnæg" xml:lang="os">
    <form xml:lang="os-x-iron" type="lemma">
        <orth>dīdīnæg</orth>
    </form>
    <form xml:lang="os-x-digor" type="dialectal">
        <orth>dedenæg</orth>
    </form>
    <sense>
        <cit type="translation" xml:lang="ru">
            <q>цветок</q>
        </cit>
        <cit type="translation" xml:lang="en">
            <q>flower</q>
        </cit>
    </sense>
    <cit type="exampleGroup">
        <cit type="example">
            <quote>aly xorz dīdīnæǵy tæfæj adæjmaǵy zærdæ rūxs kodta</quote>
            <cit type="translation" xml:lang="ru">
                <q>от запаха разных приятных цветов светлело на сердце человека</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>the smell of different pleasant colors brightened the human heart</q>
            </cit>
            <bibl>
                <ref target="#src_Сека"/>
                <biblScope>16</biblScope>
            </bibl>
        </cit>
        <cit type="example">
            <quote>ærtydta aly dīdīnæg</quote>
            <cit type="translation" xml:lang="ru">
                <q>нарвал всяких цветов</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>[he] picked all sorts of flowers</q>
            </cit>
            <bibl>
                <ref target="#src_Munk."/>
                <biblScope>106</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>bajvadæncæ mæ dedengutæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>увяли мои цветы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>my flowers have withered</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>12</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>zinnuj ærdozæ didingutæj %i(sic!)%i fælust</quote>
            <cit type="translation" xml:lang="ru">
                <q>виднеется полянка, убранная цветами</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>a little field full of flowers is visible</q>
            </cit>
            <bibl>
                <ref target="#src_Iræf"/>
                <biblScope>26</biblScope>
            </bibl>
        </cit>
        <cit type="example" xml:lang="os-x-digor">
            <usg type="dialect">
                <lang/>
            </usg>
            <quote>mudi binʒitæ... dedengutæbæl baduncæ</quote>
            <cit type="translation" xml:lang="ru">
                <q>пчелы садятся на цветы</q>
            </cit>
            <cit type="translation" xml:lang="en">
                <q>bees sit on flowers</q>
            </cit>
            <bibl>
                <ref target="#src_AK"/>
                <biblScope>III 64</biblScope>
            </bibl>
        </cit>
    </cit>
